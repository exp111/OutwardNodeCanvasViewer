  m_Name: Dialogue_GabriellaSullivan_BC_WhisperingBones
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-1596.0,"y":-4212.0},"zoomFactor":0.9661478,"nodes":[{"_condition":{"questRef":{"_value":{"m_itemID":7011203}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":2535.0,"y":1665.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_condition":{"questRef":{"_value":{"m_itemID":7011204}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":2220.0,"y":1875.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"6"},{"_condition":{"QuestEventRef":{"m_eventUID":"cL_IvJSCFEWagDwmq-m6gg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":1740.0,"y":2070.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"10"},{"statement":{"_text":"Ugh...I\u2019m really not enjoying this whole Chamberlady thing. While I\u2019ve got nothing against playing politics sometimes, it\u2019s exhausting as a full-time occupation. Ellat\u2019s name, I need a drink.","_meta":"Gabriella_HM_HaPeace_11"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1425.0,"y":2235.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"13"},{"_position":{"x":2130.0,"y":2460.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"15"},{"statement":{"_text":"It\u2019s good to finally have peace again. Now I can focus on bringing in wild game and clearing out troublesome creatures and not on drilling troops!","_meta":"Gabriella_HM_HaPeace_10"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1875.0,"y":2235.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"16"},{"statement":{"_text":"Don\u2019t worry about me, [PlayerName]. My injures are healing nicely. I\u2019ll be out hunting again before you know it. If anything, I\u2019m just upset Rissa didn\u2019t see Julius\u2019s betrayal coming. I think it\u2019s time for her to retire as leader of the Krypteia!","_meta":"Gabriella_BC_Bones_26"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2400.0,"y":2115.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"18"},{"_condition":{"QuestEventRef":{"m_eventUID":"ft8ICwXzBESOrn78N1N3ww"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2760.0,"y":2280.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"20"},{"_condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"NI04g3-pUUi0fNJ5RFwwVg"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"7TiDsioyfE-rOJhxYxRsYA"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":2565.0,"y":2400.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"23"},{"statement":{"_text":"[PlayerName]! Thank the Ancestors! We''re in a bad spot right now! Did you find Julius?","_meta":"Gabriella_BC_Bones_15"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2190.0,"y":3060.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"30"},{"availableChoices":[{"statement":{"_text":"I found his body. He tried to shank Sagard Battleborn on account of a girl in red who said she really needed to get into the Necropolis, or her mistress would kill her.","_meta":"Player_Gabriella_BC_Bones_a_15"}}],"_position":{"x":2100.0,"y":3195.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"32"},{"statement":{"_text":"Lovely. I can see where that story is going. The damn fool let himself be manipulated by the enemy. Now, things make sense.","_meta":"Gabriella_BC_Bones_16"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1905.0,"y":3315.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"36"},{"availableChoices":[{"statement":{"_text":"What happened to you two? You\u2019re injured.","_meta":"Player_Gabriella_BC_Bones_a_16"}}],"_position":{"x":1830.0,"y":3465.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"38"},{"statement":{"_text":"As soon as we got this far, ghosts started pouring out from the bottom of the necropolis. I think they''re memories that frayed off the Ancestral Memories. That damned Lich worshipper must have already started to corrupt them. I got taken by surprise... If it wasn''t for Roland...","_meta":"Gabriella_BC_Bones_17"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1575.0,"y":3615.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"42"},{"availableChoices":[{"statement":{"_text":"Wait, Roland saved you?","_meta":"Player_Gabriella_BC_Bones_a_17"}},{"statement":{"_text":"Are you going to make it?","_meta":"Player_Gabriella_BC_Bones_b_17"}}],"_position":{"x":1650.0,"y":3735.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"44"},{"statement":{"_text":"It turns out he''s got a knack for ghosts and spirits. He sensed them coming and managed to drive them off.","_meta":"Gabriella_BC_Bones_18"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1410.0,"y":3900.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"50"},{"statement":{"_text":"The ghosts are in a blind fury and sealed the way deeper. I need you to <color=#00CCFF><b>find the Altars for the Sutherland and Aberdeen families and make an offering there</b></color>. If you burn some <color=#00CCFF><b>Greasy Fern</b></color> at those altars, it should calm these ghosts down enough that they''ll recognize us.","_meta":"Gabriella_BC_Bones_20"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1500.0,"y":4110.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"52"},{"statement":{"_text":"If you don\u2019t have any, scour the Necropolis, we use <color=#00CCFF><b>Greasy Fern</b></color> when communing with the ancestors, so there should be enough here.","_meta":"Gabriella_BC_Bones_24"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1290.0,"y":4260.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"54"},{"_condition":{"QuestEventRef":{"m_eventUID":"Dlzvc7PmMUOpdTh_Hwh-og"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":1530.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"56"},{"_position":{"x":1530.0,"y":4650.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"59"},{"_action":{"QuestEventRef":{"m_eventUID":"Dlzvc7PmMUOpdTh_Hwh-og"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":1635.0,"y":4530.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"60"},{"statement":{"_text":"Yeah, the injury isn''t too bad but I''m not going to be able to lift a sword for a while. I''m going to have to count on you and Roland to stop her.","_meta":"Gabriella_BC_Bones_19"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1800.0,"y":3960.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"63"},{"statement":{"_text":"Good work on the barrier, [PlayerName]. Roland went up ahead to <color=#00CCFF><b>deal with the altar and our unwelcome guest</b></color>. Go catch up with him and stop her!","_meta":"Gabriella_BC_Bones_21"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2700.0,"y":2700.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"65"},{"availableChoices":[{"statement":{"_text":"You''re injured! What happens if a ghost comes after you when no one is here to back you up?","_meta":"Player_Gabriella_BC_Bones_a_21"}}],"_position":{"x":2775.0,"y":2820.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"67"},{"statement":{"_text":"I don''t matter. No single life matters. But that altar, and the memories within? They matter. If they aren''t cleansed of the Scarlet Lady''s influence, the Collective and everyone in it will suffer greatly. Go!","_meta":"Gabriella_BC_Bones_22"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2700.0,"y":2925.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"71"},{"_position":{"x":2775.0,"y":3075.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"73"},{"_condition":{"QuestEventRef":{"m_eventUID":"L6R48GY_4EWHtfYaKmDQzw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3075.0,"y":3030.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"74"},{"_condition":{"QuestEventRef":{"m_eventUID":"BIg2OuMcd0W5izEaDq0JxA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2925.0,"y":3180.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"77"},{"statement":{"_text":"Jullius has been <color=#00CCFF><b>staying at the inn</b></color> during his investigation, if you can\u2019t find him out and about, he\u2019ll probably be there. Ask around.","_meta":"Gabriella_BC_Bones_14"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2445.0,"y":3330.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"80"},{"_position":{"x":2550.0,"y":3435.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"82"},{"_condition":{"QuestEventRef":{"m_eventUID":"_0_Ewk9bTUe9E1G5RzfGog"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2970.0,"y":3420.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"83"},{"_condition":{"QuestEventRef":{"m_eventUID":"YFIalayWjkG4RCTBwaCmZQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2505.0,"y":3570.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"86"},{"statement":{"_text":"Well, I''ve confirmed that thing is dead. Pretty solid work too, both of you.","_meta":"Gabriella_BC_Bones_10"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2070.0,"y":4125.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"89"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"BIg2OuMcd0W5izEaDq0JxA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"Stop":{},"Music":{"_value":"BGM_EventDanger"},"$type":"NodeCanvas.Tasks.Actions.StartMusicEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":2025.0,"y":4275.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"91"},{"statement":{"_text":"Well, I''m satisfied that both of you know how to handle yourselves. I''ll initiate you both the moment you''re ready. The sooner both of you are connected with the Ancestral Memories, the better.","_meta":"Gabriella_BC_Bones_12"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2250.0,"y":4560.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"99"},{"statement":{"_text":"[PlayerName], can you go <color=#00CCFF><b>fetch Julius</b></color>? I''ll need an experienced Krypteia mage for this. In the meantime, Roland, you''re with me. We''re going to the Necropolis to prepare for the initiation.","_meta":"Gabriella_BC_Bones_13"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2130.0,"y":4695.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"101"},{"availableChoices":[{"statement":{"_text":"Do you know where I can find him?","_meta":"Player_Gabriella_BC_Bones_a_13"}}],"_position":{"x":2400.0,"y":4815.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"103"},{"statement":{"_text":"Jullius has been <color=#00CCFF><b>staying at the inn</b></color> during his investigation, if you can\u2019t find him out and about, he\u2019ll probably be there. Ask around.","_meta":"Gabriella_BC_Bones_14"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2280.0,"y":4950.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"107"},{"_condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":6600121}},"minAmount":{"_value":1},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"},"_position":{"x":2490.0,"y":5040.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"109"},{"_action":{"actions":[{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":6600121}}],"Amount":[{"_value":1}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"},{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":4300240}},"Quantity":{"_value":2},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":4300260}},"Quantity":{"_value":1},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":4400031}},"Quantity":{"_value":1},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":6500110}},"Quantity":{"_value":1},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":2175.0,"y":5160.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"116"},{"_action":{"QuestEventRef":{"m_eventUID":"BIg2OuMcd0W5izEaDq0JxA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":2445.0,"y":5325.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"150"},{"_position":{"x":2565.0,"y":5490.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"153"},{"statement":{"_text":"Did you kill that green Scourge Beast yet?","_meta":"Gabriella_BC_Bones_06"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2640.0,"y":3765.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"154"},{"availableChoices":[{"statement":{"_text":"Yes, the monster is dead.","_meta":"Player_Gabriella_BC_Bones_b_06"}}],"_position":{"x":2685.0,"y":3870.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"156"},{"statement":{"_text":"Really? Did you bring back proof?","_meta":"Gabriella_BC_Bones_08"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2625.0,"y":3975.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"160"},{"_condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":6600121}},"minAmount":{"_value":1},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"},"_position":{"x":2700.0,"y":4080.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"162"},{"availableChoices":[{"statement":{"_text":"Yeah, here\u2019s a piece of its chitin hide. Is this going to be enough to convince you?","_meta":"Player_Gabriella_BC_Bones_b_08"}}],"_position":{"x":2565.0,"y":4245.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"169"},{"statement":{"_text":"Hmm... Yes, this is from that monster alright, I can smell the blood on it. It''s pretty beat up too. You must have done a bang up job on the thing.","_meta":"Gabriella_BC_Bones_11"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2460.0,"y":4365.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"173"},{"availableChoices":[{"statement":{"_text":"Uhh... No...I Must have lost it...","_meta":"Player_Gabriella_BC_Bones_a_08"}}],"_position":{"x":2910.0,"y":4275.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"175"},{"statement":{"_text":"You couldn''t have just brought back a chunk of the monster? Fine. I''ll go check, myself. <color=#00CCFF><b>I''ll be back in about two days</b></color>.","_meta":"Gabriella_BC_Bones_09"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2940.0,"y":4440.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"179"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":3045.0,"y":4560.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"181"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"YFIalayWjkG4RCTBwaCmZQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"hAlacUNhR0u3HHxX8F66Tw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":2970.0,"y":4650.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"183"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":3120.0,"y":4770.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"190"},{"_position":{"x":3045.0,"y":4860.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"192"},{"statement":{"_text":"Did you kill that green Scourge Beast yet?","_meta":"Gabriella_BC_Bones_06"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3030.0,"y":3600.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"193"},{"availableChoices":[{"statement":{"_text":"Not yet. We\u2019re working on it.","_meta":"Player_Gabriella_BC_Bones_a_06"}}],"_position":{"x":3210.0,"y":3765.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"195"},{"statement":{"_text":"Well, get on it already. You\u2019re already wasting my precious investigation time.","_meta":"Gabriella_BC_Bones_07"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3135.0,"y":3885.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"199"},{"_position":{"x":3300.0,"y":4005.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"201"},{"_condition":{"QuestEventRef":{"m_eventUID":"KMpTo3oZMUKj8PM-KzcaLg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3645.0,"y":3420.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"202"},{"statement":{"_text":"Roland told me how you bailed him out at the Conflux Mountain. I''m glad Rissa sent you. At least one of you knows what they''re doing. Look, I''m honestly not convinced that desperately recruiting anyone into the Krypteia will help matters.","_meta":"Gabriella_BC_Bones_01"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3585.0,"y":3555.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"205"},{"statement":{"_text":"If you want me to actually be fine with this, you need to prove you can handle a serious fight. I need you to prove you can stare death in the face and come out on top.","_meta":"Gabriella_BC_Bones_01_B"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3630.0,"y":3690.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"207"},{"statement":{"_text":"There''s an <color=#00CCFF><b>old temple</b></color> that belonged to the Cabal of Winds in Enmerkar that''s been a lair for Scourge Beasts since the collapse. The other day I saw a <color=#00CCFF><b>distinctive bright green Shell Horror</b></color> enter the place. I want you to <color=#00CCFF><b>go kill it</b></color> for me.","_meta":"Gabriella_BC_Bones_02"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3690.0,"y":3825.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"209"},{"availableChoices":[{"statement":{"_text":"You want us to kill a Scourge Beast? Are you trying to kill us?","_meta":"Player_Gabriella_BC_Bones_a_02"}},{"statement":{"_text":"Green Scourge Beast, kill it. Got it.","_meta":"Player_Gabriella_BC_Bones_b_02"}}],"_position":{"x":3780.0,"y":3945.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"211"},{"statement":{"_text":"If you can''t handle a Scourge Beast, I don''t see how you could handle anything the Scarlet Lady will throw at us. I won''t trust anyone who can''t or won''t put their life on the line if I ask them to.","_meta":"Gabriella_BC_Bones_03"},"_actorName":"Gabriella Sullivan","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3600.0,"y":4110.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"217"},{"statement":{"_text":"The Temple is up <color=#00CCFF><b>on the mountain in the South-West</b></color> of Enmerkar forest. You will know it when you see it. Come back here when that monster is dead. <color=#00CCFF><b>Don\u2019t forget to take Roland with you</b></color>, he\u2019s being tested too.","_meta":"Gabriella_BC_Bones_05"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3660.0,"y":4380.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"219"},{"_action":{"QuestEventRef":{"m_eventUID":"L6R48GY_4EWHtfYaKmDQzw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3645.0,"y":4500.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"221"},{"_position":{"x":3795.0,"y":4620.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"224"},{"statement":{"_text":"Have you even fought one before? You''re either foolishly overconfident, or you''ve been up to things even I haven''t heard about. I hope for your sake it''s not the former.","_meta":"Gabriella_BC_Bones_04"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4005.0,"y":4140.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"225"},{"statement":{"_text":"Greetings. I hope you\u2019re well!","_meta":"Salutation_Generic_02"},"_actorName":"name_unpc_gabriellasullivan_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4020.0,"y":3585.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"227"},{"_position":{"x":4095.0,"y":3765.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"229"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"6"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"20"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"6"},"_targetNode":{"$ref":"10"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"6"},"_targetNode":{"$ref":"18"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"10"},"_targetNode":{"$ref":"13"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"10"},"_targetNode":{"$ref":"16"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"13"},"_targetNode":{"$ref":"15"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"16"},"_targetNode":{"$ref":"15"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"18"},"_targetNode":{"$ref":"15"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"20"},"_targetNode":{"$ref":"23"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"20"},"_targetNode":{"$ref":"74"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"23"},"_targetNode":{"$ref":"30"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"23"},"_targetNode":{"$ref":"65"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"30"},"_tar
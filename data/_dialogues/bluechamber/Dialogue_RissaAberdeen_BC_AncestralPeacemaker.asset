  m_Name: Dialogue_RissaAberdeen_BC_AncestralPeacemaker
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-2887.0,"y":-4724.0},"zoomFactor":0.983226,"nodes":[{"_condition":{"questRef":{"_value":{"m_itemID":7011204}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":3765.0,"y":3075.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"You''re free to do as you please, [PlayerName]. I trust your instincts. Do whatever you feel you need to do.","_meta":"Negotiate_BC_ANC_PEACE_85"},"_position":{"x":3465.0,"y":3300.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"6"},{"_position":{"x":3465.0,"y":3450.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"8"},{"_condition":{"QuestEventRef":{"m_eventUID":"_PaN89YluUSonCS3LQmPbQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4125.0,"y":3315.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"9"},{"_condition":{"checkMode":"AnyTrueSuffice","conditions":[{"QuestEventRef":{"m_eventUID":"bRGI2Ws5DEm0Xy6S-Hcumw"},"MinStack":3,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"vstF8FsBHUSGuBANS5fb8Q"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":3810.0,"y":3450.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"12"},{"statement":{"_text":"That was unexpected. I''m glad you came along.","_meta":"Negotiate_BC_ANC_PEACE_78"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3405.0,"y":3780.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"19"},{"availableChoices":[{"statement":{"_text":"Me too. It''s not every day you get to be present for a divine ascension.","_meta":"Player_Negotiate_BC_ANC_PEACE_a_78"}},{"statement":{"_text":"After all I\u2019ve been through, I need a vacation and ten barrels of wine.","_meta":"Player_Negotiate_BC_ANC_PEACE_b_78"}}],"_position":{"x":3450.0,"y":3900.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"21"},{"statement":{"_text":"History was made here today. This will be the spark that ignites great change across Aurai. We will need to watch this situation closely.","_meta":"Negotiate_BC_ANC_PEACE_86"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3225.0,"y":4065.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"27"},{"statement":{"_text":"You were a real asset out there today, [PlayerName]. I look forward to seeing what path you carve for yourself from here on out.","_meta":"Negotiate_BC_ANC_PEACE_80"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3375.0,"y":4290.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"29"},{"_condition":{"QuestEventRef":{"m_eventUID":"vstF8FsBHUSGuBANS5fb8Q"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3375.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"31"},{"statement":{"_text":"Your defense of Monsoon bought us the time we needed for Calixa\u2019s ascension to be completed. You\u2019ve earned this.","_meta":"Negotiate_BC_ANC_PEACE_82"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3090.0,"y":4605.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"34"},{"_action":{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":3100091}},"Quantity":{"_value":1},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":6300030}},"Quantity":{"_value":3},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"},"_position":{"x":3180.0,"y":4755.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"36"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"PeySuUam6UGq6rzHRAgQ4g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"quest":{},"questRef":{"_value":{"m_itemID":7011501}},"$type":"NodeCanvas.Tasks.Actions.GiveQuest"},{"Stop":{},"Music":{"_value":"BGM_OutwardTheme"},"$type":"NodeCanvas.Tasks.Actions.StartMusicEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3435.0,"y":4905.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"51"},{"_condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":5600050}},"minAmount":{"_value":1},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"},"_position":{"x":3435.0,"y":5025.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"63"},{"_action":{"actions":[{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":5600050}}],"Amount":[{"_value":1}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"},{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":6300030}},"Quantity":{"_value":3},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3315.0,"y":5145.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"70"},{"_targetNode":{"$ref":"63"},"_position":{"x":3315.0,"y":5250.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"89"},{"statement":{"_text":"You''re free to do as you please, [PlayerName]. I trust your instincts. Do whatever you feel you need to do.","_meta":"Negotiate_BC_ANC_PEACE_85"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3345.0,"y":5355.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"90"},{"_position":{"x":3480.0,"y":5490.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"92"},{"statement":{"_text":"Your stalwart defense of Monsoon saved lives, [PlayerName]. Also, Simeon asked that I give this armor to you. May it serve you well.","_meta":"Negotiate_BC_ANC_PEACE_81"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3645.0,"y":4605.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"93"},{"_action":{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":3100090}},"Quantity":{"_value":1},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":3100091}},"Quantity":{"_value":1},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":3100092}},"Quantity":{"_value":1},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"},"_position":{"x":3600.0,"y":4770.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"95"},{"statement":{"_text":"Granted. You\u2019ve more than done your part here.","_meta":"Negotiate_BC_ANC_PEACE_79"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3645.0,"y":4110.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"115"},{"_condition":{"QuestEventRef":{"m_eventUID":"0y4f5zTcmUKqRT7zMUkZPw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4095.0,"y":3675.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"117"},{"_condition":{"QuestEventRef":{"m_eventUID":"Ut_b4qIDZUmEcxTjirr7JA"},"ExpiryTime":168,"$type":"NodeCanvas.Tasks.Conditions.Condition_CheckQuestEventExpiry"},"_position":{"x":4245.0,"y":3870.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"120"},{"statement":{"_text":"You''re late! Negotiations were supposed to start already! At least tell me everything went well on your end!","_meta":"Rissa_BC_ANC_PEACE_17"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4065.0,"y":4035.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"123"},{"availableChoices":[{"statement":{"_text":"I got Sagard to agree to pull back his armies. He won''t cause any more trouble.","_meta":"Player_Rissa_BC_ANC_PEACE_a_18"},"condition":{"QuestEventRef":{"m_eventUID":"ge4uFxQLPku1yCssyKDJtg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"I couldn''t convince Sagard. Is seems like he''s gone rogue with a large detachment of our army.","_meta":"Player_Rissa_BC_ANC_PEACE_b_18"},"condition":{"QuestEventRef":{"m_eventUID":"M4r4R8hH0Ey8hZalHbj7dw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":4485.0,"y":4200.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"125"},{"statement":{"_text":"Thank the ancestors. I don''t know what you had to do to change his mind, but it can''t have been an easy task.","_meta":"Rissa_BC_ANC_PEACE_19"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4155.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"135"},{"statement":{"_text":"And the Montcalm clan? Did you speak with Rastarte?","_meta":"Rissa_BC_ANC_PEACE_21"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4545.0,"y":4560.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"137"},{"availableChoices":[{"statement":{"_text":"They accept peace, but they''ll need soldiers to protect them. They\u2019ve been offering tribute to the undead from beyond the Ghost Pass for protection. The spirits will attack them once the offerings stop coming.","_meta":"Player_Rissa_BC_ANC_PEACE_a_21"},"condition":{"QuestEventRef":{"m_eventUID":"s1cho7sD1kyq4rfA-YxEPQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"No luck. Their grudge is strong enough that they''d rather abandon us to worship ghosts as their protectors.","_meta":"Player_Rissa_BC_ANC_PEACE_b_21"},"condition":{"QuestEventRef":{"m_eventUID":"dJJBXv0s6k6id0prsgKeMQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":4545.0,"y":4695.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"139"},{"statement":{"_text":"I''ll see to it that they get the protection they need. I''m amazed you succeeded, [PlayerName]. I\u2019d given up hope we''d ever resolve our conflict with them.","_meta":"Rissa_BC_ANC_PEACE_22"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4155.0,"y":4920.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"149"},{"_condition":{"QuestEventRef":{"m_eventUID":"I0ZciWg9iUWxQMRdAU7vIg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4410.0,"y":5130.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"151"},{"statement":{"_text":"And the Ash Giants? Did you manage to pacify them?","_meta":"Rissa_BC_ANC_PEACE_24"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4245.0,"y":5280.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"154"},{"availableChoices":[{"statement":{"_text":"The Ashborn are eager for peace. They\u2019re glad to return to working silk and porcelain without dealing with the conflicts of humans. ","_meta":"Player_Rissa_BC_ANC_PEACE_a_24"}}],"_position":{"x":4305.0,"y":5460.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"156"},{"statement":{"_text":"I''m surprised. I would have expected them to be difficult to convince... I shouldn''t have judged an entire people based on secondhand impressions. They\u2019re proving to be quite helpful to our cause.","_meta":"Rissa_BC_ANC_PEACE_25"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4245.0,"y":5595.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"160"},{"statement":{"_text":"Negotiations are beginning, and I\u2019ll need you with me. The situation is very delicate. There was a coup in Levant. Queen Calixa was even poisoned! She''s conscious for now, but I don''t know how long it will last. I need backup if disaster strikes.","_meta":"Rissa_BC_ANC_PEACE_26"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4665.0,"y":5790.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"162"},{"availableChoices":[{"statement":{"_text":"What in Ellat\u2019s name happened there?","_meta":"Player_Rissa_BC_ANC_PEACE_a_26"}}],"_position":{"x":4785.0,"y":5910.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"164"},{"statement":{"_text":"I don\u2019t know, but she was exposed to an unknown toxin. A healer from the Holy Mission gave her antidotes but he said it can only postpone the inevitable. Our peace talk must happen now, or never.","_meta":"Rissa_BC_ANC_PEACE_27"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4680.0,"y":6090.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"168"},{"_position":{"x":4860.0,"y":6270.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"170"},{"statement":{"_text":"Then our feud with them can\u2019t get any worse. If they acquired a legion of ghosts, we won\u2019t be able to drive them out of the Chersonese to protect Cierzo from their raids.","_meta":"Rissa_BC_ANC_PEACE_23"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4590.0,"y":4965.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"171"},{"statement":{"_text":"So, we face civil war, then...I can only hope his fear of Elatt will keep him from declaring war against Levant again... At least he abandoned his position, he won\u2019t have the the political power he once held.","_meta":"Rissa_BC_ANC_PEACE_20"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4725.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"173"},{"statement":{"_text":"You''re right on time, [PlayerName]! Tell me, how did it go?","_meta":"Rissa_BC_ANC_PEACE_18"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4635.0,"y":4050.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"175"},{"_condition":{"QuestEventRef":{"m_eventUID":"TgLS5hK3fE-pofU_R730ww"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":5760.0,"y":3945.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"177"},{"_condition":{"QuestEventRef":{"m_eventUID":"I0ZciWg9iUWxQMRdAU7vIg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":5700.0,"y":6900.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"180"},{"statement":{"_text":"Remember, [PlayerName], you need to deal with Sagard, here in Berg, and the Montcalm tribe in the North of the Chersonese, then make your way Monsoon, in the Hallowed Marsh.","_meta":"Rissa_BC_ANC_PEACE_15"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5595.0,"y":7035.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"183"},{"statement":{"_text":"While you''re in the Hallowed Marsh, make sure you <color=#00CCFF><b>get the Ash Giants to agree to with the peace talks as well</b></color>.","_meta":"Rissa_BC_ANC_PEACE_16"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5415.0,"y":7215.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"185"},{"_position":{"x":5670.0,"y":7350.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"187"},{"statement":{"_text":"Remember, [PlayerName], you need to deal with Sagard, here in Berg, and the Montcalm tribe in the North of the Chersonese, then make your way Monsoon, in the Hallowed Marsh.","_meta":"Rissa_BC_ANC_PEACE_15"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6135.0,"y":7140.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"188"},{"_position":{"x":6360.0,"y":7335.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"190"},{"statement":{"_text":"[PlayerName], the situation between us and Levant has just shifted. The war is coming to a close.","_meta":"Rissa_BC_ANC_PEACE_01"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5970.0,"y":4065.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"191"},{"availableChoices":[{"statement":{"_text":"The war is ending? What happened?","_meta":"Player_Rissa_BC_ANC_PEACE_a_01"}}],"_position":{"x":6090.0,"y":4215.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"193"},{"statement":{"_text":"The Holy Mission just sent a messenger. Elatt demands peace. A truce has just been declared. We will negotiate with Levant once we\u2019ve <color=#00CCFF><b>gotten the rest of the Chamberlords to agree</b></color> to peace talks.","_meta":"Rissa_BC_ANC_PEACE_02"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6075.0,"y":4350.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"197"},{"availableChoices":[{"statement":{"_text":"Good, the war\u2019s gone on long enough.","_meta":"Player_Rissa_BC_ANC_PEACE_a_02"}},{"statement":{"_text":"What?! But we''re finally winning!","_meta":"Player_Rissa_BC_ANC_PEACE_b_02"}},{"statement":{"_text":"Why should we obey him? Levant struck first, remember?","_meta":"Player_Rissa_BC_ANC_PEACE_c_02"}}],"_position":{"x":6150.0,"y":4485.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"199"},{"statement":{"_text":"I agree. Levant doesn''t bother taking many prisoners. If the war continues for much longer, there won\u2019t be anyone left to enjoy the peace.","_meta":"Rissa_BC_ANC_PEACE_03"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5940.0,"y":4665.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"207"},{"statement":{"_text":"I''ll convince the main families to comply but there\u2019s a few that I''ll need you to deal with. Please, [PlayerName], try to use some tact and diplomacy here.","_meta":"Rissa_BC_ANC_PEACE_06"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6225.0,"y":5010.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"209"},{"statement":{"_text":"First, I need you to <color=#00CCFF><b>talk to Sagard Battleborn</b></color>. He is aggressively opposed to peace with Levant and unless we deal with him, he\u2019ll ignite another war on his own. <color=#00CCFF><b>Make him call off our soldiers by any means necessary</b></color>.","_meta":"Rissa_BC_ANC_PEACE_07"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6225.0,"y":5145.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"211"},{"statement":{"_text":"Then, there''s the <color=#00CCFF><b>Montcalm tribe in the North of the Chersonese</b></color>, near the Ghost Pass. With the truce, they have suspended hostilities against other tribes for the time being. <color=#00CCFF><b>Speak with their Chamberlord, Rastarte</b></color>, and convince him to support the peace with Levant.","_meta":"Rissa_BC_ANC_PEACE_08"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6225.0,"y":5280.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"213"},{"_condition":{"QuestEventRef":{"m_eventUID":"I0ZciWg9iUWxQMRdAU7vIg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":6270.0,"y":5415.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"215"},{"statement":{"_text":"Finally, you have the most experience <color=#00CCFF><b>dealing</b></color> with the Ash Giants. You should be the one to convince them to accept peace negotiations. You might want to speak with them last, as they\u2019re the closest to Monsoon.","_meta":"Rissa_BC_ANC_PEACE_09"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5970.0,"y":5580.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"218"},{"availableChoices":[{"statement":{"_text":"Why can''t you handle these people?","_meta":"Player_Rissa_BC_ANC_PEACE_a_09"}},{"statement":{"_text":"Alright, I''ll get on this.","_meta":"Player_Rissa_BC_ANC_PEACE_b_09"}}],"_position":{"x":6495.0,"y":5730.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"220"},{"statement":{"_text":"Because Sagard won''t listen to anyone who\u2019s not a hardened warrior, and he''s still furious at me over what happened with Julius. You''ll have a better chance at changing his mind than l will.","_meta":"Rissa_BC_ANC_PEACE_10"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6195.0,"y":5865.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"226"},{"statement":{"_text":"And the Montcalm tribe still wants my head after I learned the Scarlet Lady had a pawn in their tribe, a beloved singer, and ordered Gabriella to put an end to her. If Gabriella or I showed our faces there, they''d kill us on the spot.","_meta":"Rissa_BC_ANC_PEACE_11"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6165.0,"y":6000.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"228"},{"statement":{"_text":"Please be especially wary around the Montcalm tribe. Our scouts said they hang enemies around their encampment, and that undead rise from tunnels beyond the Ghost Pass to feed on them.","_meta":"Rissa_BC_ANC_PEACE_12"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6480.0,"y":6195.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"230"},{"statement":{"_text":"<color=#00CCFF><b>When you\u2019ve convinced all of them to agree to the negotiations, meet me at Elatt''s temple in Monsoon</b></color>.","_meta":"Rissa_BC_ANC_PEACE_13"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6495.0,"y":6375.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"232"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"TgLS5hK3fE-pofU_R730ww"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"Ut_b4qIDZUmEcxTjirr7JA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":6450.0,"y":6540.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"234"},{"statement":{"_text":"You speak like Sagard! The war was only meant to be defensive, to protect us from Levant''s aggression. Punishing them all for the attack on Berg will only lead to more violence in the future.","_meta":"Rissa_BC_ANC_PEACE_04"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6210.0,"y":4755.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"241"},{"statement":{"_text":"Maybe you''d like to be the one to tell a god that you disagree with ending the war, then? The messenger said Elatt \"demanded\" peace, understand?","_meta":"Rissa_BC_ANC_PEACE_05"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6510.0,"y":4665.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"243"},{"statement":{"_text":"I\u2019ve been communing daily with the Ancestral Memories to seek their advice about the war with Levant. Come back in a few days, maybe we\u2019ll have come up with something by then.","_meta":"Rissa_BC_ANC_PEACE_28"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4425.0,"y":3465.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"245"},{"_position":{"x":4635.0,"y":3600.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"247"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"6"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"9"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"6"},"_targetNode":{"$ref":"8"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"9"},"_targetNode":{"$ref":"12"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"9"},"_targetNode":{"$ref":"245"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"12"},"_targetNode":{"$ref":"19"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"12"},"_targetNode":{"$ref":"117"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"19"},"_targetNode":{"$ref":"21"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"21"},"_targetNode":{"$ref":"27"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"21"},"_targetNode":{"$ref":"115"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"27"},"_targetNode":{"$ref":"29"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"29"},"_targetNode":{"$ref":"31"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"31"},"_targetNode":{"$ref":"34"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"31"},"_targetNode":{"$ref":"93"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"34"},"_targetNode":{"$ref":"36"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"36"},"_targetNode":{"$ref
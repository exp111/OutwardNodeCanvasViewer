  m_Name: Dialogue_RolandArgenson_BC_WhisperingBones_Chersonese
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-1696.0,"y":-4908.0},"zoomFactor":0.9454105,"nodes":[{"_condition":{"questRef":{"_value":{"m_itemID":7011203}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":2835.0,"y":2340.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"Greetings. I hope you\u2019re well!","_meta":"Salutation_Generic_02"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2640.0,"y":2505.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"6"},{"_position":{"x":2640.0,"y":2670.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"8"},{"_condition":{"QuestEventRef":{"m_eventUID":"KMpTo3oZMUKj8PM-KzcaLg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3000.0,"y":2715.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"9"},{"_action":{"dialogueStarter":{"_name":"Dialogue_RolandArgenson_BB/BC_WhisperingBonesEme","_targetVariableID":"f4acbc38-c119-4102-aa3d-836c09706a79"},"$type":"NodeCanvas.Tasks.Actions.BranchDialogue"},"_position":{"x":2505.0,"y":2925.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"12"},{"_condition":{"QuestEventRef":{"m_eventUID":"S5-gCtrRlkynpw7Y7XzWgQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3120.0,"y":2985.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"15"},{"_condition":{"QuestEventRef":{"m_eventUID":"KrK6qemiCEq1exn_oQEd1A"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2250.0,"y":3360.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"18"},{"statement":{"_text":"I''m going to go report to Gabriella once I\u2019ve caught my breath. <color=#00CCFF><b>I''ll see you back in Berg</b></color>, [PlayerName].","_meta":"Roland_BC_Bones_24"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2175.0,"y":5055.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"21"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":2355.0,"y":5160.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"23"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"KMpTo3oZMUKj8PM-KzcaLg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":4100270}},"Quantity":{"_value":2},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":4300020}},"Quantity":{"_value":2},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":2265.0,"y":5280.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"25"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":2310.0,"y":5415.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"44"},{"_position":{"x":2295.0,"y":5535.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"46"},{"statement":{"_text":"Gahh...I\u2019ve made a terrible mistake...I gave up so much of my life to the Ley Line that my body is as frail as a spiderweb. I couldn\u2019t even take on a single Pistol Shrimp like this. I''m useless. I''ll never shake this Blood Price. I''ll never amount to anything.","_meta":"Roland_BC_Bones_18"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2550.0,"y":3555.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"47"},{"availableChoices":[{"statement":{"_text":"What are you going to do about it? Just whine? Or are you going to actually try?","_meta":"Player_Roland_BC_Bones_a_18"}},{"statement":{"_text":"Roland, get your chin up. There''s always hope if you keep at it.","_meta":"Player_Roland_BC_Bones_b_18"}}],"_position":{"x":2745.0,"y":3705.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"49"},{"statement":{"_text":"You''re right. I have either get good or die trying. No in-between. Nothing will change if I don''t. All I can do is just push ahead and make it happen, or die trying.","_meta":"Roland_BC_Bones_19"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2415.0,"y":3885.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"55"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"KrK6qemiCEq1exn_oQEd1A"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"8lbz0Jh_iUuDS4wpX_-vog"},"StackAmount":3,"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":2475.0,"y":4050.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"57"},{"availableChoices":[{"statement":{"_text":"So, what was it we were looking for?","_meta":"Player_Roland_BC_Bones_a_19"}}],"_position":{"x":2730.0,"y":4245.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"64"},{"statement":{"_text":"It''s my dad''s old compass. Yzan and I borrowed it back when we were kids to go run around outside. You can probably imagine how that ended up. Stupid Pistol shrimp would have killed us if it wasn''t for the old Warden coming in to save us. It''s all I have left to remember my dad by.","_meta":"Roland_BC_Bones_21"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2685.0,"y":4380.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"68"},{"availableChoices":[{"statement":{"_text":"I can see why you''d come back for this.","_meta":"Player_Roland_BC_Bones_a_21"}},{"statement":{"_text":"It looks broken, it\u2019s probably not even worth 10 silvers.","_meta":"Player_Roland_BC_Bones_b_21"}}],"_position":{"x":2820.0,"y":4530.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"70"},{"statement":{"_text":"Thanks for helping me find it again, [PlayerName]. I won''t forget this.","_meta":"Roland_BC_Bones_22"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2550.0,"y":4740.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"76"},{"statement":{"_text":"Yeah, but to me it''s worth a lot more than that. Thanks for helping me find it again.","_meta":"Roland_BC_Bones_23"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3000.0,"y":4740.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"78"},{"statement":{"_text":"Oh, right. Hope. The little carrot you dangle in front of a horse to make it keep going. Life''s already beaten me with the damn stick, why should I care about the stupid carrot that I''ll never manage to reach? Hope\u2019s just a big, cruel lie we tell ourselves when there''s nothing we can do.","_meta":"Roland_BC_Bones_20"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2925.0,"y":3900.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"80"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"KrK6qemiCEq1exn_oQEd1A"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"8lbz0Jh_iUuDS4wpX_-vog"},"StackAmount":2,"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":2970.0,"y":4050.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"82"},{"_condition":{"QuestEventRef":{"m_eventUID":"2DJ3Edf-4k-VBM_QREs6qA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4080.0,"y":3405.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"89"},{"_condition":{"QuestEventRef":{"m_eventUID":"uIXuJKuV5EeLHL7kfZDGfQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3630.0,"y":3675.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"92"},{"statement":{"_text":"Hey...[PlayerName]? While we''re back here in the Chersonese... There''s <color=#00CCFF><b>somewhere I want to go</b></color>. I''d feel better if you came along.","_meta":"Roland_BC_Bones_12"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3405.0,"y":5535.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"95"},{"statement":{"_text":"When we were kids, Yzan and I went into the <color=#00CCFF><b>Starfish cave on the beach near Cierzo</b></color>. I... lost something there. Something that belonged to my father.","_meta":"Roland_BC_Bones_15"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3300.0,"y":5685.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"97"},{"availableChoices":[{"statement":{"_text":"I''m not taking a detour for us to pick up an old trinket you dropped in a cave years ago. We have work to do.","_meta":"Player_Roland_BC_Bones_a_15"}},{"statement":{"_text":"Sure, why not. It''s not that far out of the way.","_meta":"Player_Roland_BC_Bones_b_15"}}],"_position":{"x":3480.0,"y":5820.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"99"},{"statement":{"_text":"I suppose... It probably got destroyed or stolen years ago anyway. I\u2019ll head back to Berg and to report to Gabriella, then.","_meta":"Roland_BC_Bones_16"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3135.0,"y":6000.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"105"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":3255.0,"y":6120.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"107"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"KMpTo3oZMUKj8PM-KzcaLg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":4100270}},"Quantity":{"_value":2},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":4300020}},"Quantity":{"_value":2},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3180.0,"y":6225.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"109"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":3255.0,"y":6375.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"128"},{"_position":{"x":3255.0,"y":6480.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"130"},{"statement":{"_text":"Really? Thanks, [PlayerName]. It means a lot to me. The cave I''m talking about is the one right <color=#00CCFF><b>near Cierzo''s back door, on the beach where the tunnel from the storage tunnel comes out.</b></color>","_meta":"Roland_BC_Bones_17"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3630.0,"y":6015.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"131"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":3795.0,"y":6135.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"133"},{"_action":{"QuestEventRef":{"m_eventUID":"S5-gCtrRlkynpw7Y7XzWgQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3705.0,"y":6240.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"135"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":3795.0,"y":6330.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"138"},{"_position":{"x":3810.0,"y":6435.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"140"},{"statement":{"_text":"Oh, Elatt''s name... Everything hurts. Everything. Hurts. I had no idea the Mana ritual was so painful.","_meta":"Roland_BC_Bones_06"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3795.0,"y":3810.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"141"},{"availableChoices":[{"statement":{"_text":"Roland, exactly how much of your life did you sacrifice to tap into the Ley Line''s Mana?","_meta":"Player_Roland_BC_Bones_a_06"}}],"_position":{"x":3930.0,"y":3930.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"143"},{"statement":{"_text":"I don''t know. I just kept pouring my life into the Ley Line, as much as I could bear... I wanted as much Mana as I could take.","_meta":"Roland_BC_Bones_07"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3885.0,"y":4035.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"147"},{"availableChoices":[{"statement":{"_text":"Roland, you complete moron. Do you have a death wish?","_meta":"Player_Roland_BC_Bones_a_07"}},{"statement":{"_text":"You must have gained a ton of magical power. How does it feel?","_meta":"Player_Roland_BC_Bones_b_07"}}],"_position":{"x":3825.0,"y":4155.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"149"},{"statement":{"_text":"You''re asking? I honestly don''t care what happens to me anymore. Rissa has me by the stones. If I don''t work for her, I can''t pay my Blood Price. If I do work for her, I face almost certain death. Both options are impossible to deal with. I don\u2019t exactly have much to lose.","_meta":"Roland_BC_Bones_08"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3750.0,"y":4440.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"155"},{"statement":{"_text":"I''m already able to use Magic. See this sword? I made it myself out of my own Mana. I can make a blade whenever I need one, now!","_meta":"Roland_BC_Bones_10"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3825.0,"y":4920.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"157"},{"availableChoices":[{"statement":{"_text":"That''s... a sword made of Mana? That''s actually kind of impressive for a new mage. Do you know how to use it?","_meta":"Player_Roland_BC_Bones_a_10"}}],"_position":{"x":3945.0,"y":5040.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"159"},{"statement":{"_text":"It might be made of Mana, but it''s still a sword. The pointy end goes into the bad guys. How hard can it be to use?","_meta":"Roland_BC_Bones_11"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3885.0,"y":5175.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"163"},{"statement":{"_text":"I feel worn and empty... but also full of power. For the first time in my life, I think I can actually make a difference. With this, I can actually reshape my future.","_meta":"Roland_BC_Bones_09"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4140.0,"y":4515.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"165"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"8lbz0Jh_iUuDS4wpX_-vog"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"uIXuJKuV5EeLHL7kfZDGfQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4020.0,"y":4710.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"167"},{"_condition":{"QuestEventRef":{"m_eventUID":"m4qEz7skW0utc4vD40qQfg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":5055.0,"y":3525.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"174"},{"statement":{"_text":"Who... What... Oh. [PlayerName]. That was you who saved me? Thank you, I really owe you one. That rock mantis really had me.","_meta":"Roland_BC_Bones_02"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4890.0,"y":3645.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"177"},{"availableChoices":[{"statement":{"_text":"You''re injured. Hang on, let me patch that up.","_meta":"Player_Roland_BC_Bones_a_02"}}],"_position":{"x":5010.0,"y":3810.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"179"},{"statement":{"_text":"I have some bandages on me, I can handle it myself. I''ll patch this up and I''ll keep going.","_meta":"Roland_BC_Bones_03"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4920.0,"y":3945.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"183"},{"availableChoices":[{"statement":{"_text":"Whoa whoa whoa, I''m coming with you. I don''t want to have to bail you out again!","_meta":"Player_Roland_BC_Bones_a_03"}},{"statement":{"_text":"Fine by me. Don''t get into any more trouble.","_meta":"Player_Roland_BC_Bones_b_03"}}],"_position":{"x":5040.0,"y":4065.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"185"},{"statement":{"_text":"No. If I''m going to do this, I need to try to make it on my own. Give me a few minutes head start to <color=#00CCFF><b>get to the heart of the mountain</b></color>, alright?","_meta":"Roland_BC_Bones_04"},"_actorName":"Roland Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4770.0,"y":4215.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"191"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":5085.0,"y":4410.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"193"},{"_action":{"QuestEventRef":{"m_eventUID":"2DJ3Edf-4k-VBM_QREs6qA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":5070.0,"y":4530.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"195"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":5115.0,"y":4620.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"198"},{"_position":{"x":5145.0,"y":4710.0},"$type":"NodeCanvas
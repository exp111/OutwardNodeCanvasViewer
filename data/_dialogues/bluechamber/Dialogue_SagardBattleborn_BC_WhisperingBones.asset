  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-4587.511,"y":-3826.256},"nodes":[{"_condition":{"questRef":{"_value":{"m_itemID":7011203}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":3720.0,"y":3135.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_action":{"dialogueStarter":{"_name":"Dialogue_SagardBattleborn_BB/BC_AncPeacemaker","_targetVariableID":"27c7a44a-6530-43f1-a456-e54836b2b35e"},"$type":"NodeCanvas.Tasks.Actions.BranchDialogue"},"_position":{"x":3285.0,"y":3360.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"6"},{"_condition":{"QuestEventRef":{"m_eventUID":"BIg2OuMcd0W5izEaDq0JxA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3810.0,"y":3390.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"9"},{"_condition":{"QuestEventRef":{"m_eventUID":"ft8ICwXzBESOrn78N1N3ww"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3450.0,"y":3570.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"12"},{"statement":{"_text":"Seriously, what is going on in my damn city?! Why is no one giving me a damn straight answer?!","_meta":"Sagard_BC_Bones_05"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3330.0,"y":4260.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"15"},{"_targetNode":{"availableChoices":[{"statement":{"_text":"Farewell.","_meta":"Merchant_Generic_Farewell_01"}},{"statement":{"_text":"I want to train with you.","_meta":"Merchant_Generic_Train_01"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"skill":{"_value":1},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_KnowSkill"}}],"_position":{"x":4830.0,"y":3675.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"18"},"_position":{"x":3765.0,"y":4935.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"17"},{"statement":{"_text":"Oy, you! Yes, you! Go fetch Rissa! One of her idiots just tried to stab me!","_meta":"Sagard_BC_Bones_01"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3690.0,"y":3720.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"27"},{"availableChoices":[{"statement":{"_text":"Is that Julius''s corpse behind you? What happened here?","_meta":"Player_Sagard_BC_Bones_a_01"}}],"_position":{"x":3825.0,"y":3810.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"29"},{"statement":{"_text":"He was talking with a girl in red, and it turned into an argument. She was telling him that there was something she needed to do, and he was refusing, telling her that he wouldn''t let her \"throw her life away\". ","_meta":"Sagard_BC_Bones_02"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3885.0,"y":3930.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"33"},{"statement":{"_text":"Then she said <color=#FF0000><b>\"If I don''t get into that necropolis, my mistress will kill me\"</b></color>, and I decided it might be a good idea to them ask a few questions.","_meta":"Sagard_BC_Bones_06"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3990.0,"y":4050.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"35"},{"statement":{"_text":"That''s when your friend over there decided to try to take me with a dagger, and the girl grabbed something off his belt and bolted for the door!","_meta":"Sagard_BC_Bones_03"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3915.0,"y":4155.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"37"},{"availableChoices":[{"statement":{"_text":"Julius, you idiot. This sounds like the oldest story in the book.","_meta":"Player_Sagard_BC_Bones_a_03"}}],"_position":{"x":4050.0,"y":4245.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"39"},{"statement":{"_text":"What are you blabbering about? One of Rissa\u2019s goons tries to murder me and you\u2019re concerned about the girl that ran instead?!","_meta":"Sagard_BC_Bones_04"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4020.0,"y":4335.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"43"},{"availableChoices":[{"statement":{"_text":"You''ll have to go get Rissa yourself. I need to catch up with that woman before she breaks something important in the necropolis.","_meta":"Player_Sagard_BC_Bones_a_04"}}],"_position":{"x":4140.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"45"},{"_condition":{"QuestEventRef":{"m_eventUID":"5NfRrLWmaUqlWrev4DCIYg"},"ExpiryTime":960,"$type":"NodeCanvas.Tasks.Conditions.Condition_CheckQuestEventExpiry"},"_position":{"x":4035.0,"y":4575.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"49"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"ft8ICwXzBESOrn78N1N3ww"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"yrzBAz3Rwke-w-0YmbVz_Q"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"uKt6piKIsUCOle_AD9PNtg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3930.0,"y":4695.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"52"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"ft8ICwXzBESOrn78N1N3ww"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"uKt6piKIsUCOle_AD9PNtg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4245.0,"y":4695.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"61"},{"statement":{"_text":"Unless you have big news or we\u2019re under attack or you\u2019re here to drink with me, I don\u2019t think I have much to say to you. Free time is hard to come by for a Chamberlord of such a big city and intend to enjoy every moment I can snatch.","_meta":"Sagard_BC_Prequest_07"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4200.0,"y":3570.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"68"},{"$ref":"18"},{"_position":{"x":4515.0,"y":3915.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"70"},{"statement":{"_text":"If you pay me, sure, I\u2019ll give you a private lesson. You wanna know the most effective way to finish off a opponent with a 2-handed axe once you\u2019ve got them on the floor? Pay my tab for tonight, about 50 silver, and I\u2019ll teach you.","_meta":"Trainer_Micro_Finisher_01"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5160.0,"y":3960.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"71"},{"availableChoices":[{"statement":{"_text":"Deal, here\u2019s your money.","_meta":"Merchant_Generic_Pay_01"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":50},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"No, thanks.","_meta":"Merchant_Generic_No_02"}}],"_position":{"x":5190.0,"y":4095.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"73"},{"statement":{"_text":"Alright, come with me. We\u2019re going to go practice this on a couple of old logs. Here, I\u2019ll even let you use my-","_meta":"Trainer_Micro_Finisher_02"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4935.0,"y":4275.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"85"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":5055.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"87"},{"_action":{"actions":[{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":50}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"},{"RewardReceiver":"Instigator","SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":8100300}},"Quantity":{"_value":1},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":5055.0,"y":4500.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"89"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":5085.0,"y":4620.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"108"},{"statement":{"_text":"Hah, there you go, you\u2019ve gotten the hang of it! Remember, while you turn for extra momentum, you should step forward and put your full weight into it!","_meta":"Trainer_Micro_Finisher_03"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4995.0,"y":4710.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"110"},{"_position":{"x":5175.0,"y":4845.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"112"},{"_position":{"x":5490.0,"y":4290.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"113"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"6"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"9"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"9"},"_targetNode":{"$ref":"12"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"9"},"_targetNode":{"$ref":"68"},"$type":"NodeCanvas.Dialogue
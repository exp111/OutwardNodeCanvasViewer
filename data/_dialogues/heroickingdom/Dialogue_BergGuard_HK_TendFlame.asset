  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-299.0,"y":-769.0},"zoomFactor":0.2500012,"nodes":[{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"vW4sarzBGkalTwy_KhGI6A"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"75jOpzbxWk6tLFZ1W04nPg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4320.0,"y":3675.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"2"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.SetDialogueInterruptable"},"_position":{"x":4354.0,"y":3894.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"9"},{"statement":{"_text":"Halt right there, [PlayerName]! We saw you go into the archives shortly before it exploded and mercenaries from Levant came charging out! You\u2019re coming with us!","_meta":"ExilerGuard_HK_TendFlame_01"},"_actorName":"name_unpc_bergguard_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4228.0,"y":4010.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"11"},{"availableChoices":[{"statement":{"_text":"It wasn\u2019t me! They\u2019re just using me as a patsy!","_meta":"Player_ExilerGuard_HK_TendFlame_a_01"}},{"statement":{"_text":"Wait, what do you mean mercenaries from Levant came charging out?!","_meta":"Player_ExilerGuard_HK_TendFlame_b_01"}},{"statement":{"_text":"Wait, wait, bring Rissa over here, I\u2019m sure we can work something out!","_meta":"Player_ExilerGuard_HK_TendFlame_c_01"}}],"_position":{"x":4303.0,"y":4160.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"13"},{"statement":{"_text":"Don\u2019t play dumb with us! After the bomb went off, those mercenaries <color=#00CCFF><b>kidnapped Rissa and your ambassador, Kirouac and took off with them to somewhere to the South of the forest</b></color>! Tell me where their hideout is and maybe I won\u2019t hurt you!","_meta":"ExilerGuard_HK_TendFlame_02"},"_actorName":"name_unpc_bergguard_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4243.0,"y":4340.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"21"},{"availableChoices":[{"statement":{"_text":"I\u2019m going to get to the bottom of this. I\u2019m going after them.","_meta":"Player_Gabriella_TendFlame_a_02"}},{"statement":{"_text":"They took Kirouac? Dammit, there goes my payday!","_meta":"Player_Gabriella_TendFlame_b_02"}},{"statement":{"_text":"Wait, Rissa\u2019s been taken too?!","_meta":"Player_ExilerGuard_HK_TendFlame_a_02"}}],"_position":{"x":4348.0,"y":4490.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"23"},{"statement":{"_text":"Enough of this! Get down on your knees, criminal! You\u2019re coming with us! Do not resist!","_meta":"ExilerGuard_HK_TendFlame_03"},"_actorName":"name_unpc_bergguard_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4273.0,"y":4685.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"31"},{"availableChoices":[{"statement":{"_text":"(Run away) I\u2019m getting out of here!","_meta":"Player_ExilerGuard_HK_TendFlame_a_03"}}],"_position":{"x":4378.0,"y":4820.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"33"},{"statement":{"_text":"Hey! HEY! HALT! Get back here, you Levantin scum!","_meta":"ExilerGuard_HK_TendFlame_04"},"_actorName":"name_unpc_bergguard_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4318.0,"y":4940.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"37"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.SetDialogueInterruptable"},"_position":{"x":4370.0,"y":5082.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"39"},{"_action":{"Area":{"_value":"Emercar"},"SpawnPoint":{},"$type":"NodeCanvas.Tasks.Actions.AreaSwitchPlayers"},"_position":{"x":4308.0,"y":5413.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"41"},{"_position":{"x":43
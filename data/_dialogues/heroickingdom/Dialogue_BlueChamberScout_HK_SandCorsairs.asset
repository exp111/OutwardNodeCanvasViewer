  m_Name: Dialogue_BlueChamberScout_HK_SandCorsairs
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-2620.0,"y":-3595.0},"zoomFactor":0.8057283,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"4NE1-GUpO0KiCVKM0LQjyg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3930.0,"y":3360.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"Don\u2019t thank me. We may have found an arrangement, but we\u2019re still on opposite sides of the war. Until the boss man calls us off, we\u2019re enemies.","_meta":"Scout_HK_Corsairs_09"},"_actorName":"name_unpc_bluechamberscout_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3765.0,"y":3600.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"5"},{"_position":{"x":3900.0,"y":3720.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"7"},{"statement":{"_text":"Halt! State your business...Here? I remember you. That trog spawn from Cierzo with the Blood Price. What are you doing here?","_meta":"Scout_HK_Corsairs_01"},"_actorName":"name_unpc_bluechamberscout_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4155.0,"y":3495.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"8"},{"availableChoices":[{"statement":{"_text":"I should be asking that. What''s a Blue Chamber tribesman doing working with the Sand Corsairs?","_meta":"Player_Scout_HK_Corsairs_a_01"}},{"statement":{"_text":"..."}},{"statement":{"_text":"..."}}],"_position":{"x":4260.0,"y":3630.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"10"},{"statement":{"_text":"None of your business. Now, you will turn around and walk away and not look back. We have orders to kill anyone who interferes with our operation, and I''d rather not spill the blood of someone from my old hometown.","_meta":"Scout_HK_Corsairs_02"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4230.0,"y":3780.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"18"},{"availableChoices":[{"statement":{"_text":"I''m invoking a Tribal Favor. Tell me what I need to know. People are starving to death.","_meta":"Player_Scout_HK_Corsairs_b_02"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":5600050}},"minAmount":{"_value":1},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"Oh look, 100 silver just fell out of my pockets. Enough to buy you a place away from this awful desert.","_meta":"Player_Scout_HK_Corsairs_c_02"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":100},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"You WILL tell me where the Sand Corsairs operate from, or things will get violent.","_meta":"Player_Scout_HK_Corsairs_a_02"}},{"statement":{"_text":"Sorry"}}],"_position":{"x":4215.0,"y":3900.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"20"},{"statement":{"_text":"...I may be on an un-sanctioned mission behind enemy lines, but I remember my duty to the Collective. A Tribal Favor must always be recognized. Fine. Even someone like Sagard should be able to understand.","_meta":"Scout_HK_Corsairs_04"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3630.0,"y":4170.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"42"},{"statement":{"_text":"The Corsairs loot incoming food caravans, and then give us some of the food, to deliver to the Slums. The idea was to starve Levant, and use offers of food to try to push the slum people into open revolt.","_meta":"Scout_HK_Corsairs_06"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3525.0,"y":4335.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"44"},{"statement":{"_text":"The Sand Corsairs are operating out of the ruins of Old Levant. <color=#00CCFF><b>It''s that old ruined city under Abrassar''s Vigil crystal</b></color>. Not that it''ll do you any good, <color=#00CCFF><b>you could never take the place on your own</b></color>.","_meta":"Scout_HK_Corsairs_07"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3570.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"46"},{"statement":{"_text":"There are dozens of men there, and it''s been camouflaged by some Soroboreans as an archaeology site. Even if you could get past them, the Corsairs have an army in there. You\u2019d be walking into a deathtrap.","_meta":"Scout_HK_Corsairs_08"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3525.0,"y":4575.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"48"},{"availableChoices":[{"statement":{"_text":"Sounds like I need backup then. Thank you for the information.","_meta":"Player_Scout_HK_Corsairs_a_08"}}],"_position":{"x":3660.0,"y":4680.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"50"},{"statement":{"_text":"Don\u2019t thank me. We may have found an arrangement, but we\u2019re still on opposite sides of the war. Until the boss man calls us off, we\u2019re enemies.","_meta":"Scout_HK_Corsairs_09"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3495.0,"y":4785.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"54"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"4NE1-GUpO0KiCVKM0LQjyg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"870p43IYo0Oh3H7l21iYvg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":5600050}}],"Amount":[{"_value":1}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3465.0,"y":4905.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"56"},{"_action":{"QuestEventRef":{"m_eventUID":"nuPhQGKDlkSKlz_Y0zixSg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3435.0,"y":5055.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"70"},{"_position":{"x":3540.0,"y":5190.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"73"},{"statement":{"_text":"...This could buy our troops a lot of much-needed weapons...Fine. I don''t like what the Corsairs are doing, so I''ll trade your silver for what I know. Sagard won''t be happy, but he''ll understand.","_meta":"Scout_HK_Corsairs_05"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4125.0,"y":4170.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"74"},{"statement":{"_text":"The Corsairs loot incoming food caravans, and then give us some of the food, to deliver to the Slums. The idea was to starve Levant, and use offers of food to try to push the slum people into open revolt.","_meta":"Scout_HK_Corsairs_06"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4080.0,"y":4320.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"76"},{"statement":{"_text":"The Sand Corsairs are operating out of the ruins of Old Levant. <color=#00CCFF><b>It''s that old ruined city under Abrassar''s Vigil crystal</b></color>. Not that it''ll do you any good, <color=#00CCFF><b>you could never take the place on your own</b></color>.","_meta":"Scout_HK_Corsairs_07"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4125.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"78"},{"statement":{"_text":"There are dozens of men there, and it''s been camouflaged by some Soroboreans as an archaeology site. Even if you could get past them, the Corsairs have an army in there. You\u2019d be walking into a deathtrap.","_meta":"Scout_HK_Corsairs_08"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4080.0,"y":4575.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"80"},{"availableChoices":[{"statement":{"_text":"Sounds like I need backup then. Thank you for the information.","_meta":"Player_Scout_HK_Corsairs_a_08"}}],"_position":{"x":4230.0,"y":4680.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"82"},{"statement":{"_text":"Don\u2019t thank me. We may have found an arrangement, but we\u2019re still on opposite sides of the war. Until the boss man calls us off, we\u2019re enemies.","_meta":"Scout_HK_Corsairs_09"},"_actorName":"Blue Chamber Scout","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4065.0,"y":4800.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"86"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"4NE1-GUpO0KiCVKM0LQjyg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"DQV1p47sv0-Lu2XKTnQafw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":100}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4170.0,"y":4920.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"88"},{"_action":{"QuestEventRef":{"m_eventUID":"nuPhQGKDlkSKlz_Y0zixSg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4170.0,"y":5055.0},"$type":"NodeCanvas.DialogueTr
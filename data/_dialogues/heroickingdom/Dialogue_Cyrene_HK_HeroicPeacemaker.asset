  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-4000.471,"y":-3545.044},"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"w_7mviMtzkOhcHYq_SgtBg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4185.0,"y":3405.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_condition":{"QuestEventRef":{"m_eventUID":"KDM8FIvcJkSarO7j93SVYw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4095.0,"y":3615.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"5"},{"statement":{"_text":"Yzan, how could you turn on us? I thought you understood! This peace must not be allowed to happen, or Levant has no future!","_meta":"Cyrene_HK_HeroPeace_01"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3585.0,"y":3885.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"8"},{"statement":{"_text":"Cyrene, stop this now! If we don''t stop this war, all we do is damn Levant to destruction!","_meta":"Yzan_HK_HeroPeace_22"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"bca54c73-11ce-4a6b-a22a-01330b6fba0c","_position":{"x":3555.0,"y":4005.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"10"},{"statement":{"_text":"Never. I will not let the slaughter of my troops go unanswered, and I will never forgive those damn Blues for the famine they caused. Besides, it''s too late. I''ve already shot Calixa with a Scourge Venom Bullet.","_meta":"Cyrene_HK_HeroPeace_02"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3540.0,"y":4110.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"12"},{"statement":{"_text":"As the new queen of Levant, I will not abide traitors to the city like you. Prepare to meet your ancestors, [PlayerName]!","_meta":"Cyrene_HK_HeroPeace_06"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3555.0,"y":4230.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"14"},{"statement":{"_text":"I''m sorry it had to come to this. If you won''t stand aside, then I will make you stand aside! Defend yourself!","_meta":"Yzan_HK_HeroPeace_23"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"bca54c73-11ce-4a6b-a22a-01330b6fba0c","_position":{"x":3570.0,"y":4350.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"16"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":4065.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"18"},{"_action":{"QuestEventRef":{"m_eventUID":"rFwrK5u9gEa46_FzMg11ow"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3960.0,"y":4560.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"20"},{"_position":{"x":4050.0,"y":4770.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"23"},{"statement":{"_text":"You''re too late, [PlayerName]. It''s over. Your king and queen lie broken on the floor","_meta":"Cyrene_HK_HeroPeace_04"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4230.0,"y":3855.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"24"},{"availableChoices":[{"statement":{"_text":"What have you done to them?!","_meta":"Player_Cyrene_HK_HeroPeace_a_04"}}],"_position":{"x":4410.0,"y":4005.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"26"},{"statement":{"_text":"I shot Calixa with a bullet containing an incurable poison. If Zephyrien, a champion of Elatt, could not survive it, I don''t see how a mere mortal can. And without her watching his back, Simeon''s recklessness was easy to exploit.","_meta":"Cyrene_HK_HeroPeace_05"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4290.0,"y":4170.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"30"},{"statement":{"_text":"As the new queen of Levant, I will not abide traitors to the city like you. Prepare to meet your ancestors, [PlayerName]!","_meta":"Cyrene_HK_HeroPeace_06"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4275.0,"y":4305.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"32"},{"statement":{"_text":"Keep walking. I\u2019m on duty and have no time for you. Or anyone else for that matter. Shoo.","_meta":"Cyrene_BC_Mix_Legacy_01"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4485.0,"y":3555.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"34"},{"_position":{"x":4620.0,"y":3690.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"36"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"34"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"8"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"24"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"8"},"_targetNode":{"$ref":"10"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"10"},"_targetNode":{"$ref":"12"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"12"},"_targetNode":{"$ref":"14"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"14"},"_targetNode":{"$ref":"16"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"16"},"_targetNode":{"$ref":"18"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"18"},"_targetNode":{"$ref":"20"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"20"},"_targetNode":{"$ref":"23"},"
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-1364.0,"y":-1638.0},"zoomFactor":0.573065,"nodes":[{"_condition":{"questRef":{"_value":{"m_itemID":7011301}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":3270.0,"y":2970.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_action":{"dialogueStarter":{"_name":"Dialogue_Kirouac_BB/HK_MouthToFeed","_targetVariableID":"c16fbb68-54f4-42ee-9583-b738247b2700"},"$type":"NodeCanvas.Tasks.Actions.BranchDialogue"},"_position":{"x":2715.0,"y":3240.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"6"},{"_condition":{"checkMode":"AnyTrueSuffice","conditions":[{"QuestEventRef":{"m_eventUID":"bUCPtMJfwEeO6rc3khS3Gw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"SQ29t58YcUWxaqsGjiJ-sw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":3615.0,"y":3180.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"9"},{"_condition":{"QuestEventRef":{"m_eventUID":"SQ29t58YcUWxaqsGjiJ-sw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3435.0,"y":3390.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"16"},{"_condition":{"QuestEventRef":{"m_eventUID":"bUCPtMJfwEeO6rc3khS3Gw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2850.0,"y":3480.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"19"},{"_action":{"QuestEventRef":{"m_eventUID":"bUCPtMJfwEeO6rc3khS3Gw"},"RemoveAllStack":true,"$type":"NodeCanvas.Tasks.Actions.RemoveQuestEvent"},"_position":{"x":2565.0,"y":3630.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"22"},{"_condition":{"QuestEventRef":{"m_eventUID":"75jOpzbxWk6tLFZ1W04nPg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2670.0,"y":3825.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"25"},{"availableChoices":[{"statement":{"_text":"There were some mercenaries down there with a bomb. I managed to stop them.","_meta":"Player_Kirouac_TendFlame_a_11"}}],"_position":{"x":2370.0,"y":4080.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"28"},{"statement":{"_text":"Oh Elatt\u2019s breath, this is the last thing we needed. This could cause a major diplomatic incident! Alright, you go and try to track down where they came from, I\u2019ll try to handle the diplomatic side of things.","_meta":"Kirouac_TendFlame_12"},"_actorName":"name_unpc_kirouac_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2235.0,"y":4230.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"32"},{"statement":{"_text":"If you don\u2019t have any leads, double check downstairs for evidence, in case you missed something.","_meta":"Kirouac_TendFlame_13"},"_actorName":"name_unpc_kirouac_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2175.0,"y":4320.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"34"},{"_position":{"x":2295.0,"y":4440.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"36"},{"_action":{"QuestEventRef":{"m_eventUID":"75jOpzbxWk6tLFZ1W04nPg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":2775.0,"y":3975.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"37"},{"_condition":{"QuestEventRef":{"m_eventUID":"ffzi9_pt3kmSc6wBR7VvAg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3525.0,"y":3750.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"40"},{"statement":{"_text":"That was too much excitement for one lifetime. I''m not going anywhere outside this city ever again, mark my words!","_meta":"Kirouac_TendFlame_06"},"_actorName":"name_unpc_kirouac_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3210.0,"y":3915.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"43"},{"availableChoices":[{"statement":{"_text":"Did you manage to convince the chamberlords that we aren''t responsible for the attack?","_meta":"Player_Kirouac_TendFlame_a_06"}}],"_position":{"x":3285.0,"y":4080.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"45"},{"statement":{"_text":"I was barely able to get myself and my entourage out of Berg before they declared war on us. There was nothing I could do to stop it.","_meta":"Kirouac_TendFlame_07"},"_actorName":"Kirouac","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3135.0,"y":4185.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"49"},{"_position":{"x":3240.0,"y":4350.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"51"},{"statement":{"_text":"Oh thank the winds, rescue! Thanks for coming!","_meta":"Kirouac_TendFlame_01"},"_actorName":"name_unpc_kirouac_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3810.0,"y":3960.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"52"},{"availableChoices":[{"statement":{"_text":"Kirouac! What happened?","_meta":"Player_Kirouac_TendFlame_a_01"}}],"_position":{"x":3840.0,"y":4095.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"54"},{"statement":{"_text":"I don''t know. There was an explosion, and then thugs armed to the teeth came pouring out from under the Blue Chamber. They shot the Old Levant ambassador and then grabbed me and stuffed me in a sack!","_meta":"Kirouac_TendFlame_02"},"_actorName":"Kirouac","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3735.0,"y":4260.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"58"},{"statement":{"_text":"Someone''s trying to frame us for killing Old Levant\u2019s Ambassador, I\u2019m sure of it. There''s no way Calixa or Simeon would have authorized something like this. We need to do something before the Blue Chamber takes this as an act of war!","_meta":"Kirouac_TendFlame_03"},"_actorName":"Kirouac","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3720.0,"y":4380.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"60"},{"statement":{"_text":"I can get back to Berg from here. I need to try to smooth this calamity over before it\u2019s too late!","_meta":"Kirouac_TendFlame_04"},"_actorName":"Kirouac","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3705.0,"y":4500.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"62"},{"statement":{"_text":"<color=#00CCFF><b>Investigate further if you feel you need to</b></color>. As soon as you\u2019re done though, <color=#00CCFF><b>report back to Cyrene in Levant</b></color>. I get the feeling Enmerkar might not be safe for any of us soon enough.","_meta":"Kirouac_TendFlame_05"},"_actorName":"Kirouac","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3720.0,"y":4650.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"64"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":3870.0,"y":4755.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"66"},{"_action":{"QuestEventRef":{"m_eventUID":"ffzi9_pt3kmSc6wBR7VvAg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3795.0,"y":4875.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"68"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":3870.0,"y":4965.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"71"},{"_position":{"x":3870.0,"y":5070.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"73"},{"_condition":{"QuestEventRef":{"m_eventUID":"yClnldXH2UOcBtJ8XtrZGA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4425.0,"y":3750.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"74"},{"statement":{"_text":"Hey, [PlayerName], I think Cyrene\u2019s gone. We need her here in case something happens...Can you check Berg\u2019s Inn real quick for her?","_meta":"Kirouac_TendFlame_08"},"_actorName":"name_unpc_kirouac_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4260.0,"y":3915.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"77"},{"_position":{"x":4485.0,"y":4170.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"79"},{"_condition":{"QuestEventRef":{"m_eventUID":"AA0VyQO--kqZFB2KLzx71Q"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4890.0,"y":3930.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"80"},{"statement":{"_text":"I don\u2019t understand why the Collective is even giving the claims of Old Levant any attention. Calixa and Simeon were named rulers of Abrassar desert by Elatt himself. ","_meta":"Kirouac_BC_Mix_Legacy_01"},"_actorName":"name_unpc_kirouac_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4695.0,"y":4035.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"83"},{"statement":{"_text":"Old Levant abandoned Abrassar to the beasts of the Scourge. They have no right to demand to have it back after we did all the hard work of cleansing it of the scourge and re-settling it!","_meta":"Kirouac_BC_Mix_Legacy_02"},"_actorName":"name_unpc_kirouac_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4710.0,"y":4155.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"85"},{"statement":{"_text":"The doors of the Heroic Kingdom are open to them. They\u2019re welcome to come join us! And yet they spit in our outstretched hand again and again. I don\u2019t understand why they cling to the ghost of their past pride so tightly.","_meta":"Kirouac_TendFlame_10"},"_actorName":"name_unpc_kirouac_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4725.0,"y":4260.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"87"},{"_position":{"x":4890.0,"y":4365.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"89"},{"statement":{"_text":"Oh, hello, [PlayerName]. How are you doing?","_meta":"Kirouac_TendFlame_09"},"_actorName":"name_unpc_kirouac_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5160.0,"y":4065.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"90"},{"_position":{"x":5355.0,"y":4290.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"92"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"6"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"9"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"9"},"_targetNode":{"$ref":"16"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"9"},"_targetNode":{"$ref":"74"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"16"},"_targetNode":{"$ref":"19"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"16"},"_targetNode":{"$ref":"40"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"19"},"_targetNode":{"$ref":"22"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"19"},"_targetNode":{"$ref":"25"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"22"},"_targetNode":{"$ref":"25"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"25"},"_targetNode":{"$ref":"28"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"25"},"_targetNode":{"$ref":"37"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"28"},"_targetNode":{"$ref":"32"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"32"},"_targetNode":{"$ref":"34"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"34"},"_targetNode":{"$ref":"36"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"37"},"_t
  m_Name: Dialogue_YzanArgenson_HK_HeroicPeacemaker
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-2192.0,"y":-3125.0},"nodes":[{"_condition":{"questRef":{"_value":{"m_itemID":7011304}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":3105.0,"y":3180.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"Simeon made me his top general. Honestly, considering how close I was to turning on them both...I don\u2019t really feel like I deserve it. Thank you for everything, [PlayerName]. I wouldn\u2019t be here without you. I\u2019m glad you came to Levant.","_meta":"Yzan_HK_HeroPeace_28"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2805.0,"y":3315.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"6"},{"_position":{"x":2667.0,"y":3469.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"8"},{"_condition":{"QuestEventRef":{"m_eventUID":"wwfkTyWcbUqQJsNRGzVzbA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3330.0,"y":3405.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"9"},{"statement":{"_text":"That hurt to do. But I''m convinced now, it was the right thing for Levant. Thank you for pulling me back from that abyss, [PlayerName]. Levant and I are in your debt.","_meta":"Yzan_HK_HeroPeace_26"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2787.0,"y":3529.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"12"},{"_condition":{"QuestEventRef":{"m_eventUID":"YRXtMmD6BEWQ43gWY2GHOg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_color":{"r":0.7019608,"g":1.0,"b":0.4980392,"a":1.0},"_position":{"x":2709.0,"y":3667.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"14"},{"statement":{"_text":"<color=#00CCFF><b>Go, hurry to Monsoon</b></color>. I''ll look after the city while all of you are gone. You can trust me, [PlayerName].","_meta":"Yzan_HK_HeroPeace_27"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2500.0,"y":3854.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"17"},{"_position":{"x":2575.0,"y":3974.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"19"},{"_position":{"x":2963.0,"y":3850.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"20"},{"_condition":{"QuestEventRef":{"m_eventUID":"C7CzIM_xqkie9ZjWQwWWKg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3720.0,"y":3555.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"21"},{"_condition":{"QuestEventRef":{"m_eventUID":"KDM8FIvcJkSarO7j93SVYw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3210.0,"y":3840.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"24"},{"statement":{"_text":"Right on time, [PlayerName]. Let\u2019s put an end to this coup, together.","_meta":"Player_Yzan_HK_HeroPeace_c_31"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2850.0,"y":4080.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"27"},{"statement":{"_text":"Yzan, how could you turn on us? I thought you understood! This peace must not be allowed to happen, or Levant has no future!","_meta":"Cyrene_HK_HeroPeace_01"},"_actorName":"name_unpc_cyrene_01","_actorParameterID":"ef42df9a-8227-40a1-be68-a074df17c119","_position":{"x":2685.0,"y":4275.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"29"},{"statement":{"_text":"Cyrene, stop this now! If we don''t stop this war, all we do is damn Levant to destruction!","_meta":"Yzan_HK_HeroPeace_22"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"bca54c73-11ce-4a6b-a22a-01330b6fba0c","_position":{"x":2595.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"31"},{"statement":{"_text":"Never. I will not let the slaughter of my troops go unanswered, and I will never forgive those damn Blues for the famine they caused. Besides, it''s too late. I''ve already shot Calixa with a Scourge Venom Bullet.","_meta":"Cyrene_HK_HeroPeace_02"},"_actorName":"name_unpc_cyrene_01","_actorParameterID":"ef42df9a-8227-40a1-be68-a074df17c119","_position":{"x":2580.0,"y":4500.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"33"},{"statement":{"_text":"As the new queen of Levant, I will not abide traitors to the city like you. Prepare to meet your ancestors, [PlayerName]!","_meta":"Cyrene_HK_HeroPeace_06"},"_actorName":"name_unpc_cyrene_01","_actorParameterID":"ef42df9a-8227-40a1-be68-a074df17c119","_position":{"x":2595.0,"y":4605.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"35"},{"statement":{"_text":"I''m sorry it had to come to this. If you won''t stand aside, then I will make you stand aside! Defend yourself!","_meta":"Yzan_HK_HeroPeace_23"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"bca54c73-11ce-4a6b-a22a-01330b6fba0c","_position":{"x":2595.0,"y":4725.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"37"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":3105.0,"y":4845.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"39"},{"_action":{"QuestEventRef":{"m_eventUID":"rFwrK5u9gEa46_FzMg11ow"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":2985.0,"y":4950.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"41"},{"_position":{"x":3075.0,"y":5145.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"44"},{"statement":{"_text":"You\u2019re too late, [PlayerName]. You\u2019ve lost. Calixa is doomed, and Simeon is defeated. Nothing stands between Cyrene and the throne!","_meta":"Player_Yzan_HK_HeroPeace_c_32"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3315.0,"y":4050.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"45"},{"availableChoices":[{"statement":{"_text":"What have you done to them?!","_meta":"Player_Cyrene_HK_HeroPeace_a_04"}}],"_position":{"x":3360.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"47"},{"statement":{"_text":"I shot Calixa with a bullet containing an incurable poison. If Zephyrien, a champion of Elatt, could not survive it, I don''t see how a mere mortal can. And without her watching his back, Simeon''s recklessness was easy to exploit.","_meta":"Cyrene_HK_HeroPeace_05"},"_actorName":"name_unpc_cyrene_01","_actorParameterID":"ef42df9a-8227-40a1-be68-a074df17c119","_position":{"x":3330.0,"y":4560.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"51"},{"statement":{"_text":"As the new queen of Levant, I will not abide traitors to the city like you. Prepare to meet your ancestors, [PlayerName]!","_meta":"Cyrene_HK_HeroPeace_06"},"_actorName":"name_unpc_cyrene_01","_actorParameterID":"ef42df9a-8227-40a1-be68-a074df17c119","_position":{"x":3315.0,"y":4695.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"53"},{"_condition":{"QuestEventRef":{"m_eventUID":"KDM8FIvcJkSarO7j93SVYw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3960.0,"y":3780.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"55"},{"statement":{"_text":"Dammit, what a mess. <color=#00CCFF><b>We need to stop Cyrene</b></color>. She told me she has a bullet loaded with an incurable poison, designed to pierce any armor. She''s going to use it on Calixa, then order the army to go into open revolt.","_meta":"Yzan_HK_HeroPeace_19"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3690.0,"y":3870.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"58"},{"availableChoices":[{"statement":{"_text":"But what about Simeon? How will she get past him?","_meta":"Player_Yzan_HK_HeroPeace_a_19"}}],"_position":{"x":3870.0,"y":3990.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"60"},{"statement":{"_text":"Simeon may be a great fighter, but he''s often careless. With Calixa taken down, Cyrene will be more than a match for him if she takes any backup with her. She''s been planning for this possibility ever since the attack on Berg.","_meta":"Yzan_HK_HeroPeace_20"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3735.0,"y":4110.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"64"},{"statement":{"_text":"We don''t have much time, [PlayerName]. I''m going to take my personal troops and turn them back around to defend the city. <color=#00CCFF><b>You go warn Calixa</b></color>, I''ll meet you there!","_meta":"Yzan_HK_HeroPeace_21"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3720.0,"y":4230.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"66"},{"_position":{"x":3885.0,"y":4350.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"68"},{"_condition":{"QuestEventRef":{"m_eventUID":"oCIusrhya0CNGRR-tLY-Sg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4625.0,"y":3941.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"69"},{"_condition":{"QuestEventRef":{"m_eventUID":"Uy-GTqjsHE2DuHxnsvyEAQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4425.0,"y":4125.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"72"},{"_action":{"QuestEventRef":{"m_eventUID":"Uy-GTqjsHE2DuHxnsvyEAQ"},"RemoveAllStack":true,"$type":"NodeCanvas.Tasks.Actions.RemoveQuestEvent"},"_position":{"x":4290.0,"y":4290.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"75"},{"_condition":{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"MinStack":3,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4455.0,"y":4470.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"78"},{"statement":{"_text":"[PlayerName], I take it you got my message? I\u2019m glad you made it.","_meta":"Yzan_HK_HeroPeace_01"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4185.0,"y":4635.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"81"},{"statement":{"_text":"Well, now that you''re here, I may as well ask. Cyrene and I could use your help. We need to move against Calixa and Simeon before they agree to this ridiculous peace and doom us all.","_meta":"Yzan_HK_HeroPeace_03"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4440.0,"y":4755.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"83"},{"availableChoices":[{"statement":{"_text":"You''d betray Levant, Yzan?","_meta":"Player_Yzan_HK_HeroPeace_a_03"}}],"_position":{"x":4575.0,"y":4875.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"85"},{"statement":{"_text":"No, it''s Simeon and Calixa who are betraying Levant. They want to make peace with an enemy that has committed unforgivable crimes against us. They shame the losses of every man and woman who has died in this war.","_meta":"Yzan_HK_HeroPeace_04"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4485.0,"y":4965.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"89"},{"statement":{"_text":"Why are you hesitating? The head that wears the crown doesn''t matter, it''s what they stand for. Simeon and Calixa didn''t stop the Sand Corsairs or bring food to Levant. We did that. You and me.","_meta":"Yzan_HK_HeroPeace_05"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4455.0,"y":5085.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"91"},{"availableChoices":[{"statement":{"_text":"You think that the people who founded the Heroic Kingdom have forgotten what it''s about?","_meta":"Player_Yzan_HK_HeroPeace_a_05"}},{"statement":{"_text":"Maybe they just aren''t blinded by rage like you and Cyrene. You''ve both been on the front lines too much to see the bigger picture.","_meta":"Player_Yzan_HK_HeroPeace_c_05"}},{"statement":{"_text":"They could have done more if they weren''t forced to pour so many resources and people into this losing war!","_meta":"Player_Yzan_HK_HeroPeace_b_05"}}],"_position":{"x":4470.0,"y":5235.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"93"},{"statement":{"_text":"Power corrupts the mind. They have held power for too long and grown complacent! I see no other reason why they''d accept peace talks! All this means is that once the war is over, we should change our form of government to one with more oversight!","_meta":"Yzan_HK_HeroPeace_06"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4125.0,"y":5445.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"101"},{"_targetNode":{"statement":{"_text":"Why are you arguing to spare the Blue Chamber from our justice? This war has only proven that the ideals of Levant are better for everyone than what the Blue Chamber believes in!","_meta":"Yzan_HK_HeroPeace_09"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4410.0,"y":5835.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"104"},"_position":{"x":4245.0,"y":5550.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"103"},{"statement":{"_text":"You''re telling me that I should just let go what happened to our troops? To our people? Rage is the only appropriate response!","_meta":"Yzan_HK_HeroPeace_08"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4410.0,"y":5505.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"106"},{"_targetNode":{"$ref":"104"},"_position":{"x":4515.0,"y":5625.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"108"},{"statement":{"_text":"I...I know that''s right. I know how badly the war depleted our treasury. But still! They should have done more! Simeon should have been on the front lines with us!","_meta":"Yzan_HK_HeroPeace_07"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4740.0,"y":5415.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"109"},{"_action":{"QuestEventRef":{"m_eventUID":"Uy-GTqjsHE2DuHxnsvyEAQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4725.0,"y":5595.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"111"},{"$ref":"104"},{"availableChoices":[{"statement":{"_text":"You left Cierzo because of how they forced Roland''s problems onto you. You''re doing that same exact thing to Levant right now with this damn civil war!","_meta":"Player_Yzan_HM_HaPeace_c_10"}},{"statement":{"_text":"Don''t pretend to be so self-righteous. You just still hold a grudge against the Blue Chamber Collective for what happened with Roland!","_meta":"Player_Yzan_HK_HeroPeace_a_09"}},{"statement":{"_text":"So regicide, betrayal and brutal crackdowns on your poorest citizens are the true ideals of Levant?","_meta":"Player_Yzan_HM_HaPeace_b_10"}}],"_position":{"x":4485.0,"y":5970.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"114"},{"statement":{"_text":"I...No, you can''t say what we''re doing is the same! I''m just...I''m trying to protect them! I don''t...Damn you, [PlayerName]!","_meta":"Yzan_HM_HaPeace_13"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4125.0,"y":6150.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"122"},{"_action":{"QuestEventRef":{"m_eventUID":"Uy-GTqjsHE2DuHxnsvyEAQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4155.0,"y":6270.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"124"},{"statement":{"_text":"The Blue Chamber has committed atrocities against us, they''ve let people like Sagard Battleborn run loose and support bandits that wanted only our complete destruction! How can you expect me to forgive them for that?","_meta":"Yzan_HK_HeroPeace_12"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4395.0,"y":6555.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"127"},{"availableChoices":[{"statement":{"_text":"So you''ll become as bad as Sagard in order to take revenge on him?","_meta":"Player_Yzan_HK_HeroPeace_a_12"}},{"statement":{"_text":"So, you want to keep throwing our people into a war we can''t win until we have no one left?","_meta":"Player_Yzan_HK_HeroPeace_b_12"}},{"statement":{"_text":"Yzan, Levant is barely surviving. One more crisis will kill us all. Is this war more important than our survival?","_meta":"Player_Yzan_HK_HeroPeace_c_12"}}],"_position":{"x":4440.0,"y":6690.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"129"},{"statement":{"_text":"You and I both know that''s a load of crap! Sagard tried to starve us to death the moment the war began. Innocent children would have died because of him. This isn''t the same at all!","_meta":"Yzan_HK_HeroPeace_13"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4095.0,"y":6885.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"137"},{"_targetNode":{"statement":{"_text":"Even if I did turn against this coup, it\u2019s too late. It\u2019s already begun. I\u2019m already a traitor. They\u2019d line me up against a wall and shoot me even if I changed sides again. I\u2019m committed now.","_meta":"Yzan_HK_HeroPeace_29"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4440.0,"y":7335.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"140"},"_position":{"x":4185.0,"y":6990.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"139"},{"statement":{"_text":"We have to take that chance. To agree to negotiations now would be nothing less than total surrender. I can''t put my faith in the Holy Mission to protect us from the Blue Chamber''s rapacious demands.","_meta":"Yzan_HK_HeroPeace_14"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4335.0,"y":6990.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"142"},{"_targetNode":{"$ref":"140"},"_position":{"x":4425.0,"y":7125.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"144"},{"statement":{"_text":"Yes! ...No. But...The war is the only way we can survive...Dammit! Either way, I risk dooming my country to destruction! But which option gives us the best odds of survival?","_meta":"Yzan_HK_HeroPeace_15"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4680.0,"y":6885.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"145"},{"_action":{"QuestEventRef":{"m_eventUID":"Uy-GTqjsHE2DuHxnsvyEAQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4680.0,"y":7125.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"147"},{"$ref":"140"},{"availableChoices":[{"statement":{"_text":"Simeon and Calixa don\u2019t know you\u2019re a traitor. That\u2019s why we aren\u2019t fighting each other right now.","_meta":"Player_Yzan_HK_HeroPeace_c_30"}},{"statement":{"_text":"You could just play it like you were a double agent. You\u2019re good at that.","_meta":"Player_Yzan_HK_HeroPeace_a_30"}},{"statement":{"_text":"Some things are worth risking dying for. That\u2019s why you joined Levant, right?","_meta":"Player_Yzan_HK_HeroPeace_b_30"}}],"_position":{"x":4440.0,"y":7530.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"150"},{"statement":{"_text":"You\u2019re asking me to put my life on the line on just a hope and a rumor, [PlayerName]. What kind of risks do you think I\u2019m willing to take?","_meta":"Yzan_HK_HeroPeace_32"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3990.0,"y":7725.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"158"},{"_targetNode":{"_condition":{"QuestEventRef":{"m_eventUID":"Uy-GTqjsHE2DuHxnsvyEAQ"},"MinStack":4,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":5340.0,"y":4290.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"161"},"_position":{"x":4110.0,"y":7845.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"160"},{"statement":{"_text":"Huh. I hate to admit it, but you\u2019re right on that point. The question is, have I become the mask I hide behind, or not?","_meta":"Yzan_HK_HeroPeace_31"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4410.0,"y":7740.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"164"},{"_action":{"QuestEventRef":{"m_eventUID":"Uy-GTqjsHE2DuHxnsvyEAQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4455.0,"y":7875.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"166"},{"_targetNode":{"$ref":"161"},"_position":{"x":4515.0,"y":7980.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"169"},{"_targetNode":{"$ref":"161"},"_position":{"x":4815.0,"y":7740.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"170"},{"statement":{"_text":"And haven''t they proved me right? For all their talk about how \"the tribe is family\", and how we must all come together, they will stop at nothing to destroy anyone who doesn''t bow to their oppressive tribal laws!","_meta":"Yzan_HK_HeroPeace_11"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4470.0,"y":6300.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"171"},{"_targetNode":{"$ref":"127"},"_position":{"x":4620.0,"y":6420.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"173"},{"statement":{"_text":"It''s what the damned Blues pushed us to! We never wanted any of this! Why don''t you ask the them why they think they have the right to decide the fates of nations that aren''t part of them?","_meta":"Yzan_HM_HaPeace_12"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4935.0,"y":6255.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"174"},{"_targetNode":{"$ref":"127"},"_position":{"x":5160.0,"y":6390.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"176"},{"statement":{"_text":"[PlayerName], how did you find me here? We camouflaged ourselves as best we could!","_meta":"Yzan_HK_HeroPeace_02"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4620.0,"y":4635.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"177"},{"statement":{"_text":"I\u2019ve got work to do, [PlayerName]. I can\u2019t stay and chit-chat.","_meta":"Yzan_NE_Call_To_Advent_28"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4764.0,"y":4066.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"179"},{"_position":{"x":4849.0,"y":4199.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"181"},{"statement":{"_text":"You''re too late, [PlayerName]. It''s over. Your king and queen lie broken on the floor","_meta":"Cyrene_HK_HeroPeace_04"},"_actorName":"name_unpc_cyrene_01","_actorParameterID":"ef42df9a-8227-40a1-be68-a074df17c119","_position":{"x":3270.0,"y":4230.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"182"},{"_condition":{"QuestEventRef":{"m_
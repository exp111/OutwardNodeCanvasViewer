  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-3632.0,"y":-3846.0},"zoomFactor":0.9304977,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"m5NbKim2NUK-OLQcjm7fDg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4425.0,"y":3555.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"[PlayerName], you''re awake. Good. You have some time left in this world after all. How do you feel?","_meta":"Elinara_HM_HaPeace_03"},"_actorName":"name_unpc_ellinara_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4245.0,"y":3705.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"5"},{"availableChoices":[{"statement":{"_text":"Not all that terrible, considering. I still feel stronger than I ever have.","_meta":"Player_Elinara_HM_HaPeace_a_03"}}],"_position":{"x":4290.0,"y":3840.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"7"},{"statement":{"_text":"[PlayerName], the light of the Ascension is a balm against the Corruption, but it is not a cure. With every passing day, the Corruption will eat away at you, and destroy you from within. Should you fall now, you will surely not survive.","_meta":"Elinara_HM_HaPeace_04"},"_actorName":"Ellinara","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4260.0,"y":3990.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"11"},{"statement":{"_text":"You likely only have about a month before you truly stand at death''s door. Make good use of the final days you have left to live. Set your affairs to rest as best you can, so you may pass without regrets.","_meta":"Elinara_HM_HaPeace_05"},"_actorName":"name_unpc_ellinara_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4260.0,"y":4125.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"13"},{"availableChoices":[{"statement":{"_text":"Well, I knew what I was getting into...","_meta":"Player_Elinara_HM_HaPeace_a_05"}},{"statement":{"_text":"Oh crap. This was a bad idea.","_meta":"Player_Elinara_HM_HaPeace_b_05"}}],"_position":{"x":4365.0,"y":4260.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"15"},{"statement":{"_text":"Indeed you did. And I thank you for it.","_meta":"Elinara_HM_HaPeace_06"},"_actorName":"name_unpc_ellinara_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4050.0,"y":4425.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"21"},{"statement":{"_text":"The others were able to protect Monsoon from the Scourge attack. There were very few casualties. ","_meta":"Elinara_HM_HaPeace_08"},"_actorName":"name_unpc_ellinara_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4260.0,"y":4725.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"23"},{"statement":{"_text":"You have done your service to Aurai. You have gone above and beyond what anyone could have asked of you. You are free to do with your remaining life what you wish. Live it well, and meet your ancestors with pride, [PlayerName].","_meta":"Elinara_HM_HaPeace_09"},"_actorName":"name_unpc_ellinara_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4260.0,"y":4845.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"25"},{"_position":{"x":4410.0,"y":4980.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"27"},{"statement":{"_text":"Doesn''t Elatt always tell you? Never do anything that you do not understand. Because now you live with the consequences of your rash action.","_meta":"Elinara_HM_HaPeace_07"},"_actorName":"name_unpc_ellinara_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4575.0,"y":4425.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"28"},{"statement":{"_text":"You''ve accomplished a lot in such a short time, Missionary. <color=#00CCFF><b>Cardinal Bourlamaque wanted to see you</b></color>. I think you''ll be working under him from now on. Congratulations on your promotion!","_meta":"Elinara_HM_TRUTH_06"},"_actorName":"name_unpc_ellinara_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4740.0,"y":3705.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"30"},{"_position":{"x":4875.0,"y":3840.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"32"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"30"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"7"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"7"},"_targetNode":{"$ref":"11"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"11"},"_targetNode":{"$ref":"13"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"13"},"_targetNode":{"$ref":"15"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"15"},"_targetNode":{"$ref":"21"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"15"},"_targetNode":{"$ref":"28"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"21"},"_targetNode":{"$ref":"23"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"23"},"_targetNode":{"$ref":"25"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"25"},"_targetNode":{"$ref":"27"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"28"},"_targetNode":{"$ref":"23"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"30"},"_targetNode":{"$ref":"32"},"$type":"NodeCanvas.DialogueTrees.DTConnection"}],"localBlackboard":{"_name":"Local Blackboard","_variables":{"gInstigator":{"_name":"gInstigator","_id":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b","$t
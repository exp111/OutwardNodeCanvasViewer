  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-4353.0,"y":-4258.0},"zoomFactor":0.9999441,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"1nGk1TyMbUi3VmSdn32zCg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4275.0,"y":3705.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"Those invading Scourge were tough. I haven\u2019t had a battle that intense in quite a while! I know my place is here, teaching the next generation of missionaries and templars, but Elatt\u2019s name do I miss the thrill of adventure.","_meta":"Galira_HM_HaPeace_04"},"_actorName":"name_unpc_galira_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4035.0,"y":3885.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"5"},{"_position":{"x":4125.0,"y":4035.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"7"},{"_condition":{"QuestEventRef":{"m_eventUID":"0go87L2r50eo1PqlTf3EUA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4560.0,"y":4020.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"8"},{"statement":{"_text":"I can\u2019t believe that Sagard Battleborn sent thugs to spill Levantin blood in our marsh. If I ever cross paths with him again, I will be sorely tempted to introduce him to my fist!","_meta":"Galira_HM_HaPeace_03"},"_actorName":"name_unpc_galira_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4305.0,"y":4155.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"11"},{"_position":{"x":4350.0,"y":4305.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"13"},{"statement":{"_text":"Something on your mind?","_meta":"Galira_HM_HaPeace_01"},"_actorName":"Galira","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4740.0,"y":4185.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"14"},{"
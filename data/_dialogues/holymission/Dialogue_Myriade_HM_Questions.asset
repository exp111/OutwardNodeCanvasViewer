  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-5089.0,"y":-6252.0},"nodes":[{"_condition":{"checkMode":"AnyTrueSuffice","conditions":[{"QuestEventRef":{"m_eventUID":"PzgvrExJE0SRSLhuFVOhMw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"z7P9PbwVYUy4WfXDr2cMug"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":6360.0,"y":6195.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_condition":{"QuestEventRef":{"m_eventUID":"qHj6iybWh0aEnzDGP-kp_w"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":6045.0,"y":6390.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"9"},{"_condition":{"QuestEventRef":{"m_eventUID":"S6usujloPU2h-FF6GY_dbA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":5625.0,"y":6555.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"12"},{"_condition":{"questRef":{"_value":{"m_itemID":7011101}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":5445.0,"y":6705.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"15"},{"statement":{"_text":"Thanks again for your help. We''re taking care to eat away at the Corruption regularly. It won''t be a problem again, I promise!","_meta":"Myriade_HM_Questions_41"},"_actorName":"name_unpc_ancienundead_01 ","_actorParameterID":"7cce4164-d690-4c14-ace8-a82fa4d868e8","_position":{"x":5085.0,"y":6900.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"19"},{"_position":{"x":5205.0,"y":7020.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"21"},{"statement":{"_text":"Hah, thank you. It was a joint effort! Eh? Ehhh?","_meta":"Myriade_HM_Questions_38"},"_actorName":"name_unpc_ancienundead_01 ","_actorParameterID":"7cce4164-d690-4c14-ace8-a82fa4d868e8","_position":{"x":5625.0,"y":6900.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"22"},{"availableChoices":[{"statement":{"_text":"Har har, you''re killing me, Myriade.","_meta":"Player_Myriade_HM_Questions_a_38"}},{"statement":{"_text":"Stop! One more bad joke and I''ll take you back apart again!","_meta":"Player_Myriade_HM_Questions_b_38"}}],"_position":{"x":5640.0,"y":7095.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"24"},{"statement":{"_text":"You\u2019re no fun. I''ll keep my fellow undead on a tight leash from now on. I won''t let them forget what they almost became.","_meta":"Myriade_HM_Questions_39"},"_actorName":"name_unpc_ancienundead_01 ","_actorParameterID":"7cce4164-d690-4c14-ace8-a82fa4d868e8","_position":{"x":5565.0,"y":7290.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"30"},{"statement":{"_text":"Anyone who goes to nibble on tasty Montcalm Tribe tribute treats are no longer welcome to bind their memories here. I''m sure that threat will keep most of them from going bad again. A place where memories can stick is hard to come by.","_meta":"Myriade_HM_Questions_40"},"_actorName":"name_unpc_ancienundead_01 ","_actorParameterID":"7cce4164-d690-4c14-ace8-a82fa4d868e8","_position":{"x":5550.0,"y":7425.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"32"},{"_position":{"x":5670.0,"y":7530.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"34"},{"statement":{"_text":"I. Ow. Mind. Memory. Can''t. Think.","_meta":"Myriade_HM_Questions_44"},"_actorName":"name_unpc_ancienundead_01 ","_actorParameterID":"7cce4164-d690-4c14-ace8-a82fa4d868e8","_position":{"x":5955.0,"y":6675.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"35"},{"statement":{"_text":"(The blast of Corruption from the altar seems to have stumned Myriade. You should give him a moment to recover.)","_meta":"Myriade_HM_Questions_45"},"_position":{"x":5970.0,"y":6810.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"37"},{"_position":{"x":6135.0,"y":6945.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"39"},{"statement":{"_text":"Things have gotten really bad while I was in pieces. The altar we\u2019ve bound our memories to is smothered by the Corruption. It\u2019s too much for me to eat away without losing my mind.","_meta":"Myriade_HM_Questions_33"},"_actorName":"name_unpc_ancienundead_01 ","_actorParameterID":"7cce4164-d690-4c14-ace8-a82fa4d868e8","_position":{"x":6375.0,"y":6570.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"40"},{"_condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"zfzikAeu4E2jVt618o5D0g"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"HbTd6ustoU-VhQeidJcAEw"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":6465.0,"y":6735.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"42"},{"statement":{"_text":"I really hope your lady friend there knows what she''s doing. Corruption this dense takes on a life of its own.","_meta":"Myriade_HM_Questions_34"},"_actorName":"name_unpc_ancienundead_01 ","_actorParameterID":"7cce4164-d690-4c14-ace8-a82fa4d868e8","_position":{"x":6360.0,"y":6930.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"49"},{"_position":{"x":6645.0,"y":7140.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"51"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"9"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"40"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"9"},"_targetNode":{"$ref":"12"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"9"},"_targetNode":{"$ref":"40"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"12"},"_targetNode":{"$ref":"15"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"12"},"_t
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-2210.0,"y":-2498.0},"zoomFactor":0.6033394,"nodes":[{"_condition":{"questRef":{"_value":{"m_itemID":7011103}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":2925.0,"y":2175.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_action":{"dialogueStarter":{"_name":"Dialogue_Oliele_BB/HMHallowPeacemaker","_targetVariableID":"82da3266-5cbc-4cc0-865f-f6e5b79eef40"},"$type":"NodeCanvas.Tasks.Actions.BranchDialogue"},"_position":{"x":2520.0,"y":2400.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"6"},{"_condition":{"QuestEventRef":{"m_eventUID":"RINZXKvXh0u3HebOm3C-sA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3285.0,"y":2355.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"9"},{"statement":{"_text":"The cardinal wanted to promote me to Templar for what we did. I told him to wait. I\u2019m no longer sure if I want to pursue the martial path or the philosopher\u2019s path.","_meta":"Oliele_HM_TRUTH_34"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3049.0,"y":2484.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"12"},{"statement":{"_text":"On one hand, I want to change the world with my words, on the other I want to destroy the Scourge with my own two hands if I can...It\u2019s a hard decision to make.","_meta":"Oliele_HM_TRUTH_35"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3000.0,"y":2625.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"14"},{"_position":{"x":3060.0,"y":2760.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"16"},{"_condition":{"QuestEventRef":{"m_eventUID":"FeqU9xQrKU-pO7KtsQ0VWw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3630.0,"y":2565.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"17"},{"_action":{"dialogueStarter":{"_name":"Dialogue_Oliele_BB/HMTruthElattSpeech","_targetVariableID":"6b32807b-dafb-4f5b-8ef8-6b1d6fdbeab6"},"$type":"NodeCanvas.Tasks.Actions.BranchDialogue"},"_position":{"x":3285.0,"y":2895.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"20"},{"_condition":{"QuestEventRef":{"m_eventUID":"hunWfUJOVkeNXBE7k2HBjw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4140.0,"y":3000.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"23"},{"_action":{"dialogueStarter":{"_name":"Dialogue_Oliele_BB/HMTruthTabletFound","_targetVariableID":"a1cf503d-24cf-44e7-bd9d-55fe35a41bed"},"$type":"NodeCanvas.Tasks.Actions.BranchDialogue"},"_position":{"x":3690.0,"y":3225.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"26"},{"_condition":{"QuestEventRef":{"m_eventUID":"sfAc3zxAp0KY2Y74qoNz7g"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4875.0,"y":3315.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"29"},{"_condition":{"QuestEventRef":{"m_eventUID":"L3pFbgJ6oU6u_GmcEQlaHA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4376.0,"y":3473.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"32"},{"statement":{"_text":"I think we''ve really hit it big, [PlayerName]. Something terrible happened here, I can sense it in the air. And I''ve found this tablet. It''s in the Old Language. I can''t read it. But right there...That''s Elatt''s name in the text. Dammit, if only we could read this thing.","_meta":"Oliele_HM_TRUTH_16"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3975.0,"y":3690.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"35"},{"availableChoices":[{"statement":{"_text":"Do we know anyone who could translate this?","_meta":"Player_Oliele_HM_TRUTH_a_16"}}],"_position":{"x":3975.0,"y":3840.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"37"},{"statement":{"_text":"Let''s take this <color=#00CCFF><b>back to Monsoon</b></color>. I''m positive someone can get it done there. I\u2019ll meet you back there.","_meta":"Oliele_HM_TRUTH_17"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3690.0,"y":4005.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"41"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":3975.0,"y":4110.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"43"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"hunWfUJOVkeNXBE7k2HBjw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"3_soGcNagk-KcYSeqpEgMg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"yh9NMBwPcUWbZRg6pChrEQ"},"RemoveAllStack":true,"$type":"NodeCanvas.Tasks.Actions.RemoveQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3930.0,"y":4185.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"45"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":3975.0,"y":4305.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"54"},{"_position":{"x":3975.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"56"},{"_condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"twkecOaC9ku7-tP8W6VzBA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"rURiUOhDtkCw7vsioxR23g"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":4693.0,"y":3625.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"57"},{"statement":{"_text":"This is interesting...Elatt actually has nothing to do with this Garden. There was a Lich that used to live here. His protective spells are what keep the Garden growing after the great river of Abrassar was diverted away.","_meta":"Oliele_HM_TRUTH_10"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4461.0,"y":3827.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"64"},{"statement":{"_text":"This Lich left records suggesting that he used to feed on a Ley Line that crossed under where the <color=#00CCFF><b>Stone Titan''s Cave</b></color> is now, but that it was destroyed around the time the Scourge appeared. That might be a good place to explore next.","_meta":"Oliele_HM_TRUTH_11"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4434.0,"y":4006.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"66"},{"availableChoices":[{"statement":{"_text":"I''m mighty curious about the Stone Titan''s Cave.","_meta":"Player_Oliele_HM_TRUTH_c_06"}},{"statement":{"_text":"Old Levant sounds like the biggest lead.","_meta":"Player_Oliele_HM_TRUTH_b_06"},"condition":{"QuestEventRef":{"m_eventUID":"gvEeJpR5GEWuvVP0Fcl27g"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"Let\u2019s regroup in Levant, I need supplies.","_meta":"Player_Oliele_HM_TRUTH_d_06"},"condition":{"QuestEventRef":{"m_eventUID":"gvEeJpR5GEWuvVP0Fcl27g"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":4470.0,"y":4260.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"68"},{"statement":{"_text":"Yes, me too. It''s not too far from Old Levant. When you <color=#00CCFF><b>start seeing enormous stone statues, you''ll know you''re in the right place</b></color>. Well, I''m off.","_meta":"Oliele_HM_TRUTH_09"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3887.0,"y":4515.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"80"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":4032.0,"y":4615.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"82"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"rURiUOhDtkCw7vsioxR23g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"L3pFbgJ6oU6u_GmcEQlaHA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3984.0,"y":4766.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"84"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":4076.0,"y":4906.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"91"},{"_position":{"x":4320.0,"y":5070.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"93"},{"statement":{"_text":"I agree. <color=#00CCFF><b>It''s right next to Abrassar''s Vigil</b></color>, so it shouldn''t be too hard to find. See you there.","_meta":"Oliele_HM_TRUTH_08"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4356.0,"y":4517.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"94"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":4596.0,"y":4637.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"96"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"rURiUOhDtkCw7vsioxR23g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"0XFBolA7PEK7VQvem6tZ4Q"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4550.0,"y":4757.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"98"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":4596.0,"y":4922.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"105"},{"statement":{"_text":"I had no idea Sorobor was excavating these ruins. Although there\u2019s plenty of evidence there were bandits camping here until very recently...Either way, I found some historical information here.","_meta":"Oliele_HM_TRUTH_12"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4995.0,"y":4125.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"107"},{"statement":{"_text":"These records show that there used to be a Ley Line under where the <color=#00CCFF><b>Stone Titan\u2019s cave</b></color> is now. The day that the Scourge happened, mages from Old Levant felt the Ley Line swell and then be destroyed...","_meta":"Oliele_HM_TRUTH_14"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4995.0,"y":4290.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"109"},{"_targetNode":{"statement":{"_text":"The coincidences just keep piling up around that cave. For an entire Ley Line to vanish...Something powerful must have happened there. It might be a good place to go examine next.","_meta":"Oliele_HM_TRUTH_15"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4560.0,"y":5220.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"112"},"_position":{"x":5115.0,"y":4575.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"111"},{"_condition":{"QuestEventRef":{"m_eventUID":"LyFRnbl_1EKPfQXF3ORocA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":5647.0,"y":3395.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"114"},{"_condition":{"QuestEventRef":{"m_eventUID":"pnO35kD3006EmJ_fq8yYUQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":5558.0,"y":3533.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"117"},{"_condition":{"checkMode":"AnyTrueSuffice","conditions":[{"QuestEventRef":{"m_eventUID":"rURiUOhDtkCw7vsioxR23g"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"gvEeJpR5GEWuvVP0Fcl27g"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":5550.0,"y":3810.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"120"},{"statement":{"_text":"Where should we check next?","_meta":"Oliele_HM_TRUTH_06-b"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5445.0,"y":4080.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"127"},{"_targetNode":{"availableChoices":[{"statement":{"_text":"Old Levant sounds like the biggest lead.","_meta":"Player_Oliele_HM_TRUTH_b_06"},"condition":{"QuestEventRef":{"m_eventUID":"gvEeJpR5GEWuvVP0Fcl27g"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"I''m mighty curious about the Stone Titan''s Cave.","_meta":"Player_Oliele_HM_TRUTH_c_06"},"condition":{"QuestEventRef":{"m_eventUID":"hunWfUJOVkeNXBE7k2HBjw"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"Let''s check the garden.","_meta":"Player_Oliele_HM_TRUTH_a_06"},"condition":{"QuestEventRef":{"m_eventUID":"rURiUOhDtkCw7vsioxR23g"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":5910.0,"y":5010.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"130"},"_position":{"x":5445.0,"y":4245.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"129"},{"statement":{"_text":"Elatt sent you, didn''t he, [PlayerName]? To keep an eye on me?","_meta":"Oliele_HM_TRUTH_02"},"_actorName":"Oliele","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5745.0,"y":4110.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"144"},{"availableChoices":[{"statement":{"_text":"I''m here to help you with your search. Ellinara said you were close to discovering something.","_meta":"Player_Oliele_HM_TRUTH_a_02"}},{"statement":{"_text":"Don''t think I''m happy about this long trip either, Oliele. How goes the search?","_meta":"Player_Oliele_HM_TRUTH_b_02"}}],"_position":{"x":5865.0,"y":4245.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"146"},{"statement":{"_text":"I''ve managed to dig up three leads on Elatt''s origins. An Ancient \"Garden\", the Old Levant Ruins, and the Stone Titan''s cave. Any of them might have what we''re looking for.","_meta":"Oliele_HM_TRUTH_03"},"_actorName":"Oliele","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5790.0,"y":4440.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"152"},{"statement":{"_text":"The Ancient \"Garden\" is within sight of Levant itself. It''s the only place where anything still grows out here. If it''s the only place the devastation didn''t touch...There must be a reason.","_meta":"Oliele_HM_TRUTH_04"},"_actorName":"Oliele","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5865.0,"y":4590.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"154"},{"statement":{"_text":"Secondly, there are the ruins of Old Levant, where Elatt was born. They may still have records about his mortal life there. Abrassar''s Vigil was built above it, so there''s an easy landmark for us to use to get there.","_meta":"Oliele_HM_TRUTH_05"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5850.0,"y":4710.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"156"},{"statement":{"_text":"Finally, there''s the Stone Titan''s Cave. The giant stone statues that the cave is named for only appeared around the time the Scourge happened. It''s too strange to be a coincidence.","_meta":"Oliele_HM_TRUTH_06"},"_actorName":"Oliele","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5850.0,"y":4845.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"158"},{"$ref":"130"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"sfAc3zxAp0KY2Y74qoNz7g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"0XFBolA7PEK7VQvem6tZ4Q"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":5415.0,"y":5250.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"160"},{"statement":{"_text":"I agree. <color=#00CCFF><b>It''s right next to Abrassar''s Vigil</b></color>, so it shouldn''t be too hard to find. See you there.","_meta":"Oliele_HM_TRUTH_08"},"_actorName":"name_unpc_oliele_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5205.0,"y":5400.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"167"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":5520.0,"y":5520.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"169"},{"_action":{"QuestEventRef":{"m_eventUID":"yh9NMBwPcUWbZRg6pChrEQ"},"RemoveAllStack":true,"$type":"NodeCanvas.Tasks.Actions.RemoveQuestEvent"},"_position":{"x":5415.0,"y":5595.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"171"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":5505.0,"y":5685.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"174"},{"_position":{"x":5505.0,"y":5790.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"176"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"sfAc3zxAp0KY2Y74qoNz7g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"L3pFbgJ6oU6u_GmcEQlaHA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":5880.0,"y":5280.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"177"},{"statement":{"_text":"Yes, me too. It''s not too far from Old Levant. When you <color=#00CCFF><b>start seeing enormous stone statues, you''ll know you''re in the right place</b></color>. Well, I''m off.","_meta":"Oliele_HM_TRUTH_09"},"_act
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-4061.999,"y":-3195.999},"zoomFactor":0.8382685,"nodes":[{"_condition":{"questRef":{"_value":{"m_itemID":7011104}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":3600.0,"y":2850.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"I don\u2019t believe this. I can\u2019t believe this. We let Levant gain a goddess? How could Rissa let that happen?! This will be our doom, mark my words. Mark. My. Words.","_meta":"Sagard_BC_ANC_PEACE_15"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3495.0,"y":3165.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"6"},{"_position":{"x":3630.0,"y":3315.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"8"},{"_condition":{"QuestEventRef":{"m_eventUID":"grSVcJ1mFkOqghOAleUm7g"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3780.0,"y":3000.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"9"},{"_condition":{"QuestEventRef":{"m_eventUID":"iBABfYLhb06SBB0k-_2m1A"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3945.0,"y":3255.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"12"},{"statement":{"_text":"You already have my word that I\u2019ll pull back my armies. The message is already being sent to my warriors.","_meta":"Sagard_HM_HaPeace_04"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3495.0,"y":3450.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"15"},{"_targetNode":{"availableChoices":[{"statement":{"_text":"Farewell.","_meta":"Merchant_Generic_Farewell_01"}},{"statement":{"_text":"I want to train with you.","_meta":"Merchant_Generic_Train_01"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"skill":{"_value":1},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_KnowSkill"}}],"_position":{"x":2490.0,"y":3150.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"18"},"_position":{"x":3645.0,"y":3630.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"17"},{"_condition":{"QuestEventRef":{"m_eventUID":"p6LZhC-ufkWrMRxVm6Dazw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4035.0,"y":3495.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"27"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"iBABfYLhb06SBB0k-_2m1A"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"agNMO8se3U6-flO9HiI4hg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3900.0,"y":3675.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"30"},{"statement":{"_text":"I don''t believe it. How in Elatt''s name did you beat me?","_meta":"Sagard_BC_ANC_PEACE_11"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3735.0,"y":3810.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"39"},{"statement":{"_text":"Bah, it\u2019s not fair when Elatt gives you his strength on top of your own! Fine! You win. I\u2019ll call off my armies. I\u2019m smart enough to know when I\u2019m whupped.","_meta":"Sagard_HM_HaPeace_05"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3660.0,"y":3945.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"41"},{"_targetNode":{"_condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"SwFRKikVwU-R97E9VBFPnQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"06JSjPGJ6Eu-cGNAIcP-qw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"iBABfYLhb06SBB0k-_2m1A"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":4245.0,"y":6480.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"44"},"_position":{"x":3735.0,"y":4110.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"43"},{"_condition":{"QuestEventRef":{"m_eventUID":"0go87L2r50eo1PqlTf3EUA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4380.0,"y":3780.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"53"},{"_condition":{"QuestEventRef":{"m_eventUID":"rl1_bi9s4kyV5bV9UeW7eg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4170.0,"y":4005.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"56"},{"statement":{"_text":"Well well, [PlayerName] the Champion of Elatt. This is a surprise meeting. Here to finally pledge your aid in slaughtering those desert rats?","_meta":"Sagard_HM_HaPeace_01"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4005.0,"y":4185.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"59"},{"availableChoices":[{"statement":{"_text":"It''s time we ended this war. Elatt demands peace.","_meta":"Player_CaughtSpy_HM_HaPeace_a_01"}}],"_position":{"x":4395.0,"y":4410.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"61"},{"statement":{"_text":"Well, that''s too bad for him. We aren''t going to stop fighting a war that we''re finally winning. No force on Aurai will make me change my mind.","_meta":"Sagard_HM_HaPeace_03"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4380.0,"y":4620.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"65"},{"availableChoices":[{"isUnfolded":false,"statement":{"_text":"I''ll find a way to change your mind, believe me.","_meta":"Player_Sagard_BC_ANC_PEACE_a_01"}},{"statement":{"_text":"You owe me a favor, Sagard. I''m here to collect.","_meta":"Player_Sagard_BC_ANC_PEACE_b_01"},"condition":{"QuestEventRef":{"m_eventUID":"jm1rWjLBL0WcOYhEgGpyug"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"(Hold out a shimmering Tsar Stone)","_meta":"Player_Sagard_BC_ANC_PEACE_c_01"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":6200010}},"minAmount":{"_value":1},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"Call off your armies, or I''ll call them off for you.","_meta":"Player_Sagard_BC_ANC_PEACE_d_01"}}],"_position":{"x":4395.0,"y":4815.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"67"},{"statement":{"_text":"Hah, I doubt that very much. What could possibly make me change my stance on the war, [PlayerName]?","_meta":"Sagard_BC_ANC_PEACE_02"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3870.0,"y":4965.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"85"},{"_position":{"x":3930.0,"y":5100.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"87"},{"statement":{"_text":"What? You think one measly personal favor is worth ending a war? Have you lost your senses, pup?","_meta":"Sagard_BC_ANC_PEACE_03"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4080.0,"y":5145.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"88"},{"availableChoices":[{"statement":{"_text":"Do you really want everyone in the Collective to know that you don''t honor your word, Sagard?","_meta":"Player_Sagard_BC_ANC_PEACE_a_03"}}],"_position":{"x":4155.0,"y":5280.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"90"},{"statement":{"_text":"You\u2019d attack me with words and not steel? Fine. My reputation means too much to me to stain it over this. I\u2019ll call off my soldiers. But when Levant attacks again, everyone will remember that I was right to fear them!","_meta":"Sagard_BC_ANC_PEACE_04"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3945.0,"y":5415.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"94"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"iBABfYLhb06SBB0k-_2m1A"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"agNMO8se3U6-flO9HiI4hg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3900.0,"y":5610.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"96"},{"$ref":"44"},{"statement":{"_text":"(A rumble shakes you from head to toe, and Elatt''s voice comes like a gust of hot wind)","_meta":"Elatt_HM_HaPeace_04"},"_actorName":"name_unpc_narrator","_actorParameterID":"f9fa549f-e81c-4e58-a950-709d350fce4e","_position":{"x":4050.0,"y":6690.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"105"},{"statement":{"_text":"[PlayerName], things go poorly in Abrassar. My messengers have been imprisoned. Civil war strikes the city of Levant. <color=#00CCFF><b>You are needed there, desperately</b></color>.","_meta":"Elatt_HM_HaPeace_05"},"_actorName":"name_unpc_elatt_01 ","_actorParameterID":"20ccb8ef-54a6-4340-a1dd-c06dc2b8e23b","_position":{"x":4065.0,"y":6825.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"107"},{"availableChoices":[{"statement":{"_text":"Isn''t Soeran closer than I am?","_meta":"Player_Elatt_HM_HaPeace_a_05"}}],"_position":{"x":4215.0,"y":6945.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"109"},{"statement":{"_text":"No, he is not. He is standing between both armies, trying to stop any further combat between either side. He cannot make the journey to Abrassar. This task is up to you.","_meta":"Elatt_HM_HaPeace_06"},"_actorName":"name_unpc_elatt_01 ","_actorParameterID":"20ccb8ef-54a6-4340-a1dd-c06dc2b8e23b","_position":{"x":4170.0,"y":7050.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"113"},{"_position":{"x":4755.0,"y":7260.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"115"},{"statement":{"_text":"Is that...That''s a Tsar Stone! With just two more of those, I could make armor that could deflect even a Manticore\u2019s strikes! Don''t you dare tempt me, pup!","_meta":"Sagard_BC_ANC_PEACE_05"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4665.0,"y":5580.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"116"},{"availableChoices":[{"statement":{"_text":"It''s yours if you call off your armies.","_meta":"Player_Sagard_BC_ANC_PEACE_a_05"}},{"statement":{"_text":"I just wanted to see your reaction.","_meta":"Player_Sagard_BC_ANC_PEACE_b_05"}}],"_position":{"x":4785.0,"y":5760.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"118"},{"statement":{"_text":"Damn you, [PlayerName]. A gem like this is worth any price. I can''t possibly turn you down. Fine. I''ll call off my soldiers. You win. Now hand it over!","_meta":"Sagard_BC_ANC_PEACE_06"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4500.0,"y":5940.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"124"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"iBABfYLhb06SBB0k-_2m1A"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":6200010}}],"Amount":[{"_value":1}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"},{"QuestEventRef":{"m_eventUID":"agNMO8se3U6-flO9HiI4hg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4590.0,"y":6165.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"126"},{"statement":{"_text":"Well,you\u2019ve seen it. Satisfied? You know how priceless that gem is, don''t you?","_meta":"Sagard_BC_ANC_PEACE_07"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4995.0,"y":6030.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"142"},{"_position":{"x":5205.0,"y":6180.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"144"},{"statement":{"_text":"Is that a threat? Do you know what you''re starting, you little weasel? Think again.","_meta":"Sagard_BC_ANC_PEACE_08"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4965.0,"y":5100.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"145"},{"availableChoices":[{"statement":{"_text":"On second thought, never mind.","_meta":"Player_Sagard_BC_ANC_PEACE_a_08"}},{"statement":{"_text":"Oh, is the great Sagard Battleborn too pathetic to fight one puny little scout?","_meta":"Player_Sagard_BC_ANC_PEACE_b_08"}}],"_position":{"x":5250.0,"y":5280.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"147"},{"statement":{"_text":"Smart choice, pup.","_meta":"Sagard_BC_ANC_PEACE_09"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5145.0,"y":5475.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"153"},{"_position":{"x":5145.0,"y":5595.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"155"},{"statement":{"_text":"You\u2019ve just made a terrible mistake. I\u2019ll enjoy breaking every bone in your body, weasel.","_meta":"Sagard_BC_ANC_PEACE_10"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5445.0,"y":5460.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"156"},{"_action":{"QuestEventRef":{"m_eventUID":"_ZoYiXnjckmCoSPEf3fnqA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":5490.0,"y":5610.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"158"},{"_position":{"x":5550.0,"y":5745.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"161"},{"statement":{"_text":"Well, Elatt''s newest Templar. What do you want [PlayerName]? Here to finally pledge your aid in slaughtering those desert rats?","_meta":"Sagard_HM_HaPeace_02"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4440.0,"y":4170.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"162"},{"_condition":{"QuestEventRef":{"m_eventUID":"Og71f8G5a0eVmLxZB0yOKg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_color":{"r":0.7019608,"g":1.0,"b":0.4980392,"a":1.0},"_position":{"x":5255.0,"y":3982.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"164"},{"_condition":{"QuestEventRef":{"m_eventUID":"rl1_bi9s4kyV5bV9UeW7eg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4943.0,"y":4224.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"167"},{"statement":{"_text":"Well well, [PlayerName] the Champion of Elatt. This is a surprise meeting. Here to finally pledge your aid in slaughtering those desert rats?","_meta":"Sagard_HM_HaPeace_01"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4920.0,"y":4373.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"170"},{"statement":{"_text":"No? Fine. Then leave me alone. I have plans for Levant\u2019s destruction to create!","_meta":"Sagard_HM_HaPeace_06"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5356.0,"y":4612.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"172"},{"_targetNode":{"$ref":"18"},"_position":{"x":5995.0,"y":4967.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"174"},{"statement":{"_text":"Well, Elatt''s newest Templar. What do you want [PlayerName]? Here to finally pledge your aid in slaughtering those desert rats?","_meta":"Sagard_HM_HaPeace_02"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5350.0,"y":4382.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"175"},{"_condition":{"checkMode":"AnyTrueSuffice","conditions":[{"QuestEventRef":{"m_eventUID":"Wl08NWMJokemPVfTEyT3UA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"pBUpwOif4kmiMl6ViWv2hw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_color":{"r":0.7019608,"g":1.0,"b":0.4980392,"a":1.0},"_position":{"x":6048.0,"y":4162.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"177"},{"statement":{"_text":"Hah! I knew it! Levant has finally made their last mistake!","_meta":"Sagard_BC_Mix_Legacy_03"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5864.0,"y":4367.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"184"},{"statement":{"_text":"Now is not the time to debate the obvious. We gather our troops and strike NOW, before they strike us again! They likely have their whole army on the road for Berg as we speak!","_meta":"Sagard_BC_Mix_Legacy_05"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5878.0,"y":4531.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"186"},{"statement":{"_text":"Move along. I have combat strategies to prepare for when those desert rats in Levant inevitably come after us. It\u2019s only a matter of time before they get too big for their bloomers and try to take what isn\u2019t theirs.","_meta":"Sagard_BC_Prequest_06"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6352.0,"y":4414.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"188"},{"statement":{"_text":"You look troubled, is there something wrong?"},"_actorName":"Sagard Battleborn","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6885.0,"y":4725.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"190"},{"availableChoices":[{"statement":{"_text":"No, I am fine thank you for caring."}},{"statement":{"_text":"I am here in the name of Elatt to ask for the peace with Levant."}},{"statement":{"_text":"..."}}],"_position":{"x":7215.0,"y":4935.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"192"},{"statement":{"_text":"Okay, have a nice day [PlayerName]!"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6855.0,"y":5115.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"200"},{"_position":{"x":6915.0,"y":5235.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"202"},{"statement":{"_text":"And what I am gaining by doing that?"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":7290.0,"y":5115.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"203"},{"availableChoices":[{"statement":{"_text":"Nothing I guess."}},{"statement":{"_text":"What about 100 silvers?"}},{"statement":{"_text":"I get a tribal favor for you."},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{},"
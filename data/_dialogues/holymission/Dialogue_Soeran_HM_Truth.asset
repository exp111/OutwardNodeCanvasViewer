  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-2229.0,"y":-2826.0},"zoomFactor":0.9773415,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"vrIRrZ10ZkOAydreo1qFCQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2910.0,"y":2640.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_condition":{"QuestEventRef":{"m_eventUID":"LgmYZpa1AkWBYcmiTkYv0w"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2475.0,"y":2820.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"5"},{"statement":{"_text":"And so we had to resort to violence...May they find their ancestors and rest in peace...","_meta":"Sorean_HM_TRUTH_02"},"_actorName":"Soeran","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2280.0,"y":2940.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"8"},{"_position":{"x":2400.0,"y":3075.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"10"},{"_condition":{"QuestEventRef":{"m_eventUID":"2bl8GI1hbUG7zGr_PTVwsw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":2745.0,"y":2970.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"11"},{"statement":{"_text":"I\u2019m glad we could avoid bloodshed. It\u2019s a shame that they felt they had to leave, but such is their choice.","_meta":"Sorean_HM_TRUTH_04"},"_actorName":"Soeran","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2595.0,"y":3135.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"14"},{"_position":{"x":2670.0,"y":3285.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"16"},{"statement":{"_text":"You have a silver tongue, [PlayerName]. I\u2019m impressed that you managed to make them consider reason again. We could use someone like you to fill the gap that Zephyrien left behind!","_meta":"Sorean_HM_TRUTH_03"},"_actorName":"Soeran","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3105.0,"y":3150.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"17"},{"_position":{"x":3225.0,"y":3300.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"19"},{"statement":{"_text":"They''re too far gone for me to reason with...I''m worried they might become violent. We''ve been discussing their unfounded beliefs for hours now. If you can convince them, please, do it. I don''t want bloodshed here today.","_meta":"Sorean_HM_TRUTH_01"},"_actorName":"Soeran","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3225.0,"y":2850.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"20"},{"_position":{"x":3435.0,"y":2970.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"22"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"20"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"8"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"11"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"8"},"_targetNode":{"$ref":"10"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"11"},"_targetNode":{"$ref":"14"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"11"},"_targetNode":{"$ref":"17"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"14"},"_targetNode":{"$ref":"16"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"17"},"_targetNode":{"$ref":"19"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"20"},"_targetNode":{"$ref":"22"},"$type":"NodeCanvas.DialogueTrees.DTConnection"}],"localBlackboard":{"_name":"Local Blackboard","_variables":{"gInstigator":{"_name":"gInstigator","_id":"0483cf6b-da62-4a57-aaed-40f
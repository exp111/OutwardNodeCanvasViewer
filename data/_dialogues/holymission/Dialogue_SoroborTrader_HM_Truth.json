{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-2630.0,"y":-3323.0},"zoomFactor":0.9944789,"nodes":[{"statement":{"_text":"Hey there, looking for something specific?","_meta":"Sorobor_HM_TRUTH_01"},"_actorName":"SoroborTrader","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2985.0,"y":2700.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"2"},{"availableChoices":[{"statement":{"_text":"Do you have anything that might let us translate the Old Language?","_meta":"Player_Sorobor_HM_TRUTH_a_01"}}],"_position":{"x":3030.0,"y":2835.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"4"},{"statement":{"_text":"Old Language, eh? Actually, I think I do. An explorer recently turned in an ancient peace treaty that was made about 80 years ago. It''s written in the Old Language on one side and a rough form of the New Language on the other.","_meta":"Sorobor_HM_TRUTH_02"},"_actorName":"SoroborTrader","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2970.0,"y":2985.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"8"},{"statement":{"_text":"I''ve got a parchment copy of it right here, actually. You can use this to translate from one language to the other. It''s yours for 150 Silver.","_meta":"Sorobor_HM_TRUTH_03"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2955.0,"y":3105.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"10"},{"availableChoices":[{"statement":{"_text":"Done. Give it here.","_meta":"Player_Sorobor_HM_TRUTH_a_03"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":150},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"That price is too high.","_meta":"Player_Sorobor_HM_TRUTH_b_03"}}],"_position":{"x":3030.0,"y":3240.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"12"},{"statement":{"_text":"Deal! <color=#00CCFF><b>Good luck with your translation job</b></color>!","_meta":"Sorobor_HM_TRUTH_04"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2670.0,"y":3420.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"24"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"HDoJDxyvMUC4dsWAgvSUsA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":150}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":2865.0,"y":3570.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"26"},{"_position":{"x":2970.0,"y":3705.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"38"},{"statement":{"_text":"Well, that''s the price that the Headmaster decided, and he said not to adjust it. Sorry, but I can''t give you a better deal than that.","_meta":"Sorobor_HM_TRUTH_05"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3345.0,"y":3420.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"39"},{"_position":{"x":3480.0,"y":3540.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"41"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"4"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"4"},"_targetNode":{"$ref":"8"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"8"},"_targetNode":{"$ref":"10"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"10"},"_targetNode":{"$ref":"12"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"12"},"_targetNode":{"$ref":"24"},"
  m_Name: Dialogue_Merchant_CaravanTrader
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-4849.0,"y":-3544.0},"zoomFactor":0.7500094,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"sm812Cio9ki5ssbsiPr3Fw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":5745.0,"y":4710.0},"_UID":"1030efc0-b3ea-45b0-9043-c1606ca1ac76","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"Cierzo is probably my favourite town in Aurai. It\u2019s small and peaceful, but still bustling with activity! If there\u2019s anything you need, come to me! A Soroborean always has the best deals!","_meta":"Merchant_Cierzo_CaravanTrader_01"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":5715.0,"y":5190.0},"_UID":"462994ee-546d-4cc9-bc43-a2842d61b387","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"5"},{"availableChoices":[{"statement":{"_text":"I want to buy something.","_meta":"Merchant_Generic_Shop_01"}},{"statement":{"_text":"I would like to travel.","_meta":"Travel_SO_OpenMenu_01_Player"},"condition":{"DLC":{"_value":"Soroboreans"},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsDLC"}},{"statement":{"_text":"I\u2019d like to talk.","_meta":"Merchant_Generic_Talk_01"}},{"statement":{"_text":"Do you have anything that might let us translate the Old Language?","_meta":"Player_Sorobor_HM_TRUTH_a_01"},"condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"hunWfUJOVkeNXBE7k2HBjw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"HDoJDxyvMUC4dsWAgvSUsA"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"}},{"statement":{"_text":"What\u2019s it like, working for Sorobor Academy?","_meta":"Player_Caravaneer_SO_Prequest_00_a"},"condition":{"conditions":[{"DLC":{"_value":"Soroboreans"},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsDLC"},{"QuestEventRef":{"m_eventUID":"bjVloYMQxk6KXx0gph2A1Q"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"quest":{"_value":1},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_KnowQuest"}],"$type":"NodeCanvas.Framework.ConditionList"}},{"statement":{"_text":"Farewell.","_meta":"Merchant_Generic_Farewell_01"}}],"_position":{"x":6810.0,"y":5670.0},"_UID":"953f68e9-52ce-4fff-9b69-e40b6c98bf16","_tag":"Choices","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"7"},{"_action":{"Merchant":{"_name":"myMerchant","_targetVariableID":"af622068-8aa0-4642-a896-364cdfa42d1b"},"PlayerCharacter":{"_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"$type":"NodeCanvas.Tasks.Actions.ShopDialogueAction"},"_position":{"x":6390.0,"y":5850.0},"_UID":"5468d223-6158-402a-b2a8-8dd1dc31d30f","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"37"},{"_subTree":{"_value":2},"actorParametersMap":{"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95":""},"variablesMap":{"59b1bec3-5814-434a-901e-51ab31076630":{"_type":"Character","_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"af622068-8aa0-4642-a896-364cdfa42d1b":{"_type":"Merchant","_name":"myMerchant","_targetVariableID":"af622068-8aa0-4642-a896-364cdfa42d1b"},"fc7761d3-81f2-4180-a9ac-2b874b9d48e3":{"_type":"MerchantFastTravel","_name":"merchantTravel","_targetVariableID":"0c79e361-b715-42f8-915d-4f9caed98f93"}},"_position":{"x":6491.0,"y":5953.0},"_UID":"cc66d0d6-4b51-4f15-8deb-d9de649445eb","$type":"NodeCanvas.DialogueTrees.SubDialogueTree","$id":"41"},{"_position":{"x":6431.0,"y":6073.0},"_UID":"670c6b0c-d5a8-4421-b74a-c457651f906c","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"48"},{"_sourceNodeUID":"953f68e9-52ce-4fff-9b69-e40b6c98bf16","_position":{"x":6580.0,"y":6110.0},"_UID":"6c91e6d5-2a27-43e2-82a0-513a61061792","$type":"NodeCanvas.DialogueTrees.Jumper","$id":"49"},{"statement":{"_text":"What\u2019s on your mind?","_meta":"SoroborMerchant_HK_Corsairs_01"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6855.0,"y":6000.0},"_UID":"16b4a8ea-1326-4ade-ae1e-315468d3ef27","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"50"},{"availableChoices":[{"statement":{"_text":"Nothing in particular","_meta":"Player_SoroborMerchant_HK_Corsairs_b_01"}},{"statement":{"_text":"I need you to stop extorting the people of the slums for food.","_meta":"Player_SoroborMerchant_HK_Corsairs_a_01"},"condition":{"conditions":[{"character":{"_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":100},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"},{"QuestEventRef":{"m_eventUID":"b7cx--7vmE2UyW6IaeELXQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"jgnv5LrPnUOdwwU0bnfKMg"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"questRef":{"_value":{"m_itemID":7011302}},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"}],"$type":"NodeCanvas.Framework.ConditionList"}},{"statement":{"_text":"I need you to stop extorting the people of the slums for food.","_meta":"Player_SoroborMerchant_HK_Corsairs_a_01"},"condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"_70yblm4jUSHjKDHqfBgZQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"b7cx--7vmE2UyW6IaeELXQ"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"questRef":{"_value":{"m_itemID":7011302}},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"}],"$type":"NodeCanvas.Framework.ConditionList"}},{"statement":{"_text":"I need to talk to you about ending the famine in Levant.","_meta":"Player_Soroborean_MouthFeed_c_16"},"condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"wHsc6UNL2E-sjOl1cUnh9Q"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"questRef":{"_value":{"m_itemID":7011303}},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},{"QuestEventRef":{"m_eventUID":"fJSdc0sL90GdXz39AHFl5A"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"}}],"_position":{"x":6780.0,"y":6120.0},"_UID":"44b81665-aba3-4067-a89b-127b30bab47a","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"52"},{"statement":{"_text":"Then stay safe. That\u2019s an order! I don\u2019t like losing my customers!","_meta":"SoroborMerchant_HK_Corsairs_07"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6060.0,"y":6330.0},"_UID":"1ae08830-7431-4319-920e-cefe1643fae9","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"95"},{"_position":{"x":6135.0,"y":6465.0},"_UID":"2de1b88e-af47-4fd9-b400-2cd1f4bea565","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"97"},{"statement":{"_text":"Extort? That\u2019s a nasty word. It\u2019s simple supply and demand, my good customer. It costs a lot of money to bring food through a desert this large before it goes bad!","_meta":"SoroborMerchant_HK_Corsairs_02"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6345.0,"y":6540.0},"_UID":"42842343-40e8-4b41-a143-0c4a495ae785","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"98"},{"availableChoices":[{"statement":{"_text":"I\u2019m sure we can find an arrangement that works for everyone. That\u2019s what Soroboreans do, right?","_meta":"Player_SoroborMerchant_HK_Corsairs_a_02"}},{"statement":{"_text":"The Sand Corsairs hit every caravan except Soroborean ones. Your prices shoot up in response. Don''t you see how that looks suspicious?","_meta":"Player_SoroborMerchant_HK_Corsairs_b_02"}}],"_position":{"x":6960.0,"y":6750.0},"_UID":"d40cc014-de37-4a04-87bf-f489fef2f5e5","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"100"},{"statement":{"_text":"Alright, If you''ll eat half my costs for the next trip here, I''ll halve my prices for the people of the slums. Let''s say 200 silver upfront.","_meta":"SoroborMerchant_HK_Corsairs_03"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6615.0,"y":6945.0},"_UID":"8a6c16cb-3c57-4408-827e-f48023082953","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"106"},{"availableChoices":[{"statement":{"_text":"No deal.","_meta":"Player_SoroborMerchant_HK_Corsairs_b_04"}},{"statement":{"_text":"Deal. Feed these people.","_meta":"Player_SoroborMerchant_HK_Corsairs_a_04"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":200},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}}],"_position":{"x":6645.0,"y":7065.0},"_UID":"9b980aa7-eeba-46dc-aa7b-9d5e481862a9","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"108"},{"statement":{"_text":"No deal then. I\u2019m sorry neither of us can\u2019t afford to help them.","_meta":"SoroborMerchant_HK_Corsairs_06"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6435.0,"y":7290.0},"_UID":"7364b844-b30f-40fa-a741-80e2d47a4dd2","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"120"},{"_action":{"QuestEventRef":{"m_eventUID":"b7cx--7vmE2UyW6IaeELXQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":6510.0,"y":7410.0},"_UID":"f589c6f6-9597-4960-88b1-96bccc5eba6a","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"122"},{"_position":{"x":6570.0,"y":7545.0},"_UID":"5515ac30-5f17-4f87-9a03-5a85418aa22e","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"125"},{"statement":{"_text":"Done. I\u2019ll start handing out rations right away.","_meta":"SoroborMerchant_HK_Corsairs_05"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6855.0,"y":7275.0},"_UID":"15e1d415-104c-41ea-9132-71d5bd2c7757","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"126"},{"_action":{"actions":[{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":200}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"},{"QuestEventRef":{"m_eventUID":"b7cx--7vmE2UyW6IaeELXQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"7EoX3egRH0CnHzY2MVhfCw"},"StackAmount":3,"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"jgnv5LrPnUOdwwU0bnfKMg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":6840.0,"y":7395.0},"_UID":"018d506c-366e-4e95-b098-bc71c0a71e76","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"128"},{"_position":{"x":6960.0,"y":7545.0},"_UID":"09842920-e521-49a6-9970-fe5a5013039a","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"144"},{"statement":{"_text":"How dare you! ...Still, you have a point, the coincidence does make us look bad. Hmm...I can give you a discount. 100 Silvers upfront and I''ll lower my prices for the people of the slums.","_meta":"SoroborMerchant_HK_Corsairs_04"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7185.0,"y":6945.0},"_UID":"23c0389e-ac97-442b-9e32-1402c0007187","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"145"},{"availableChoices":[{"statement":{"_text":"No deal.","_meta":"Player_SoroborMerchant_HK_Corsairs_b_04"}},{"statement":{"_text":"Deal. Feed these people.","_meta":"Player_SoroborMerchant_HK_Corsairs_a_04"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":100},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}}],"_position":{"x":7425.0,"y":7110.0},"_UID":"65b9abae-8089-42f4-b0c1-04288760719c","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"147"},{"statement":{"_text":"No deal then. I\u2019m sorry neither of us can\u2019t afford to help them.","_meta":"SoroborMerchant_HK_Corsairs_06"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7185.0,"y":7305.0},"_UID":"4467da68-791c-4776-a63c-1947bae2a394","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"159"},{"_action":{"QuestEventRef":{"m_eventUID":"b7cx--7vmE2UyW6IaeELXQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":7260.0,"y":7425.0},"_UID":"21092543-7ee7-4aa2-8708-449f14c9151c","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"161"},{"_position":{"x":7305.0,"y":7560.0},"_UID":"90a1424a-5735-49d4-8f09-48d6175715bf","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"164"},{"statement":{"_text":"Done. I\u2019ll start handing out rations right away.","_meta":"SoroborMerchant_HK_Corsairs_05"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7590.0,"y":7290.0},"_UID":"bf9657ce-c6aa-4516-916b-1a270e120784","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"165"},{"_action":{"actions":[{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":100}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"},{"QuestEventRef":{"m_eventUID":"b7cx--7vmE2UyW6IaeELXQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"7EoX3egRH0CnHzY2MVhfCw"},"StackAmount":3,"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"jgnv5LrPnUOdwwU0bnfKMg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":7575.0,"y":7410.0},"_UID":"08836e7f-62f9-4627-86bc-2c21be35de19","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"167"},{"_position":{"x":7695.0,"y":7560.0},"_UID":"498742b5-df18-4823-bd55-d4612f69b13f","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"183"},{"statement":{"_text":"Extort? That\u2019s a nasty word. It\u2019s simple supply and demand, my good customer. It costs a lot of money to bring food through a desert this large before it goes bad!","_meta":"SoroborMerchant_HK_Corsairs_02"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6855.0,"y":6540.0},"_UID":"ff142d9c-cec5-4ac9-85bb-21d73d9256c0","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"184"},{"_condition":{"checkMode":"AnyTrueSuffice","conditions":[{"QuestEventRef":{"m_eventUID":"Wnyn2O2Ykka-uqE8rRzvXA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"MeHR0Xabj0iIFDMJ_Hd3bg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":8055.0,"y":6585.0},"_UID":"894cd57d-808d-445d-8610-e61e26ae4c84","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"186"},{"statement":{"_text":"I''ll send a courier to Levant to let them know they no longer need to worry about the famine and can stop rationing their food.","_meta":"Soroborean_MouthFeed_13"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7800.0,"y":6735.0},"_UID":"827788f6-bab2-4c21-aa58-ec25dd9b9773","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"193"},{"_position":{"x":7920.0,"y":6900.0},"_UID":"da43967e-3ea2-47f3-832d-1b7e0f650e24","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"195"},{"_condition":{"QuestEventRef":{"m_eventUID":"yfYKYVWXkEm5ejc2YjrkRA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":8295.0,"y":6840.0},"_UID":"4bf38530-38e7-4cdd-a190-78442deea376","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"196"},{"statement":{"_text":"You got it. I don''t know how you did it, but you got it. Well done. I''ve spoken with Laine about what we need to do, and it seems feasible, but barely. I''ve got just about everything set up.","_meta":"Soroborean_MouthFeed_10"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":8160.0,"y":6960.0},"_UID":"8a8ddc50-0824-414f-87f3-1aaa9dbb4f39","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"199"},{"statement":{"_text":"Unfortunately my contacts and favors are all tapped out. <color=#00CCFF><b>I need another investment of 300 silver</b></color> to wrap this up and get started, but I''m out of places to borrow the money. A lot of people went all in on this plan.","_meta":"Soroborean_MouthFeed_11"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":8085.0,"y":7095.0},"_UID":"fd88dbcc-f8fa-42c5-bb03-d620bb1f6de9","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"201"},{"_condition":{"QuestEventRef":{"m_eventUID":"H8vMawAkDEekzyCEBEKZmQ"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":8115.0,"y":7245.0},"_UID":"a0df401e-f3cb-45cf-9acf-5354f3f00f52","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"203"},{"availableChoices":[{"statement":{"_text":"Here\u2019s your 300 silver. Make it happen.","_meta":"Player_Soroborean_MouthFeed_a_11"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":300},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"I''ll go find the money, I''ll be back.","_meta":"Player_Soroborean_MouthFeed_c_11"}},{"statement":{"_text":"Tell Clayton that [PlayerName] is calling in a favor. I think you''ll find that he will be willing to help finance this little business venture.","_meta":"Player_Soroborean_MouthFeed_b_11"},"condition":{"QuestEventRef":{"m_eventUID":"H8vMawAkDEekzyCEBEKZmQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":8055.0,"y":7410.0},"_UID":"dc248cb2-d432-4c5a-a15c-6081ae536635","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"206"},{"_action":{"actions":[{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":300}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"},{"QuestEventRef":{"m_eventUID":"MeHR0Xabj0iIFDMJ_Hd3bg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":7725.0,"y":7665.0},"_UID":"db2086a4-e234-4827-9748-0bf0d4f264c5","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"222"},{"_condition":{"QuestEventRef":{"m_eventUID":"7EoX3egRH0CnHzY2MVhfCw"},"MinStack":10,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":7650.0,"y":7890.0},"_UID":"827bc8a3-eaa1-4f22-9dab-faedeae007d3","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"234"},{"_condition":{"QuestEventRef":{"m_eventUID":"sOtZIuc_90moMYkseHl24w"},"ExpiryTime":1200,"$type":"NodeCanvas.Tasks.Conditions.Condition_CheckQuestEventExpiry"},"_position":{"x":7245.0,"y":8025.0},"_UID":"7d7888c6-ce6d-406d-9916-635e298561b4","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"237"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"8MR82yjVZUSAsTKi2obXyg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"fDugZJGivEyfzzE0D4wQ4g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":7110.0,"y":8175.0},"_UID":"cbdeb583-7afe-4b9a-9444-292c6e5e0282","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"240"},{"_targetNode":{"_condition":{"QuestEventRef":{"m_eventUID":"MeHR0Xabj0iIFDMJ_Hd3bg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":7425.0,"y":8775.0},"_UID":"3f7e8421-c0d1-4f51-a523-d5bbf454c0fd","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"248"},"_position":{"x":7425.0,"y":8310.0},"_UID":"d9fcaa29-33ff-4563-8cf6-e93a2150fbde","$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"247"},{"_action":{"QuestEventRef":{"m_eventUID":"fDugZJGivEyfzzE0D4wQ4g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":7470.0,"y":8205.0},"_UID":"448306db-f4f4-47b3-9eb6-527683c8b572","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"251"},{"_condition":{"QuestEventRef":{"m_eventUID":"7EoX3egRH0CnHzY2MVhfCw"},"MinStack":6,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":7860.0,"y":8025.0},"_UID":"d63fd1c7-0a76-4a4c-b0e1-ad28a7c5ec09","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"254"},{"_condition":{"QuestEventRef":{"m_eventUID":"sOtZIuc_90moMYkseHl24w"},"ExpiryTime":840,"$type":"NodeCanvas.Tasks.Conditions.Condition_CheckQuestEventExpiry"},"_position":{"x":7575.0,"y":8325.0},"_UID":"401a0bfc-1fb7-4af4-88f4-1869a3a6f0fb","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"257"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"8MR82yjVZUSAsTKi2obXyg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"fDugZJGivEyfzzE0D4wQ4g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":7185.0,"y":8565.0},"_UID":"eccb0441-378d-4f55-9178-7496a80eff8d","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"260"},{"$ref":"248"},{"statement":{"_text":"Thank you! Alright, with this I can finance the last step of the process! Your first shipment of Marshemlon plants <color=#00CCFF><b>will be in Levant</b></color> in a matter of days!","_meta":"Soroborean_MouthFeed_12"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7110.0,"y":8985.0},"_UID":"5df6b201-8ec8-4cb4-9db1-5acca7fd0712","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"267"},{"statement":{"_text":"I''ll send a courier to Levant to let them know they no longer need to worry about the famine and can stop rationing their food.","_meta":"Soroborean_MouthFeed_13"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7110.0,"y":9105.0},"_UID":"784e4f29-2206-4997-9ea1-38f47f526358","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"269"},{"_position":{"x":7260.0,"y":9210.0},"_UID":"198eff03-957f-4496-a51c-709fe46d16c7","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"271"},{"statement":{"_text":"Ahhhh, yes, he told me about you. He said to give you the preferential treatment if you needed anything big. Heh, you must have something really good on him. I''ll send him the bill.","_meta":"Soroborean_MouthFeed_14"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7680.0,"y":8970.0},"_UID":"a2e6abf1-9ea0-4a8c-85fa-3d2b759f6183","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"272"},{"statement":{"_text":"I''ll send a courier to Levant to let them know they no longer need to worry about the famine and can stop rationing their food.","_meta":"Soroborean_MouthFeed_13"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7710.0,"y":9060.0},"_UID":"9325fea5-97bb-4a27-b483-b266613177b4","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"274"},{"_position":{"x":7875.0,"y":9165.0},"_UID":"987bd874-6de2-4a19-a1d9-8d2b6a484bf1","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"276"},{"_action":{"QuestEventRef":{"m_eventUID":"fDugZJGivEyfzzE0D4wQ4g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":7515.0,"y":8565.0},"_UID":"56d66bf6-660c-4283-91e3-a273557ca093","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"277"},{"_condition":{"QuestEventRef":{"m_eventUID":"sOtZIuc_90moMYkseHl24w"},"ExpiryTime":600,"$type":"NodeCanvas.Tasks.Conditions.Condition_CheckQuestEventExpiry"},"_position":{"x":8055.0,"y":8295.0},"_UID":"a6784830-9fca-47ec-a9f2-d3380275ac94","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"280"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"8MR82yjVZUSAsTKi2obXyg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"fDugZJGivEyfzzE0D4wQ4g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":7845.0,"y":8565.0},"_UID":"704bd8e5-c620-4f82-a624-5aca178deb30","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"283"},{"_targetNode":{"$ref":"248"},"_position":{"x":8145.0,"y":8670.0},"_UID":"be55e2b7-a5c7-43b5-a6c1-ef680deedbf2","$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"290"},{"_action":{"QuestEventRef":{"m_eventUID":"fDugZJGivEyfzzE0D4wQ4g"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":8265.0,"y":8565.0},"_UID":"a6506a78-c897-463d-8b84-c879ea0522ca","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"291"},{"statement":{"_text":"Good, let me know as soon as you have enough. The containers and specialized wagon are ready, the preservation solution is good, I just need to hire some men to dig up the plants and bring them in and I can roll out to Levant with them.","_meta":"Soroborean_MouthFeed_15"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":8085.0,"y":7635.0},"_UID":"730bb206-52a0-4a70-81cb-6628d60a2dd7","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"294"},{"_action":{"QuestEventRef":{"m_eventUID":"kXpOlk0wDkKDJD6BvjDRvQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":8145.0,"y":7755.0},"_UID":"95bf0baf-be4b-416b-bf61-284f1c2446d9","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"296"},{"_position":{"x":8205.0,"y":7845.0},"_UID":"57100bfe-2caf-4a19-bf91-49cab2aa88b6","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"299"},{"availableChoices":[{"statement":{"_text":"Tell Clayton that [PlayerName] is calling in a favor. I think you''ll find that he will be willing to help finance this little business venture.","_meta":"Player_Soroborean_MouthFeed_b_11"}}],"_position":{"x":8520.0,"y":7425.0},"_UID":"b5efce51-edcb-44e2-8047-9e363c9a657d","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"300"},{"_action":{"QuestEventRef":{"m_eventUID":"Wnyn2O2Ykka-uqE8rRzvXA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":8535.0,"y":7575.0},"_UID":"488af5f0-decb-4612-9cbd-ffb03c4dad8c","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"304"},{"_targetNode":{"$ref":"234"},"_position":{"x":8655.0,"y":7725.0},"_UID":"0ca67510-1469-4954-bb49-b42904f31702","$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"307"},{"_condition":{"QuestEventRef":{"m_eventUID":"ScfN589DhkSaRnJZGNd15w"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":9360.0,"y":7035.0},"_UID":"50b74b70-8b9a-4b3d-be64-f769abbeda3a","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"308"},{"_condition":{"QuestEventRef":{"m_eventUID":"skKS2dsiwk6b3oHosWM_Bw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":9195.0,"y":7185.0},"_UID":"c381fa02-fe18-4625-9cc5-c85c036fee28","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"311"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"yfYKYVWXkEm5ejc2YjrkRA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"skKS2dsiwk6b3oHosWM_Bw"},"RemoveAllStack":true,"$type":"NodeCanvas.Tasks.Actions.RemoveQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":9090.0,"y":7275.0},"_UID":"07b37e92-7188-4970-8a8a-db10652d3efc","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"314"},{"_targetNode":{"$ref":"199"},"_position":{"x":9135.0,"y":7410.0},"_UID":"e716542c-5b46-4ff7-bd7c-3d9eb0a4f8a9","$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"321"},{"statement":{"_text":"If you want Sorobor Academy\u2019s help with this insane Marshmelon transportation plot, you\u2019ll need to <color=#00CCFF><b>bring me the Stone Tablet that the Lich in the South-West spire is keeping</b></color>. Good luck.","_meta":"Soroborean_MouthFeed_16"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9405.0,"y":7305.0},"_UID":"92d648d4-a605-4cdd-a130-692fc8b37b24","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"322"},{"_position":{"x":9525.0,"y":7440.0},"_UID":"0589d7d0-b573-404e-bd70-3e021bb2895a","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"324"},{"statement":{"_text":"Yeah, I heard about that. Sadly, I don\u2019t think there\u2019s much more we can do. The journey there is much too long to bring the volumes of food they need.","_meta":"Soroborean_MouthFeed_00"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9810.0,"y":7245.0},"_UID":"81d3323f-e7ac-48c2-a359-056e583f64f1","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"325"},{"availableChoices":[{"statement":{"_text":"I need Sorobor to move entire Marshmelon plants to Levant to deliver Marshmelons there.","_meta":"Player_Soroborean_MouthFeed_a_00"}}],"_position":{"x":9930.0,"y":7380.0},"_UID":"ee3210df-0af4-4151-8788-fbe1adaa611d","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"327"},{"statement":{"_text":"...Ahahaha! Ahahaha, I''m sorry, for a second I thought you just asked me if we could move entire Marshmelon plants to Levant just to deliver Marshmelons there! That''s a good one!","_meta":"Soroborean_MouthFeed_01"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9900.0,"y":7515.0},"_UID":"9941a5e1-8058-450e-97d9-8408a5aa0d65","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"331"},{"statement":{"_text":"...Elatt''s name you''re not joking. I...Give me a minute, I need to speak with some people about this.","_meta":"Soroborean_MouthFeed_02"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9945.0,"y":7620.0},"_UID":"9333cc63-7d44-455a-bfd0-b827539a5ddf","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"333"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":10050.0,"y":7755.0},"_UID":"580b8a92-955c-4ade-9730-5902f99bb548","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"335"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":10050.0,"y":7845.0},"_UID":"5b77aa28-f13b-4e24-90f3-62fed8f2c457","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"337"},{"statement":{"_text":"Listen, [PlayerName], I''ve spoken with my contacts about this and we all agree, this is insanity. Do you have any idea how hard it would be to not only dig up and transport multiple massive swamp plants, but also keep them alive through the entire desert journey? Have you lost your mind?","_meta":"Soroborean_MouthFeed_03"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9960.0,"y":7935.0},"_UID":"77be1096-0219-47cc-b65a-9248e05abb00","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"339"},{"availableChoices":[{"statement":{"_text":"Levant''s survival depends on it.","_meta":"Player_Soroborean_MouthFeed_a_03"}}],"_position":{"x":10035.0,"y":8070.0},"_UID":"7ecf74cd-9c6f-4ddb-98ec-b6eb54dc1117","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"341"},{"statement":{"_text":"I had a feeling that was the case. But the price to set this up is beyond anything you can pay. That said, you''re the adventurous type, aren''t you? There is something you can do that we''d consider a fair trade.","_meta":"Soroborean_MouthFeed_04"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9960.0,"y":8175.0},"_UID":"da28aa12-c2b4-4da3-aab8-120da47933a5","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"345"},{"statement":{"_text":"There''s an <color=#00CCFF><b>ancient peace treaty that was written on a stone tablet</b></color> shortly after the scourge. Old Language on one side, New Language on the other. It could effectively be used to translate things written in the Old Language.","_meta":"Soroborean_MouthFeed_05"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9855.0,"y":8265.0},"_UID":"5a94a82d-8022-440b-97bc-178aaed49372","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"347"},{"statement":{"_text":"A \"Rosetta Stone\", if you know Aurai''s ancient history. That would be an incredible boon to Sorobor''s interests. <color=#00CCFF><b>Bring it to us, and we''ll help you</b></color> with your Marshmelon madness.","_meta":"Soroborean_MouthFeed_06"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9975.0,"y":8385.0},"_UID":"83e37265-c36a-4984-89f1-8f7e60455261","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"349"},{"availableChoices":[{"statement":{"_text":"And what''s the catch?","_meta":"Player_Soroborean_MouthFeed_a_06"}}],"_position":{"x":10080.0,"y":8520.0},"_UID":"42bfd902-072e-4bd9-8ba3-cd34c403452c","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"351"},{"statement":{"_text":"The catch is that the Tablet I want you to fetch is in the possession of the Light Mender. That Lich who lives in the <color=#00CCFF><b>Spire in the South-West of the swamp</b></color>. And he doesn''t want to share it.","_meta":"Soroborean_MouthFeed_07"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9990.0,"y":8640.0},"_UID":"71a0683e-376e-49e0-b77b-82d37bf23930","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"355"},{"availableChoices":[{"statement":{"_text":"You want me to steal from a Lich? That''s the only way?","_meta":"Player_Soroborean_MouthFeed_a_07"}},{"statement":{"_text":"A Lich''s stone tablet. Steal it. No problem.","_meta":"Player_Soroborean_MouthFeed_b_07"}}],"_position":{"x":10110.0,"y":8745.0},"_UID":"91a5b9a6-e279-47b2-8393-b3141b8e3c6c","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"357"},{"statement":{"_text":"Yes. Arm yourself well, the Light Mender has been especially hostile for the last decade and has surrounded his spire with golems that will kill on sight. Be on guard in there and try not to end up in a fight with him.","_meta":"Soroborean_MouthFeed_08"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":9750.0,"y":8955.0},"_UID":"7fde3ac7-68db-46f4-afc4-b8681fd0b7cf","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"363"},{"_action":{"QuestEventRef":{"m_eventUID":"ScfN589DhkSaRnJZGNd15w"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":10035.0,"y":9135.0},"_UID":"caea41ac-d881-45e0-8588-3874bcbb9f81","$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"365"},{"_position":{"x":10110.0,"y":9240.0},"_UID":"d1fc7c47-eeb9-47e8-8003-ae834609f62c","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"368"},{"statement":{"_text":"I can''t tell how much of that is bravado and how much is your ignorance. Risking the fury of an immortal Lich is nothing like bopping a troglodyte on the head. Avoid fighting him if you can.","_meta":"Soroborean_MouthFeed_09"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":10215.0,"y":8940.0},"_UID":"08440579-db53-4862-b40c-b0e7fbffb778","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"369"},{"_targetNode":{"statement":{"_text":"Old Language, eh? Actually, I think I do. An explorer recently turned in an ancient peace treaty that was made about 80 years ago. It''s written in the Old Language on one side and a rough form of the New Language on the other.","_meta":"Sorobor_HM_TRUTH_02"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7905.0,"y":5625.0},"_UID":"1685a718-e96a-4ded-9195-02f47ea31441","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"372"},"_position":{"x":7187.0,"y":6094.0},"_UID":"ae9aa4a4-9a83-45f1-9db9-fc6aa0eb97bc","$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"371"},{"_sourceNodeUID":"66221609-c9cd-44c3-a42b-de4be7b1eb72","_position":{"x":7365.0,"y":5950.0},"_UID":"919e058c-2a0f-41d8-9d40-169340b22ff3","$type":"NodeCanvas.DialogueTrees.Jumper","$id":"374"},{"statement":{"_text":"See you soon!","_meta":"Merchant_Berg_InnKeeper_06"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7650.0,"y":5835.0},"_UID":"1f605ca2-e850-4814-8468-e7066d15bbd5","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"375"},{"_position":{"x":7725.0,"y":5955.0},"_UID":"e2d0f6b2-bfc7-47e8-a8c9-31c938afc4fe","$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"377"},{"_condition":{"QuestEventRef":{"m_eventUID":"UGBKCzoCA0-3u2oxeaFMDg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":6150.0,"y":4815.0},"_UID":"f0b0aadf-6f6c-4ce6-8a97-52467abec4f5","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"378"},{"statement":{"_text":"I\u2019ve heard rumors of a Royal Manticore making its lair in the region...I hope it\u2019s wrong. Regular Manticores are already terrifying enough! As someone who travels these roads often, I understand that danger all too well.","_meta":"Merchant_Berg_CaravanTrader_01"},"_actorName":"Soroborean caravanner","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6030.0,"y":5055.0},"_UID":"9e9f5c52-693c-4d41-97e8-05a25e11e7c8","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"381"},{"_condition":{"QuestEventRef":{"m_eventUID":"ruklyyxmpUGcDYj1xyJSMQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":6630.0,"y":4920.0},"_UID":"86b86794-919f-47fe-8b0d-b81e915c4e98","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"383"},{"statement":{"_text":"I always enjoy my visits to Monsoon! A true oasis inside a toxic marsh... Need anything to handle the danger outside?","_meta":"Merchant_Monsoon_CaravanTrader_01"},"_actorName":"Soroborean caravanner","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6510.0,"y":5070.0},"_UID":"22f151d3-7166-4fdb-b39d-4e0499905113","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"386"},{"_condition":{"QuestEventRef":{"m_eventUID":"fJSdc0sL90GdXz39AHFl5A"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":7065.0,"y":5025.0},"_UID":"dbb4c8cc-e54e-4db4-ab48-ef5a10801e3e","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"388"},{"_condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"fW6jMpdasEW1_tM0-qAGnw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"questRef":{"_value":{"m_itemID":7011005}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":7005.0,"y":5190.0},"_UID":"a48026c8-a34d-44c0-9726-ee247c5fccb6","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"391"},{"statement":{"_text":"Hey I recognize you! It\u2019s a long way from Cierzo, isn\u2019t it? What desires can I satisfy for you today?","_meta":"Merchant_Levant_CaravanTrader_01"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6795.0,"y":5370.0},"_UID":"7a5b2ebe-c847-43f6-b409-7afa90ee3006","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"399"},{"statement":{"_text":"Hey I recognize you! It\u2019s a long way from Cierzo, isn\u2019t it? What desires can I satisfy for you today?","_meta":"Merchant_Levant_CaravanTrader_01"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7230.0,"y":5340.0},"_UID":"7a2de831-dbcf-4764-bec6-2c6fc60b4abd","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"401"},{"statement":{"_text":"I\u2019m sorry if the price of goods is a little higher than usual, it\u2019s not something I have control over when selling goods in Levant.","_meta":"ShopPressured_NEUT_04"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7245.0,"y":5445.0},"_UID":"52fa74c2-15f3-441e-9b49-357ab720420f","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"403"},{"_condition":{"QuestEventRef":{"m_eventUID":"NEX4WlJq6kGhEC3KsM1d0A"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_color":{"r":0.7019608,"g":1.0,"b":0.4980392,"a":1.0},"_position":{"x":7673.0,"y":5161.0},"_UID":"aa2a60f5-83ac-4e3c-97cd-3a5653180006","$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"405"},{"statement":{"_text":"Well, fancy running into you out here! What can I do for you?","_meta":"Travel_SO_Harmattan_01"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7612.0,"y":5328.0},"_UID":"e207f172-7d95-4408-be5e-52a49e53a091","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"408"},{"_condition":{"QuestEventRef":{"m_eventUID":"pKdDFd6IdUGF6C11co6zLw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_color":{"r":0.7019608,"g":1.0,"b":0.4980392,"a":1.0},"_position":{"x":8087.0,"y":5292.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"410"},{"statement":{"_text":"no loc - New Sirocco greeting","_meta":"Caraveneer_CA_Basebuilding_01"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7920.0,"y":5457.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"413"},{"statement":{"_text":"Well, fancy running into you out here! What can I do for you?","_meta":"CaravanTrader_NEUT_01"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":8317.0,"y":5439.0},"_UID":"4aedfc43-b321-47cd-8f21-70f20a6de038","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"415"},{"statement":{"_text":"Ah! Yes, I can see why you\u2019d be curious.  Sorobor Academy is a storied institution, going back to even before the Scourge. It\u2019s more than just a place of learning. It\u2019s where the greatest minds in Aurai congregate to shape its future.","_meta":"Caravaneer_SO_Prequest_01"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":5700.0,"y":5460.0},"_UID":"66221609-c9cd-44c3-a42b-de4be7b1eb72","_tag":"NewQuestline","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"417"},{"statement":{"_text":"As long as you can put in the work, you\u2019ll get quite a bit out of it. Connections, wealth...It asks a lot of hard work out of you, and years of effort before you see a payoff, but I can\u2019t imagine living any other way.","_meta":"Caravaneer_SO_Prequest_02"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":5715.0,"y":5595.0},"_tag":"","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"419"},{"availableChoices":[{"statement":{"_text":"I\u2019d like to join Sorobor Academy. How would I go about doing that?","_meta":"Player_Caravaneer_SO_Prequest_02_a"},"condition":{"questRef":{"_value":{"m_itemID":7011001}},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"}},{"statement":{"_text":"I\u2019d like to join Sorobor Academy. How would I go about doing that?","_meta":"Player_Caravaneer_SO_Prequest_02_a"},"condition":{"conditions":[{"questRef":{"_value":{"m_itemID":7011001}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},{"QuestEventRef":{"m_eventUID":"bjVloYMQxk6KXx0gph2A1Q"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"}},{"statement":{"_text":"Farewell.","_meta":"Merchant_Generic_Farewell_01"}}],"_position":{"x":5685.0,"y":5715.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"421"},{"statement":{"_text":"Hmmm...I\u2019d recommend you <color=#00CCFF><b>concentrate on not losing your lighthouse first</b></color>, [PlayerName]. Speak with me once that\u2019s been resolved, one way or another.","_meta":"Caravaneer_SO_Prequest_03"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":4830.0,"y":5940.0},"_tag":"","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"439"},{"_color":{"g":0.7254902,"b":0.9098039,"a":1.0},"_position":{"x":5715.0,"y":6270.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"441"},{"_action":{"quest":{},"questRef":{"_value":{"m_itemID":7011400}},"$type":"NodeCanvas.Tasks.Actions.GiveQuest"},"_position":{"x":5655.0,"y":5925.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"442"},{"statement":{"_text":"A worthy goal, [PlayerName]. Yes, I think you\u2019d fit right in. I\u2019m planning on making the journey to our home city, Harmattan right now, actually! If you\u2019d like a lift, I can absolutely take you along!  ...For a fee, of course.","_meta":"Caravaneer_SO_Prequest_04"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":5580.0,"y":6030.0},"_tag":"","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"447"},{"statement":{"_text":"Best of luck to you on your journey, my friend! ","_meta":"Caravaneer_SO_Prequest_06"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":6030.0,"y":5925.0},"_tag":"","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"449"},{"$ref":"372"},{"statement":{"_text":"I''ve got a parchment copy of it right here, actually. You can use this to translate from one language to the other. It''s yours for 150 Silver.","_meta":"Sorobor_HM_TRUTH_03"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":{"x":7920.0,"y":5730.0},"_UID":"4011d874-f808-4436-adc3-1f6b61f26bb0","$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"451"},{"availableChoices":[{"statement":{"_text":"Deal, here\u2019s your money.","_meta":"Merchant_Generic_Pay_01"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"59b1bec3-5814-434a-901e-51ab31076630"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":150},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"No, thanks.","_meta":"Merchant_Generic_No_02"}}],"_position":{"x":8025.0,"y":5865.0},"_UID":"812e7684-88c3-4a70-b10b-1693dc752285","$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"453"},{"statement":{"_text":"Deal! <color=#00CCFF><b>Good luck with your translation job</b></color>!","_meta":"Sorobor_HM_TRUTH_04"},"_actorName":"name_unpc_caravantrader_01 ","_actorParameterID":"149a7ace-9e2b-4fa1-b2da-1b263e9b6c95","_position":
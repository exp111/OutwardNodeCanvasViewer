  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-5069.0,"y":-6266.0},"nodes":[{"statement":{"_text":"Keep your distance, an investigation is in progress!","_meta":"CrimeSceneGuards_HM_Doubts_01"},"_actorName":"name_merchant_monsoonguard_01 ","_actorParameterID":"700350c3-2b24-4884-a87f-6a3f7d1695e4","_position":{"x":5775.0,"y":5250.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"2"},{"availableChoices":[{"statement":{"_text":"What\u2019s going on?","_meta":"PlayerCrimeSceneGuards_HM_Doubts_a_01"}},{"statement":{"_text":"I\u2019ll leave you to it, then.","_meta":"PlayerCrimeSceneGuards_HM_Doubts_b_01"}}],"_position":{"x":5820.0,"y":5400.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"4"},{"statement":{"_text":"There was a murder. A missionary from the Holy Mission killed a disciple in broad daylight. Until the matter is resolved, the accused will remain in the custody of Damian Lockwell. If you have further questions, speak with Ellinara!","_meta":"CrimeSceneGuards_HM_Doubts_02"},"_actorName":"name_merchant_monsoonguard_01 ","_actorParameterID":"700350c3-2b24-4884-a87f-6a3f7d1695e4","_position":{"x":5595.0,"y":5580.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"10"},{"availableChoices":[{"statement":{"_text":"Who is the suspect?","_meta":"PlayerCrimeSceneGuards_HM_Doubts_a_02"}},{"statement":{"_text":"Who was the victim?","_meta":"PlayerCrimeSceneGuards_HM_Doubts_b_02"}},{"statement":{"_text":"I\u2019ll leave you to it, then.","_meta":"PlayerCrimeSceneGuards_HM_Doubts_b_01"}}],"_position":{"x":5700.0,"y":5715.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"12"},{"statement":{"_text":"That woman who came back recently with the body parts made of light, Oliele, I think her name is.","_meta":"CrimeSceneGuards_HM_Doubts_03"},"_actorName":"name_merchant_monsoonguard_01 ","_actorParameterID":"700350c3-2b24-4884-a87f-6a3f7d1695e4","_position":{"x":5370.0,"y":6195.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"20"},{"availableChoices":[{"statement":{"_text":"Wait, Oliele? I don\u2019t believe that.","_meta":"PlayerCrimeSceneGuards_HM_Doubts_a_04"}},{"statement":{"_text":"I\u2019ll leave you to it, then.","_meta":"PlayerCrimeSceneGuards_HM_Doubts_b_01"}}],"_position":{"x":5355.0,"y":6360.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"22"},{"statement":{"_text":"We have about 8 witnesses who testify that she did it. Most of them claim that it was self-defense, but Damian Lockwell has ordered us to treat it like a homicide until proven otherwise.","_meta":"CrimeSceneGuards_HM_Doubts_05"},"_actorName":"name_merchant_monsoonguard_01 ","_actorParameterID":"700350c3-2b24-4884-a87f-6a3f7d1695e4","_position":{"x":5085.0,"y":6540.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"28"},{"_position":{"x":5160.0,"y":6690.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"30"},{"statement":{"_text":"Again, if you have any questions, speak with Ellinara. We are just doing our job, here!","_meta":"CrimeSceneGuards_HM_Doubts_06"},"_actorName":"name_merchant_monsoonguard_01 ","_actorParameterID":"700350c3-2b24-4884-a87f-6a3f7d1695e4","_position":{"x":5520.0,"y":6510.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"31"},{"_position":{"x":5670.0,"y":6600.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"33"},{"statement":{"_text":"Richard. He was one of the newer disciples of the Holy Mission. Wouldn\u2019t stop being a preachy git at the inn ever since he got promoted. He\u2019d just gotten back from a trip to Sirroco before this happened, I believe.","_meta":"CrimeSceneGuards_HM_Doubts_04"},"_actorName":"name_merchant_monsoonguard_01 ","_actorParameterID":"700350c3-2b24-4884-a87f-6a3f7d1695e4","_position":{"x":5730.0,"y":5940.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"34"},{"availableChoices":[{"statement":{"_text":"Who is the suspect?","_meta":"PlayerCrimeSceneGuards_HM_Doubts_a_02"}},{"statement":{"_text":"I\u2019ll leave you to it, then.","_meta":"PlayerCrimeSceneGuards_HM_Doubts_b_01"}}],"_position":{"x":5730.0,"y":6060.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"36"},{"_position":{"x":6090.0,"y":6315.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"42"},{"_position":{"x":6315.0,"y":5970.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"43"},{"_position":{"x":6120.0,"y":5595.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"44"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"4"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"4"},"_targetNode":{"$ref":"10"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"4"},"_targetNode":{"$ref":"44"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"10"},"_targetNode":{"$ref":"12"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"12"},"_targetNode":{"$ref":"20"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"12"},"_targetNode":{"$ref":"34"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"12"},"_targetNode":{"$ref":"43"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"20"},"_targetNode":{"$ref":"22"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"22"},"_targetNode":{"$ref":"28"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"22"},"_targetNode":{"$ref":"31"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"28"},"_targetNode":{"$ref":"30"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"31"},"_targetNode":{"$ref":"33"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"34"},"_targetNode":{"$ref":"36"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"36"},"_targetNode":{"$ref":"20"},"$type":"No
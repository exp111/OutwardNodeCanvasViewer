  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-3589.0,"y":-3372.0},"zoomFactor":0.9825918,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"kS9py1Sy4Eegcdl4_kOKBg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4005.0,"y":3540.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"Your face isn''t familiar to me. You must be new to Levant. Welcome to the Heroic Kingdom, where our hope is to give every person the chance to do what they excel at, and the chance to live the life they wish to live.","_meta":"Calixia_KL_prequest_01"},"_actorName":"name_unpc_calixa_01 ","_actorParameterID":"2c5c6178-69de-49c0-b3e9-a601ab8dc213","_position":{"x":3780.0,"y":3690.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"5"},{"statement":{"_text":"I am King Simeon, and this is my Queen, Calixa. My gut tells me that your skillset seems to lie in survival. I\u2019m sure that you\u2019d be a great asset if you lent us your talents as a mercenary.","_meta":"Calixia_KL_prequest_02"},"_actorName":"name_unpc_simeon_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3720.0,"y":3825.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"7"},{"statement":{"_text":"We are constantly under threat by the beasts and bandits of the desert, and are facing a problem with overpopulation. If you believe you can help us, then we would be overjoyed to have you.","_meta":"Calixia_KL_prequest_03"},"_actorName":"name_unpc_calixa_01 ","_actorParameterID":"2c5c6178-69de-49c0-b3e9-a601ab8dc213","_position":{"x":3675.0,"y":3930.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"9"},{"statement":{"_text":"Will you, [PlayerName], pledge your service to the ideals of the Heroic Kingdom of Levant? To help us create a place where humankind can do more than just survive?","_meta":"Calixia_KL_prequest_04"},"_actorName":"name_unpc_calixa_01 ","_actorParameterID":"2c5c6178-69de-49c0-b3e9-a601ab8dc213","_position":{"x":3675.0,"y":4035.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"11"},{"availableChoices":[{"statement":{"_text":"I don\u2019t think I\u2019m the right person for this. Sorry.","_meta":"Player_Calixia_KL_prequest_a_04"}},{"statement":{"_text":"I pledge myself to the Heroic Kingdom.","_meta":"Player_Calixia_KL_prequest_B_04"}}],"_position":{"x":3675.0,"y":4185.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"13"},{"statement":{"_text":"That\u2019s a shame. If you change your mind, the kingdom will be waiting.","_meta":"Calixia_KL_prequest_06"},"_actorName":"Simeon King of Levant","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3345.0,"y":4410.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"19"},{"_position":{"x":3420.0,"y":4590.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"21"},{"statement":{"_text":"(This is a turning point in my life... <color=#00CCFF><b>Am I certain I wish to pledge my life to the Heroic Kingdom of Levant</b></color>?)","_meta":"Calixia_KL_prequest_07"},"_position":{"x":3870.0,"y":4335.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"22"},{"availableChoices":[{"statement":{"_text":"I don\u2019t think I\u2019m the right person for this. Sorry.","_meta":"Player_Calixia_KL_prequest_a_04"}},{"statement":{"_text":"I pledge myself to the Heroic Kingdom.","_meta":"Player_Calixia_KL_prequest_B_04"}}],"_position":{"x":4035.0,"y":4500.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"24"},{"statement":{"_text":"That\u2019s a shame. If you change your mind, the kingdom will be waiting.","_meta":"Calixia_KL_prequest_06"},"_actorName":"Simeon King of Levant","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3795.0,"y":4665.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"30"},{"_position":{"x":3855.0,"y":4815.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"32"},{"statement":{"_text":"Excellent. The Heroic Kingdom of Levant is happy to have you, citizen. If you\u2019re looking for work, I\u2019m sure Cyrene will have plenty for you that will pay well.","_meta":"Calixia_KL_prequest_05"},"_actorName":"Calixa Queen of Levant","_actorParameterID":"2c5c6178-69de-49c0-b3e9-a601ab8dc213","_position":{"x":4290.0,"y":4665.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"33"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"bjVloYMQxk6KXx0gph2A1Q"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"SV3XUncnQU60ozhj9xTRpg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"1a6Zs9A_gEmScBetAraQaw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"3_soGcNagk-KcYSeqpEgMg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"bm3rB3abI0KFok2x5P0lrg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"quest":{},"questRef":{"_value":{"m_itemID":7011301}},"$type":"NodeCanvas.Tasks.Actions.GiveQuest"},{"Stop":{},"Music":{"_value":"BGM_EventQuest"},"$type":"NodeCanvas.Tasks.Actions.StartMusicEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4410.0,"y":4830.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"35"},{"_position":{"x":4440.0,"y":5040.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"55"},{"statement":{"_text":"I am sorry, but unless you have official business here, I do not have the time to spare for a visitor.","_meta":"Calixia_KL_prequest_08"},"_actorName":"name_unpc_calixa_01 ","_actorParameterID":"2c5c6178-69de-49c0-b3e9-a601ab8dc213","_position":{"x":4230.0,"y":3690.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"56"},{"_position":{"x":4395.0,"y":3825.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"58"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":
  m_Name: Dialogue_CardinalBourlamaque_Neut_Purifier
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-2924.0,"y":-4224.0},"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"n_qdf8jRWkKYY83X4u3ltQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3795.0,"y":3825.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"Welcome, [PlayerName]. I wish to thank you for dealing with those who were kidnapping and bringing harm to our people.","_meta":"Cardinal_Purifier_01"},"_actorName":"name_unpc_cardinalbourlamaque_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3540.0,"y":3975.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"5"},{"_condition":{"QuestEventRef":{"m_eventUID":"sB3LAmFL50OMEGOLA7PFSg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3510.0,"y":4125.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"7"},{"statement":{"_text":"The fact that Immaculates were hiding so close to Monsoon is troubling. They are normally intelligent enough to avoid the Hallowed Marsh entirely.","_meta":"Cardinal_Purifier_02"},"_actorName":"Cardinal Bourlamaque","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3045.0,"y":4245.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"10"},{"availableChoices":[{"statement":{"_text":"I think they were trying to find a way to break free from their instinct to attack Ellat.","_meta":"Player_Cardinal_Purifier_a_02"}},{"statement":{"_text":"If you need me to slaughter more of them, just let me know.","_meta":"Player_Cardinal_Purifier_b_02"}}],"_position":{"x":3120.0,"y":4380.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"12"},{"statement":{"_text":"Were they, now? I can only hope they someday find success, although I do not have much hope. It would benefit us all if they overcame their origins, rather than force us to destroy them.","_meta":"Cardinal_Purifier_03"},"_actorName":"Cardinal Bourlamaque","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2850.0,"y":4545.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"18"},{"statement":{"_text":"(You feel the warmth of a campfire fill you...Elatt speaks in your mind)","_meta":"Elatt_HM_Questions_01"},"_actorName":"name_unpc_narrator","_actorParameterID":"8bf5ba1d-7212-4acb-94d4-2b0cc942e331","_position":{"x":3540.0,"y":4710.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"20"},{"_condition":{"QuestEventRef":{"m_eventUID":"gIsll-PGyky5srZFHboCuA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3390.0,"y":4875.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"22"},{"statement":{"_text":"I am satisfied with your results, [PlayerName]. While I worry that without a concrete judgment, a similar situation may arise again, the crisis has been dealt with and the perpetrator will face their due punishment. You have done well.","_meta":"Elatt_Purifier_02"},"_actorName":"name_unpc_elatt_01 ","_actorParameterID":"32c57d5c-2e51-4642-b947-d73e15486db9","_position":{"x":3105.0,"y":5085.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"25"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"DGV6NDtw80yhYTpi9cFv_w"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":4300220}},"Quantity":{"_value":2},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3075.0,"y":5235.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"27"},{"_position":{"x":3105.0,"y":5445.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"41"},{"_condition":{"QuestEventRef":{"m_eventUID":"_YpTpTGglUKzg5VP7octtg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3675.0,"y":5040.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"42"},{"statement":{"_text":"I thank you for rooting out the Immaculates and mercenaries that preyed upon our people, [PlayerName]. That is all.","_meta":"Elatt_Purifier_03"},"_actorName":"name_unpc_elatt_01 ","_actorParameterID":"32c57d5c-2e51-4642-b947-d73e15486db9","_position":{"x":3480.0,"y":5220.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"45"},{"availableChoices":[{"statement":{"_text":"Wait, don\u2019t I get a reward?","_meta":"Player_Elatt_Purifier_b_03"}},{"statement":{"_text":"Thank you, Elatt.","_meta":"Player_Elatt_Purifier_a_03"}}],"_position":{"x":3585.0,"y":5355.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"47"},{"statement":{"_text":"You have already received your worldly reward from Jager Sullivan. You do not deserve more than what you have already received.","_meta":"Elatt_Purifier_04"},"_actorName":"name_unpc_elatt_01 ","_actorParameterID":"32c57d5c-2e51-4642-b947-d73e15486db9","_position":{"x":3270.0,"y":5565.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"53"},{"statement":{"_text":"(You feel Elatt\u2019s presence dissipate like a bucket being dumped on a flame. He is clearly disappointed in you)","_meta":"Elatt_Purifier_05"},"_actorName":"name_unpc_narrator","_actorParameterID":"8bf5ba1d-7212-4acb-94d4-2b0cc942e331","_position":{"x":3570.0,"y":5760.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"55"},{"_action":{"QuestEventRef":{"m_eventUID":"DGV6NDtw80yhYTpi9cFv_w"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3615.0,"y":5865.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"57"},{"_position":{"x":3750.0,"y":5970.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"60"},{"statement":{"_text":"Well done, [PlayerName]. The zeal with which you pursued justice for our fallen comrades, and with which you protected the Marsh is admirable. Ask of me, and you shall receive my blessing","_meta":"Elatt_Purifier_01"},"_actorName":"name_unpc_elatt_01 ","_actorParameterID":"32c57d5c-2e51-4642-b947-d73e15486db9","_position":{"x":3900.0,"y":5250.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"61"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"DGV6NDtw80yhYTpi9cFv_w"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":4300220}},"Quantity":{"_value":3},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":5700129}},"Quantity":{"_value":1},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4050.0,"y":5445.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"63"},{"_position":{"x":4200.0,"y":5595.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"82"},{"statement":{"_text":"I shall, but with a heavy heart. Immaculates may be Scourge Beasts, but they can think and reason. It would benefit us all if they overcame their origins, rather than force us to destroy them.","_meta":"Cardinal_Purifier_04"},"_actorName":"Cardinal Bourlamaque","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3285.0,"y":4545.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"83"},{"statement":{"_text":"Greetings. I hope you\u2019re well!","_meta":"Salutation_Generic_02"},"_actorName":"name_unpc_cardinalbourlamaque_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4080.0,"y":4050.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"85"},{"_position":{"x":4095.0,"y":4155.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"87"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"85"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"7"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{
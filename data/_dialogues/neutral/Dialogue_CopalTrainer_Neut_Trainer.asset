  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-3952.0,"y":-4187.0},"zoomFactor":0.7600141,"nodes":[{"statement":{"_text":"Hey there. Watch yourself in the swamp. Drop your guard for even a second and you\u2019re likely to find out what the inside of a Tuanosaur\u2019s belly looks like.","_meta":"Trainer_Micro_MoonSwipe_01"},"_actorName":"name_Microtrainer_CopalGuy_01","_actorParameterID":"582dea3f-5e21-4a9e-ac99-51c206aa7755","_position":{"x":5580.0,"y":5340.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"2"},{"availableChoices":[{"statement":{"_text":"I\u2019d like to talk.","_meta":"Merchant_Generic_Talk_01"}},{"statement":{"_text":"I want to train with you.","_meta":"Merchant_Generic_Train_01"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"4002dde1-5c48-4f3a-81ba-c8c506b74a65"},"skill":{"_value":1},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_KnowSkill"}}],"_position":{"x":5655.0,"y":5475.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"4"},{"statement":{"_text":"You ever been to the Jade Quarry? It\u2019s mostly overrun by vermin now, but if you have the stones for it, there\u2019s still plenty of good jade to be found in there. It\u2019s down around the south of the swamp. You\u2019ll know it by the elevator you need to ride to get inside.","_meta":"Trainer_Micro_MoonSwipe_05"},"_actorName":"name_Microtrainer_CopalGuy_01","_actorParameterID":"582dea3f-5e21-4a9e-ac99-51c206aa7755","_position":{"x":5355.0,"y":5775.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"13"},{"_position":{"x":5670.0,"y":5985.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"15"},{"statement":{"_text":"Hmm, yeah, I could teach you the MoonSwipe. It\u2019s a large, sweeping technique for Polearm weapons that\u2019s great at dealing with crowds of enemies attacking from all sides. I\u2019ll teach it to you for 50 silver. Interested?","_meta":"Trainer_Micro_MoonSwipe_02"},"_actorName":"name_Microtrainer_CopalGuy_01","_actorParameterID":"582dea3f-5e21-4a9e-ac99-51c206aa7755","_position":{"x":5940.0,"y":5715.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"16"},{"availableChoices":[{"statement":{"_text":"Deal, here\u2019s your money.","_meta":"Merchant_Generic_Pay_01"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"4002dde1-5c48-4f3a-81ba-c8c506b74a65"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":50},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"I''m fine, thank you.","_meta":"Merchant_Generic_ImFine_01"}}],"_position":{"x":6060.0,"y":5985.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"18"},{"statement":{"_text":"Excellent. Let\u2019s step outside and I\u2019ll show you. What you do is you use your shoulder to generate most of the momentum-","_meta":"Trainer_Micro_MoonSwipe_03"},"_actorName":"name_Microtrainer_CopalGuy_01","_actorParameterID":"582dea3f-5e21-4a9e-ac99-51c206aa7755","_position":{"x":5745.0,"y":6150.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"30"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":5880.0,"y":6270.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"32"},{"_action":{"actions":[{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"4002dde1-5c48-4f3a-81ba-c8c506b74a65"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":50}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"},{"RewardReceiver":"Instigator","SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":8100320}},"Quantity":{"_value":1},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":5850.0,"y":6360.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"34"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":5895.0,"y":6495.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"53"},{"statement":{"_text":"-And that''s how you do the MoonSwing. Here''s hoping it saves your life out there!","_meta":"Trainer_Micro_MoonSwipe_04"},"_actorName":"name_Microtrainer_CopalGuy_01","_actorParameterID":"582dea3f-5e21-4a9e-ac99-51c206aa7755","_position":{"x":5700.0,"y":6615.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"55"},{"_position":{"x":6090.0,"y":6765.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"57"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"4"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"4"},"_targetNode":{"$ref":"13"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"4"},"_targetNode":{"$ref":"16"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"13"},"_targetNode":{"$ref":"15"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"16"},"_targetNode":{"$ref":"18"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"18"},"_targetNode":{"$ref":"30"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"18"},"_targetNode":{"$ref":"57"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"30"},"_targetNode":{"$ref":"32"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"32"},"_targetNode":{"$ref":"34"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"34"},"_targetNode":{"$ref":"53"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"53"},"_targetNode":{"$ref":"55"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"55"},"_targetNode":{"$ref":"57"},"$type":"NodeCanvas.DialogueTrees.DTConnection"}],"localBlackboard":{"_name":"Local Blackboard","_variables":{"gInstigator":{"_name":"gInstigator","_id":"4002dde1-5c48-4f3a-81ba-c8c506b74a65","$type":"NodeCanvas.Framework.Variable`1[[Character, Assembly-
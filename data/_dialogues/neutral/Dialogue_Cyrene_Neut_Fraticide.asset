  m_Name: Dialogue_Cyrene_Neut_Fraticide
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-2589.416,"y":-1851.267},"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"aAckSkdWiU6UJA3f_UjRIw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3345.0,"y":2085.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"I\u2019m afraid I\u2019m quite busy, perhaps some other time.","_meta":"Salutation_Generic_03"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3165.0,"y":2190.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"5"},{"_position":{"x":3195.0,"y":2325.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"7"},{"_condition":{"QuestEventRef":{"m_eventUID":"2WceKae4PEaCyh_s5sjuBQ"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3420.0,"y":2295.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"8"},{"_condition":{"QuestEventRef":{"m_eventUID":"DxAXH3V5MEqcpl2Hmewekw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3255.0,"y":2490.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"11"},{"_targetNode":{"statement":{"_text":"Alright, you both have me. I know when I''m beaten. I''ll surrender.","_meta":"PrincePietro_BloodSun_02"},"_actorName":"name_unpc_princepietro_01 ","_actorParameterID":"a5650a7c-f79d-4dd6-9f4b-8006d524a90a","_position":{"x":5490.0,"y":3345.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"15"},"_position":{"x":3135.0,"y":2715.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"14"},{"_condition":{"QuestEventRef":{"m_eventUID":"P6RQa4OcwkqjdeRqtE5SQw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3495.0,"y":2895.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"17"},{"statement":{"_text":"Alright, we\u2019ve blocked off the other exits. Let\u2019s split up and take this area cleanly. <color=#00CCFF><b>Keep your eyes peeled for the Prince Pietro, and the Baron</b></color>. We don\u2019t want either of them to risk slipping through our fingers.","_meta":"Cyrene_BloodSun_14"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3195.0,"y":3000.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"20"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":3300.0,"y":3120.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"22"},{"_action":{"QuestEventRef":{"m_eventUID":"DxAXH3V5MEqcpl2Hmewekw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3240.0,"y":3225.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"24"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":3240.0,"y":3330.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"27"},{"_position":{"x":3225.0,"y":3480.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"29"},{"_condition":{"QuestEventRef":{"m_eventUID":"fW6jMpdasEW1_tM0-qAGnw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3630.0,"y":3120.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"30"},{"_targetNode":{"availableChoices":[{"statement":{"_text":"Yes, I\u2019m ready to come along, where are we going?","_meta":"Player_Cyrene_BloodSun_a_11"}},{"statement":{"_text":"No thanks. I think I\u2019ll let you handle this, Cyrene.","_meta":"Player_Cyrene_BloodSun_b_11"}}],"_position":{"x":3255.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"34"},"_position":{"x":3585.0,"y":3300.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"33"},{"statement":{"_text":"You have something about the murder of Prince Jaden? Don\u2019t just stand there, spill it!","_meta":"Cyrene_BloodSun_06"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3675.0,"y":3405.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"40"},{"availableChoices":[{"statement":{"_text":"I\u2019ve found some evidence about the plot, I think you\u2019ll want to see this.","_meta":"Player_Cyrene_BloodSun_a_06"},"condition":{"QuestEventRef":{"m_eventUID":"0H-81cGJ_0SwXDCG2f2dDg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"Sorry, I don\u2019t have anything you don\u2019t already know.","_meta":"Player_Cyrene_BloodSun_b_06"}}],"_position":{"x":3720.0,"y":3570.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"42"},{"_condition":{"QuestEventRef":{"m_eventUID":"0H-81cGJ_0SwXDCG2f2dDg"},"MinStack":3,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3525.0,"y":3720.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"50"},{"statement":{"_text":"This is...I can\u2019t avoid this conclusion any longer. The way this lines up with my own investigation...Prince Pietro, what in Elatt\u2019s name were you thinking?","_meta":"Cyrene_BloodSun_08"},"_actorName":"Cyrene","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3240.0,"y":3855.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"53"},{"statement":{"_text":"Based on what we\u2019ve found, it seems like Prince Pietro has been plotting a coup against King Simeon and Queen Calixa, and that he masterminded the attack on Berg as well. ","_meta":"Cyrene_BloodSun_09"},"_actorName":"Cyrene","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3210.0,"y":3960.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"55"},{"statement":{"_text":"Not to mention he\u2019s been making use of that damned smuggler\u2019s tunnel under the slums that Baron Montgomery has been tacitly leaving unattended to.","_meta":"Cyrene_BloodSun_10"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3165.0,"y":4080.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"57"},{"statement":{"_text":"We need to move on the pair of them. I have a squadron of troops ready to go, but <color=#00CCFF><b>I could use the extra help</b></color>. Are you ready to go now?","_meta":"Cyrene_BloodSun_11"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3150.0,"y":4200.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"59"},{"_action":{"QuestEventRef":{"m_eventUID":"fW6jMpdasEW1_tM0-qAGnw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3225.0,"y":4350.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"61"},{"$ref":"34"},{"statement":{"_text":"<color=#00CCFF><b>We\u2019re going to flush them out of that smuggler\u2019s tunnel</b></color>. You and I will go in from the Baron\u2019s manor, I\u2019ll have Yzan hold down the Palace side entrance, and my troops will cover the slums exit and the desert exit. Come on, let\u2019s roll.","_meta":"Cyrene_BloodSun_12"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2775.0,"y":4650.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"64"},{"_action":{"QuestEventRef":{"m_eventUID":"P6RQa4OcwkqjdeRqtE5SQw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":2955.0,"y":4800.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"66"},{"_action":{"Area":{"_value":"AbrassarDungeon1"},"SpawnPoint":{"_value":2},"$type":"NodeCanvas.Tasks.Actions.AreaSwitchPlayers"},"_position":{"x":2940.0,"y":4950.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"69"},{"_position":{"x":3030.0,"y":5085.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"73"},{"statement":{"_text":"Very well. Yzan and I will handle this on our own, then. Come back if you change your mind, and maybe we\u2019ll still have some way for you to help.","_meta":"Cyrene_BloodSun_13"},"_actorName":"Cyrene","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3465.0,"y":4710.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"74"},{"_position":{"x":3615.0,"y":4875.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"76"},{"statement":{"_text":"Hmm...I appreciate the evidence you have, but <color=#00CCFF><b>there\u2019s not enough</b></color> for me to move on this...I\u2019m doing my own investigation and not everything lines up yet. If you find some more, please report back to me immediately.","_meta":"Cyrene_BloodSun_07"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3720.0,"y":3885.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"77"},{"_position":{"x":3945.0,"y":4080.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"79"},{"_position":{"x":3945.0,"y":3720.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"80"},{"statement":{"_text":"I won\u2019t resist, I\u2019m not stupid. Take me in, Cyrene.","_meta":"PrincePietro_BloodSun_01"},"_actorName":"name_unpc_princepietro_01 ","_actorParameterID":"a5650a7c-f79d-4dd6-9f4b-8006d524a90a","_position":{"x":3930.0,"y":2550.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"81"},{"statement":{"_text":"Gladly. For the record, I\u2019m sorry it turned out this way Pietro. You\u2019re not who I thought you were. As for you, [PlayerName], good work.","_meta":"Cyrene_BloodSun_15"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3945.0,"y":2655.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"83"},{"_condition":{"QuestEventRef":{"m_eventUID":"JWVoR5dHPEaz3k4tK80r4Q"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4215.0,"y":2820.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"85"},{"statement":{"_text":"My men will take him Pietro into custody, [PlayerName]. The Heroic Kingdom is in your debt. I will personally report your impact to King Simeon himself, and see to it that he rewards you adequately.","_meta":"Cyrene_BloodSun_05"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4050.0,"y":3120.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"88"},{"statement":{"_text":"You also killed Baron Montgomery, I hear. I won\u2019t ask for the details. As far as I\u2019m concerned, the number of corpses in that torture room more than merits his death. I\u2019ll report you killing him as self-defense and leave it at that. Good work.","_meta":"Cyrene_BloodSun_03"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4065.0,"y":3255.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"90"},{"_action":{"QuestEventRef":{"m_eventUID":"2WceKae4PEaCyh_s5sjuBQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4200.0,"y":3390.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"92"},{"_condition":{"QuestEventRef":{"m_eventUID":"lP_FmwjhqkCgnkq5wwK0sQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4260.0,"y":3525.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"95"},{"_position":{"x":4350.0,"y":4425.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"98"},{"_condition":{"QuestEventRef":{"m_eventUID":"JWVoR5dHPEaz3k4tK80r4Q"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4425.0,"y":3660.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"99"},{"_action":{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":6200130}},"Quantity":{"_value":1},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"},"_position":{"x":4410.0,"y":3750.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"102"},{"_action":{"QuestEventRef":{"m_eventUID":"aAckSkdWiU6UJA3f_UjRIw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4470.0,"y":3855.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"112"},{"_condition":{"QuestEventRef":{"m_eventUID":"lP_FmwjhqkCgnkq5wwK0sQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":4410.0,"y":3960.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"115"},{"_action":{"Area":{"_value":"Levant"},"SpawnPoint":{},"$type":"NodeCanvas.Tasks.Actions.AreaSwitchPlayers"},"_position":{"x":4485.0,"y":4245.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"118"},{"_action":{"QuestEventRef":{"m_eventUID":"lP_FmwjhqkCgnkq5wwK0sQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4545.0,"y":4065.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"122"},{"_action":{"SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":6200130}},"Quantity":{"_value":1},"TryToEquip":{}},{"Item":{"_value":{"m_itemID":6300030}},"Quantity":{"_value":1},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"},"_position":{"x":4620.0,"y":3765.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"125"},{"_condition":{"checkMode":"AnyTrueSuffice","conditions":[{"QuestEventRef":{"m_eventUID":"qJXEm-fAkU6O7h_D9K_AFg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"27L4G_KTM0WkNHQa4j4tTw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":4650.0,"y":2955.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"140"},{"statement":{"_text":"My men tell me we\u2019ve caught Baron Montgomery. He tried to make a quick getaway into the slums for supplies before fleeing Levant and basically ran right into our arms. We\u2019ll see to it that he gets what he deserves.","_meta":"Cyrene_BloodSun_01"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4545.0,"y":3135.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"147"},{"statement":{"_text":"My men will take him Pietro into custody, [PlayerName]. The Heroic Kingdom is in your debt. I will personally report your impact to King Simeon himself, and see to it that he rewards you adequately.","_meta":"Cyrene_BloodSun_05"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4545.0,"y":3300.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"149"},{"_targetNode":{"$ref":"92"},"_position":{"x":4950.0,"y":3540.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"151"},{"statement":{"_text":"My men will take him Pietro into custody, [PlayerName]. The Heroic Kingdom is in your debt. I will personally report your impact to King Simeon himself, and see to it that he rewards you adequately.","_meta":"Cyrene_BloodSun_05"},"_actorName":"name_unpc_cyrene_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5055.0,"y":3135.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"152"},{"_targetNode":{"$ref":"92"},"_position":{"x":5280.0,"y":3255.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"154"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"aAckSkdWiU6UJA3f_UjRIw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"lP_FmwjhqkCgnkq5wwK0sQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":5010.0,"y":3345.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"155"},{"$ref":"15"},{"availableChoices":[{"statement":{"_text":"Just like that?","_meta":"Player_PrincePietro_BloodSun_a_02"}}],"_position":{"x":5505.0,"y":3465.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"162"},{"statement":{"_text":"Do I look like I can stand a chance against the two of you, cornered down here by the army? I have a far better chance of survival if I throw myself on the mercy of my parents, even if I was planning to kill them both.","_meta":"PrincePietro_BloodSun_03"},"_actorName":"name_unpc_princepietro_01 ","_actorParameterID":"a5650a7c-f79d-4dd6-9f4b-8006d524a90a","_position":{"x":5430.0,"y":3585.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"166"},{"availableChoices":[{"statement":{"_text":"Why would you do this?","_meta":"Player_PrincePietro_BloodSun_a_04"}},{"statement":{"_text":"If you want to live, I hope you''re willing to rat out your followers.","_meta":"Player_PrincePietro_BloodSun_a_05"}},{"statement":{"_text":"You''re going to come with me and not make a fuss, you hear me?","_meta":"Player_PrincePietro_BloodSun_b_05"}}],"_position":{"x":5505.0,"y":3705.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"168"},{"statement":{"_text":"My parents have grown soft. Old Levant is an existential threat to us, and still they want to welcome their people to join us! And they still treat the Blue Chamber Collective like equals! We were not elected by simple humans, we were anointed kings and queens by a literal GOD!","_meta":"PrincePietro_BloodSun_04"},"_actorName":"name_unpc_princepietro_01 ","_actorParameterID":"a5650a7c-f79d-4dd6-9f4b-8006d524a90a","_position":{"x":5190.0,"y":3900.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"176"},{"statement":{"_text":"My brother was too nosy, he would have found out about my plot to kill Calixa and Simeon eventually. And I needed to be secure in the knowledge that I would be the only true heir to Levant''s throne. It was nothing personal.","_meta":"PrincePietro_BloodSun_05"},"_actorName":"name_unpc_princepietro_01 ","_actorParameterID":"a5650a7c-f79d-4dd6-9f4b-8006d524a90a","_position":{"x":5040.0,"y":4035.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"178"},{"availableChoices":[{"statement":{"_text":"If you want to live, I hope you''re willing to rat out your followers.","_meta":"Player_PrincePietro_BloodSun_a_05"}},{"statement":{"_text":"You''re going to come with me and not make a fuss, you hear me?","_meta":"Player_PrincePietro_BloodSun_b_05"}}],"_position":{"x":4845.0,"y":4290.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"180"},{"statement":{"_text":"Never. I won''t let those who pledged their loyalty to me die for nothing. It''s the least I can do for those who came this far for me. I may just be a murderer to you, but I have principles. I\u2019d rather die than betray them.","_meta":"PrincePietro_BloodSun_06"},"_actorName":"name_unpc_princepietro_01 ","_actorParameterID":"a5650a7c-f79d-4dd6-9f4b-8006d524a90a","_position":{"x":4620.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"186"},{"availableChoices":[{"statement":{"_text":"You''re going to come with me and not make a fuss, you hear me?","_meta":"Player_PrincePietro_BloodSun_b_05"}},{"statement":{"_text":"I hope they feed you to the assassin bugs out there.","_meta":"Player_PrincePietro_BloodSun_c_05"}},{"statement":{"_text":"You''re lucky you''re worth more alive. I''d kill you myself.","_meta":"Player_PrincePietro_BloodSun_d_05"}}],"_position":{"x":5280.0,"y":4725.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"188"},{"_targetNode":{"$ref":"81"},"_position":{"x":5460.0,"y":5115.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"196"},{"_targetNode":{"$ref":"81"},"_position":{"x":5085.0,"y":4515.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"197"},{"statement":{"_text":"Never. I won''t let those who pledged their loyalty to me die for nothing. It''s the least I can do for those who came this far for me. I may just be a murderer to you, but I have principles. I\u2019d rather die than betray them.","_meta":"PrincePietro_BloodSun_06"},"_actorName":"name_unpc_princepietro_01 ","_actorParameterID":"a5650a7c-f79d-4dd6-9f4b-8006d524a90a","_position":{"x":5505.0,"y":3960.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"198"},{"availableChoices":[{"statement":{"_text":"Why would you do this?","_meta":"Player_PrincePietro_BloodSun_a_04"}},{"statement":{"_text":"You''re going to come with me and not make a fuss, you hear me?","_meta":"Player_PrincePietro_BloodSun_b_05"}}],"_position":{"x":5670.0,"y":4125.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"200"},{"statement":{"_text":"My parents have grown soft. Old Levant is an existential threat to us, and still they want to welcome their people to join us! And they still treat the Blue Chamber Collective like equals! We were not elected by simple humans, we were anointed kings and queens by a literal GOD!","_meta":"PrincePietro_BloodSun_04"},"_actorName":"name_unpc_princepietro_01 ","_actorParameterID":"a5650a7c-f79d-4dd6-9f4b-8006d524a90a","_position":{"x":5430.0,"y":4245.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"206"},{"statement":{"_text":"My brother was too nosy, he would have found out about my plot to kill Calixa and Simeon eventually. And I needed to be secure in the knowledge that I would be the only true heir to Levant''s throne. It was nothing personal.","_meta":"PrincePietro_BloodSun_05"},"_actorName":"name_unpc_princepietro_01 ","_actorParameterID":"a5650a7c-f79d-4dd6-9f4b-8006d524a90a","_position":{"x":5295.0,"y":4380.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"208"},{"_targetNode":{"$ref":"81"},"_position":{"x":6030.0,"y":4305.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"210"},{"_targetNode":{"$ref":"81"},"_position":{"x":6060.0,"y":3975.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"211"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"8"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"7"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-1528.0,"y":-5019.0},"zoomFactor":0.7406262,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"Y5SZ7n2rWUe-XfOr6TGjgA"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3135.0,"y":4845.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_action":{"Sound":"LOC_EXCL_LichListen04","TransPos":{},"$type":"NodeCanvas.Tasks.Actions.PlaySound"},"_position":{"x":3015.0,"y":4980.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"5"},{"_condition":{"quest":{"_value":1},"$type":"NodeCanvas.Tasks.Conditions.Condition_KnowQuest"},"_position":{"x":3120.0,"y":5100.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"8"},{"_condition":{"QuestEventRef":{"m_eventUID":"bSBl8yk14kiLqrk-Koq2jg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3135.0,"y":5310.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"11"},{"statement":{"_text":"Not one step further, bearer of Corruption! Turn back or your life is forfeit! The Light Mender will not tolerate your presence here!","_meta":"HallowedMarsh_SQ_LightMenderLich_01"},"_actorName":"name_unpc_GoldLich_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2925.0,"y":5430.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"14"},{"availableChoices":[{"statement":{"_text":"Who are you?","_meta":"Player)HallowedMarsh_SQ_TsarDarkLich_a_07"}},{"statement":{"_text":"I\u2019ll go wherever I damn well please, voice!","_meta":"Player_HallowedMarsh_SQ_LightMenderLich_b_01"}}],"_position":{"x":3045.0,"y":5610.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"16"},{"statement":{"_text":"I am the Light Mender. I am not someone to be trifled with. I have seen centuries pass in my time as a lich, and I will not let one such as you sully this place with the Corruption in your very blood.","_meta":"HallowedMarsh_SQ_LightMenderLich_02"},"_actorName":"name_unpc_GoldLich_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3180.0,"y":5895.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"22"},{"availableChoices":[{"statement":{"_text":"I know when I\u2019m not wanted. I\u2019ll leave.","_meta":"Player_HallowedMarsh_SQ_LightMenderLich_a_11"}},{"statement":{"_text":"Corruption? I\u2019m just your average human explorer, what are you talking about?","_meta":"Player_HallowedMarsh_SQ_LightMenderLich_a_03"}},{"statement":{"_text":"I\u2019m with the Holy Mission, Elatt\u2019s light is within me. How could I possibly bear Corruption?","_meta":"Player_HallowedMarsh_SQ_LightMenderLich_b_03"},"condition":{"QuestEventRef":{"m_eventUID":"1tkTEWMPbEGKmWItwxy4kQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":3240.0,"y":5985.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"24"},{"_targetNode":{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":3105.0,"y":7455.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"35"},"_position":{"x":2940.0,"y":6255.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"34"},{"statement":{"_text":"Every human on this wretched world is a catalyst for the Corruption. So long as even one of your kind breathes, there can be no hope for preserving the world. You must all be transformed into something free of Corruption, or cleansed with light and thunder!","_meta":"HallowedMarsh_SQ_LightMenderLich_04"},"_actorName":"name_unpc_GoldLich_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3180.0,"y":6405.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"37"},{"availableChoices":[{"statement":{"_text":"So you\u2019re planning to destroy humanity, is that it?","_meta":"Player_HallowedMarsh_SQ_LightMenderLich_a_05"}},{"statement":{"_text":"Corruption is a natural part of the world, though, isn\u2019t it?","_meta":"Player_HallowedMarsh_SQ_LightMenderLich_b_05"},"condition":{"QuestEventRef":{"m_eventUID":"U9_8HqKDp0WhjklKmO4fag"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":3285.0,"y":6555.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"39"},{"statement":{"_text":"I would rather transform and uplift your species into something better, and will dedicate many more centuries to this plan, but should I have to choose between humanity and the existence of the world, I will choose the world. I don\u2019t expect you to understand.","_meta":"HallowedMarsh_SQ_LightMenderLich_10"},"_actorName":"name_unpc_GoldLich_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3195.0,"y":6915.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"47"},{"statement":{"_text":"Leave. Now. You have nothing to fear from me if you depart, my plans will take centuries more to bring to fruition. But if you interfere with me, I will show you no mercy.","_meta":"HallowedMarsh_SQ_LightMenderLich_11"},"_actorName":"name_unpc_GoldLich_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3195.0,"y":7065.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"49"},{"availableChoices":[{"statement":{"_text":"I know when I\u2019m not wanted. I\u2019ll leave.","_meta":"Player_HallowedMarsh_SQ_LightMenderLich_a_11"}},{"statement":{"_text":"Leave? And let you continue planning the destruction of my kind? I don\u2019t think so.","_meta":"Player_HallowedMarsh_SQ_LightMenderLich_b_11"}},{"statement":{"_text":"You do not scare me mr. inventor!"}}],"_position":{"x":3270.0,"y":7215.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"51"},{"$ref":"35"},{"_action":{"AllPlayers":{"_value":true},"TurnCamera":{"_value":true},"Target":{},"TargetTrans":{"_name":"Interactions_BB/LeaveGoldLich","_targetVariableID":"91a5951b-6e27-46c8-85cd-b0a181bca550"},"$type":"NodeCanvas.Tasks.Actions.WarpCharacter"},"_position":{"x":2835.0,"y":7560.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"59"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":3075.0,"y":7665.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"65"},{"_position":{"x":3045.0,"y":7800.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"67"},{"statement":{"_text":"You would fight me? Human foolishness is proven yet again! Raise your fist against me and you will be scoured clean of Corruption where you stand, no matter if you can survive or not! You have been warned!","_meta":"HallowedMarsh_SQ_LightMenderLich_14"},"_actorName":"name_unpc_GoldLich_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3375.0,"y":7425.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"68"},{"_position":{"x":3630.0,"y":7560.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"70"},{"statement":{"_text":"Have you been speaking to that damn Plague Doctor? She is a slave to it! She would cover the world in Corruption so she might feast endlessly! She is a fallen monster of the worst kind! Trust her words at the world\u2019s peril!","_meta":"HallowedMarsh_SQ_LightMenderLich_09"},"_actorName":"name_unpc_GoldLich_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3465.0,"y":6705.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"71"},{"statement":{"_text":"You\u2019re one of Elatt\u2019s minions? Don\u2019t make me laugh. I know the circumstances of his creation. To think his existence can be a balm against the Corruption is foolish in the extreme. He lacks the means and the heart to do what needs to be done, if he even wanted to.","_meta":"HallowedMarsh_SQ_LightMenderLich_05"},"_actorName":"name_unpc_GoldLich_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3510.0,"y":6255.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"73"},{"statement":{"_text":"Not if you value your life. Turn back now, or I shall blast your flesh and bone to ashes and purify you with thunder and light!","_meta":"HallowedMarsh_SQ_LightMenderLich_03"},"_actorName":"name_unpc_GoldLich_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3255.0,"y":5715.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"75"},{"statement":{"_text":"Not one step further, bearer of Corruption! Turn back or your life is forfeit! The Light Mender will not tolerate your presence here!","_meta":"HallowedMarsh_SQ_LightMenderLich_01"},"_actorName":"name_unpc_unknown_01","_actorParameterID":"98bb55e6-7e74-4616-be73-0c8a7a9b99e8","_position":{"x":3525.0,"y":5445.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"77"},{"availableChoices":[{"statement":{"_text":"Who are you?","_meta":"Player)HallowedMarsh_SQ_TsarDarkLich_a_07"}},{"statement":{"_text":"I\u2019ll go wherever I damn well please, voice!","_meta":"Player_HallowedMarsh_SQ_LightMenderLich_b_01"}}],"_position":{"x":3615.0,"y":5580.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"79"},{"_action":{"QuestEventRef":{"m_eventUID":"bSBl8yk14kiLqrk-Koq2jg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3555.0,"y":5820.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"85"},{"statement":{"_text":"Not if you value your life. Turn back now, or I shall blast your flesh and bone to ashes and purify you with thunder and light!","_meta":"HallowedMarsh_SQ_LightMenderLich_03"},"_actorName":"name_unpc_unknown_01","_actorParameterID":"98bb55e6-7e74-4616-be73-0c8a7a9b99e8","_position":{"x":3840.0,"y":5700.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"88"},{"_action":{"quest":{},"questRef":{"_value":{"m_itemID":7011623}},"$type":"NodeCanvas.Tasks.Actions.GiveQuest"},"_position":{"x":3225.0,"y":5220.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"90"},{"_position":{"x":3495.0,"y":5070.0},"$type":"NodeCanvas
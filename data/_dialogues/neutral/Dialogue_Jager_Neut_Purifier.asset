  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-3876.49,"y":-6363.482},"zoomFactor":0.9566516,"nodes":[{"_condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"q--_LPc6JEeCyRUjVQSgAA"},"MinStack":5,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"n_qdf8jRWkKYY83X4u3ltQ"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":4620.0,"y":6180.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"You look like you have something to say to me. Out with it.","_meta":"Jager_Purifier_01"},"_actorName":"name_unpc_jagersullivan_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":4410.0,"y":6285.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"9"},{"availableChoices":[{"statement":{"_text":"I know about your little experiment with the Immaculates in the Hallowed Marsh.","_meta":"Player_Jager_Purifier_a_01"}},{"statement":{"_text":"(Say nothing about the clues)"}}],"_position":{"x":4500.0,"y":6390.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"11"},{"statement":{"_text":"Pardon me? I have no idea what you''re talking about. I haven\u2019t been there in years.","_meta":"Jager_Purifier_02"},"_actorName":"Jager Sullivan","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":4410.0,"y":6510.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"17"},{"availableChoices":[{"statement":{"_text":"You were having mercenaries kidnap Holy Mission members so you could learn to control the instinct of Immaculates.","_meta":"Player_Jager_Purifier_a_02"}},{"statement":{"_text":"(Say nothing about the clues)"}}],"_position":{"x":4485.0,"y":6615.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"19"},{"statement":{"_text":"Even assuming you''re right, which you''re not, what would be wrong with that? Learning to make the Immaculates into useful pawns rather than barely sentient monsters would benefit everyone.","_meta":"Jager_Purifier_03"},"_actorName":"Jager Sullivan","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":4410.0,"y":6735.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"25"},{"availableChoices":[{"statement":{"_text":"Let''s see if the Chamberlords think of this the same way. I think they''ll find the evidence more than convincing enough to arrest you.","_meta":"Player_Jager_Purifier_a_03"}},{"statement":{"_text":"(Say nothing about the clues)"}}],"_position":{"x":4470.0,"y":6840.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"27"},{"statement":{"_text":"Dammit, I told those idiots to burn anything that could be used as evidence! Bah, you''ve got me by the stones, [PlayerName]. How much can I buy your silence for? Give me the evidence and I''ll give you 75 silver and a piece of Hackmanite.","_meta":"Jager_Purifier_04"},"_actorName":"name_unpc_jagersullivan_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":4395.0,"y":6945.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"33"},{"availableChoices":[{"statement":{"_text":"I''ll think about it.","_meta":"Player_Jager_Purifier_a_04"}},{"statement":{"_text":"Deal. Take the evidence.","_meta":"Player_Jager_Purifier_b_04"}},{"statement":{"_text":"I want you to turn yourself in to Elatt\u2019s judgment. Many of our people died because of you.","_meta":"Player_Jager_Purifier_c_04"},"condition":{"QuestEventRef":{"m_eventUID":"1tkTEWMPbEGKmWItwxy4kQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":4425.0,"y":7080.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"35"},{"statement":{"_text":"This is the best deal that anyone can give you, pal, I guarantee it. Don\u2019t be a fool.","_meta":"Jager_Purifier_05"},"_actorName":"name_unpc_jagersullivan_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":3900.0,"y":7260.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"45"},{"_position":{"x":3975.0,"y":7380.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"47"},{"statement":{"_text":"Smart move. We won''t meet again, I assure you.","_meta":"Jager_Purifier_06"},"_actorName":"name_unpc_jagersullivan_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":4380.0,"y":7290.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"48"},{"statement":{"_text":"Smart move. We won''t meet again, I assure you.","_meta":"Jager_Purifier_06"},"_actorName":"name_unpc_jagersullivan_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":4440.0,"y":7425.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"50"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":4500.0,"y":7530.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"52"},{"_action":{"QuestEventRef":{"m_eventUID":"qRludSuL80abynZ8MjOVkg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4425.0,"y":7650.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"54"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"n_qdf8jRWkKYY83X4u3ltQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"_YpTpTGglUKzg5VP7octtg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"SilverAmount":{"_value":75},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":6200130}},"Quantity":{"_value":2},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4395.0,"y":7755.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"57"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":4395.0,"y":7905.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"73"},{"_position":{"x":4455.0,"y":8025.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"75"},{"statement":{"_text":"Not a snowball\u2019s chance in summer, pal. 100 silver and the piece of Hackmanite, and that\u2019s my final offer.","_meta":"Jager_Purifier_09"},"_actorName":"name_unpc_jagersullivan_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":4725.0,"y":7305.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"76"},{"availableChoices":[{"statement":{"_text":"I''ll think about it.","_meta":"Player_Jager_Purifier_a_04"}},{"statement":{"_text":"Deal. Take the evidence.","_meta":"Player_Jager_Purifier_b_04"}}],"_position":{"x":4800.0,"y":7410.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"78"},{"_targetNode":{"$ref":"45"},"_position":{"x":4740.0,"y":7575.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"84"},{"statement":{"_text":"Smart move. We won''t meet again, I assure you.","_meta":"Jager_Purifier_06"},"_actorName":"name_unpc_jagersullivan_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":4950.0,"y":7560.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"85"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":5070.0,"y":7650.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"87"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"n_qdf8jRWkKYY83X4u3ltQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"_YpTpTGglUKzg5VP7octtg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"SilverAmount":{"_value":100},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":6200130}},"Quantity":{"_value":3},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"},{"QuestEventRef":{"m_eventUID":"qRludSuL80abynZ8MjOVkg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":5040.0,"y":7755.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"89"},{"_action":{"
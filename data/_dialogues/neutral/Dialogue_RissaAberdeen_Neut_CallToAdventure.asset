  m_Name: Dialogue_RissaAberdeen_Neut_CallToAdventure
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-2521.0,"y":-2234.0},"zoomFactor":0.7500055,"nodes":[{"_condition":{"quest":{"_value":1},"$type":"NodeCanvas.Tasks.Conditions.Condition_KnowQuest"},"_position":{"x":5610.0,"y":1635.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_condition":{"checkMode":"AnyTrueSuffice","conditions":[{"QuestEventRef":{"m_eventUID":"n_3w-BcFfEW52Ht4Q3ZCjw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"QuestEventRef":{"m_eventUID":"-Ku9dHjTl0KeUPxWk0ZWWQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":5520.0,"y":1950.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"5"},{"_condition":{"QuestEventRef":{"m_eventUID":"zoKR1_bqiUiAetJ3uxw-Ug"},"ExpiryTime":120,"$type":"NodeCanvas.Tasks.Conditions.Condition_CheckQuestEventExpiry"},"_position":{"x":5085.0,"y":2085.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"12"},{"_action":{"Stop":{"_value":true},"Music":{},"$type":"NodeCanvas.Tasks.Actions.StartMusicEvent"},"_position":{"x":4815.0,"y":2235.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"15"},{"_action":{"Stop":{},"Music":{"_value":"BGM_EventDramatic"},"$type":"NodeCanvas.Tasks.Actions.StartMusicEvent"},"_position":{"x":4725.0,"y":2325.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"19"},{"statement":{"_text":"[PlayerName], I''m afraid that you came <color=#00CCFF><b>too late</b></color>. The mourning period has ended. <color=#00CCFF><b>The Lighthouse has been sold</b></color> and the proceeds have already been distributed to the families of the drowned sailors.","_meta":"Rissa_NE_Call_To_A_45"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4290.0,"y":2475.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"23"},{"availableChoices":[{"statement":{"_text":"Come on! I worked like a dog to earn that money! This isn''t fair!","_meta":"Player_Rissa_NE_Call_To_A_a_45"}},{"statement":{"_text":"Well, I tried.","_meta":"Player_Rissa_NE_Call_To_A_b_45"}}],"_position":{"x":4255.0,"y":2673.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"25"},{"statement":{"_text":"If you cannot succeed at a task, then you must accept the price of failure. This is one of life''s most important lessons.","_meta":"Rissa_NE_Call_To_A_46"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3885.0,"y":2925.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"31"},{"availableChoices":[{"statement":{"_text":"Lesson learned, then. I need to be more efficient...","_meta":"Player_Rissa_NE_Call_To_A_a_46"}},{"statement":{"_text":"Great, I''m homeless now. Thanks for nothing as usual, Rissa","_meta":"Player_Rissa_NE_Call_To_A_b_46"}}],"_position":{"x":4140.0,"y":3105.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"33"},{"statement":{"_text":"Indeed. The world will not wait for you. Either move at Aurai''s pace, or you will be left behind.","_meta":"Rissa_NE_Call_To_A_48"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3705.0,"y":3255.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"39"},{"_action":{"QuestEventRef":{"m_eventUID":"sAc2Dj-T_kysKXFV48Hp0A"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":3990.0,"y":3480.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"41"},{"_action":{"QuestEventRef":{"m_eventUID":"ZYzrMi1skUiJ4BgXXQ3sfw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4247.0,"y":3798.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"44"},{"_action":{"dialogueStarter":{"_name":"Dialogue_RissaAberdeen_BB/Prequest","_targetVariableID":"f484d34a-274e-4f4e-a340-3bcd80884110"},"$type":"NodeCanvas.Tasks.Actions.BranchDialogue"},"_position":{"x":4203.0,"y":3951.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"47"},{"statement":{"_text":"Bitterness won''t do you any good. The world will not wait for you. Besides, you learned how to survive on your own, like a bird pushed from its nest.","_meta":"Rissa_NE_Call_To_A_49"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4140.0,"y":3255.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"50"},{"statement":{"_text":"That you did, [PlayerName]. Even though the payment was late, you''ve proven you can survive in Aurai.","_meta":"Rissa_NE_Call_To_A_47"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4350.0,"y":2940.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"52"},{"statement":{"_text":"[PlayerName], how go your efforts?","_meta":"Rissa_NE_Call_To_A_22"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5265.0,"y":2295.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"54"},{"availableChoices":[{"statement":{"_text":"I want to talk about my Blood Price.","_meta":"Player_Rissa_NE_Call_To_A_a_22"},"condition":{"checkMode":"AnyTrueSuffice","conditions":[{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":150},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"},{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":5600050}},"minAmount":{"_value":1},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}],"$type":"NodeCanvas.Framework.ConditionList"}},{"statement":{"_text":"Who would be able to help me?","_meta":"Player_Rissa_NE_Call_To_A_b_22"}},{"statement":{"_text":"I have questions about our tribe.","_meta":"Player_Rissa_NE_Call_To_A_c_22"}},{"statement":{"_text":"I\u2019m on my way, farewell, Lady Aberdeen.","_meta":"Player_Rissa_NE_Call_To_A_a_37"}}],"_position":{"x":5205.0,"y":2505.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"56"},{"statement":{"_text":"You have a way to make that payment, [PlayerName]? Show me.","_meta":"Rissa_NE_Call_To_A_42"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4725.0,"y":2760.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"80"},{"availableChoices":[{"statement":{"_text":"I have 150 silvers right here. Here you go.","_meta":"Rissa_NE_Call_To_A_43"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":150},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"I earned a Tribal Favor. Here\u2019s the proof.","_meta":"Rissa_NE_Call_To_A_44"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":5600050}},"minAmount":{"_value":1},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}}],"_position":{"x":4860.0,"y":2970.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"82"},{"statement":{"_text":"And on time, too! I''m impressed, [PlayerName]. You''ve proven you can be an excellent asset to the tribe when you put your mind to it.","_meta":"Rissa_NE_Call_To_A_39"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4560.0,"y":3240.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"100"},{"statement":{"_text":"I''ll see that this money gets to the families that need it most. Thank you for your efforts.","_meta":"Rissa_NE_Call_To_A_40"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4545.0,"y":3390.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"102"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"8GvHUbDz90OOJWurd-RZlg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":150}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":4545.0,"y":3525.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"104"},{"statement":{"_text":"And on time, too! I''m impressed, [PlayerName]. You''ve proven you can be an excellent asset to the tribe when you put your mind to it.","_meta":"Rissa_NE_Call_To_A_39"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4950.0,"y":3330.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"116"},{"statement":{"_text":"Well done. Consider this payment dealt with. Cierzo thanks you for your great deed.","_meta":"Rissa_NE_Call_To_A_41"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4965.0,"y":3435.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"118"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"8GvHUbDz90OOJWurd-RZlg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":5600050}}],"Amount":[{"_value":1}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":5010.0,"y":3555.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"120"},{"statement":{"_text":"Eto is our best warrior, he should be able to tutor you about combat and self-defense. Burac, the gate warden, can teach you about surviving out in the wild. I\u2019m sure various other shopkeepers can teach you about things within their own field of expertise as well.","_meta":"Rissa_NE_Call_To_A_20"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5145.0,"y":2865.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"132"},{"statement":{"_text":"If you\u2019re looking for news, the Inn is always a good place to try. You can also speak with Yzan down by the beach, he may be able to help you as well.","_meta":"Rissa_NE_Call_To_A_23"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5190.0,"y":2970.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"134"},{"_position":{"x":5250.0,"y":3105.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"136"},{"_targetNode":{"statement":{"_text":"What do you wish to know?","_meta":"Rissa_NE_Call_To_A_27"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":7950.0,"y":2355.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"138"},"_position":{"x":5385.0,"y":2745.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"137"},{"statement":{"_text":"Ancestors guide you, [PlayerName]. Be safe. Be smart.","_meta":"Rissa_NE_Call_To_A_38"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5505.0,"y":2685.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"140"},{"_position":{"x":5670.0,"y":2865.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"142"},{"statement":{"_text":"I''m sorry that was the first thing you woke up to, after what you\u2019ve been through, [PlayerName]. Please, forgive them. Many of them have lost family and are not thinking straight.","_meta":"Rissa_NE_Call_To_A_01"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5790.0,"y":2085.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"143"},{"availableChoices":[{"statement":{"_text":"I can''t believe they want to seize my home! Can''t you do something about them?","_meta":"Player_Rissa_NE_Call_To_A_a_01"}}],"_position":{"x":5865.0,"y":2220.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"145"},{"statement":{"_text":"If I could do more, I would. But if you do not pay off those 4 months of missed payments on your own, the tribe will have my head. The most I could do for you is declare a mourning period and buy you a little time. For 5 days, no one in the tribe can ask for what you owe.","_meta":"Rissa_NE_Call_To_A_02"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5775.0,"y":2340.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"149"},{"statement":{"_text":"That gives you<color=#00CCFF><b>5 days to pay back all 150 silver coins</b></color>. This should be more than enough time for you to do what needs to be done. Either gather the money, or earn a <color=#00CCFF><b>Tribal Favor</b></color>, and they won''t be able to do a thing to you.","_meta":"Rissa_NE_Call_To_A_03"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5880.0,"y":2670.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"151"},{"statement":{"_text":"Once you can make the payment, come see me up at my house on top of the hill. Please, find a way to make it work, [PlayerName]. The last thing I want to do is take your home away.","_meta":"Rissa_NE_Call_To_A_04"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5895.0,"y":2850.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"153"},{"availableChoices":[{"statement":{"_text":"I can''t possibly accomplish that! I need more time!","_meta":"Player_Rissa_NE_Call_To_A_b_05"},"condition":{"QuestEventRef":{"m_eventUID":"nt9KhXoJtkOalZ-wtfueDA"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"This Blood Price is unfair! You can''t hold me responsible for things that are out of my control!","_meta":"Player_Rissa_NE_Call_To_A_c_05"},"condition":{"QuestEventRef":{"m_eventUID":"YQD53MKgwke6juWiSWI7jQ"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"Then I will have to head outside the walls.","_meta":"Player_Rissa_NE_Call_To_A_d_05"}}],"_position":{"x":5985.0,"y":3060.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"155"},{"statement":{"_text":"A dozen people lost their lives in that shipwreck, leaving grieving kin behind. Their families need the assistance immediately. Their need outweighs yours. Trust me, [PlayerName], I wouldn\u2019t be doing this to you otherwise.","_meta":"Rissa_NE_Call_To_A_09"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5610.0,"y":3315.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"167"},{"statement":{"_text":"You must find a way to pay what is owed to the tribe. The last thing I want to do is take your home away. ","_meta":"Rissa_NE_Call_To_A_10"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5550.0,"y":3435.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"169"},{"_action":{"QuestEventRef":{"m_eventUID":"nt9KhXoJtkOalZ-wtfueDA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5610.0,"y":3570.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"171"},{"_targetNode":{"$ref":"155"},"_position":{"x":5595.0,"y":3705.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"174"},{"statement":{"_text":"Have you forgotten why your family bears this Blood Price? When your grandmother was Chamberlady, she turned away the followers of Elatt in our time of greatest need, forcing the tribe to pray only to the Five Winds for protection.","_meta":"Rissa_NE_Call_To_A_11"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6015.0,"y":3375.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"175"},{"statement":{"_text":"Because of her pride, dozens were slain by the beasts of the Scourge. They paid for her foolishness with their lives. You owe it to them to pay back that debt to all those whose blood was lost in that tragedy.","_meta":"Rissa_NE_Call_To_A_12"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6000.0,"y":3525.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"177"},{"availableChoices":[{"statement":{"_text":"I understand. Bitterness still runs very deep in the tribe.","_meta":"Player_Rissa_NE_Call_To_A_a_12"}},{"statement":{"_text":"How could I possibly forget? Not a day goes by without someone reminding me!","_meta":"Player_Rissa_NE_Call_To_A_b_12"}},{"statement":{"_text":"I can''t bear this stupid Blood Price any longer. What if I just leave and never come back?","_meta":"Player_Rissa_NE_Call_To_A_c_12"}}],"_position":{"x":6090.0,"y":3645.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"179"},{"statement":{"_text":"If you understand, then get to work making that payment, [PlayerName].","_meta":"Rissa_NE_Call_To_A_13"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":5715.0,"y":3855.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"187"},{"_action":{"QuestEventRef":{"m_eventUID":"YQD53MKgwke6juWiSWI7jQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":6135.0,"y":4140.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"189"},{"_targetNode":{"$ref":"155"},"_position":{"x":6225.0,"y":4305.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"192"},{"statement":{"_text":"You live and breathe thanks to the deeds of your ancestors. You cannot embrace such a gift if you cannot also bear the costs of their actions. Without them, you would be nothing.","_meta":"Rissa_NE_Call_To_A_14"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6045.0,"y":3930.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"193"},{"statement":{"_text":"If you think you can survive alone, out there in the wild, you have that right. But remember, your tribe is your family. Casting yourself out is a fool''s choice. We can achieve more together than we can on our own.","_meta":"Rissa_NE_Call_To_A_15"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6465.0,"y":3870.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"195"},{"statement":{"_text":"That does seem like your most effective option, dangerous as it is. I doubt you''d be able to gather that much in such a short time just by staying within Cierzo.","_meta":"Rissa_NE_Call_To_A_16"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6480.0,"y":3300.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"197"},{"statement":{"_text":"Never forget, no one is an orphan in Cierzo. Though you owe us a Blood Price, we will still help you however we can. Do not hesitate to ask the rest of the tribe for advice on surviving the outside world.","_meta":"Rissa_NE_Call_To_A_17"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6630.0,"y":3435.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"199"},{"statement":{"_text":"If you can do a great deed for someone in Cierzo, we will owe you a <color=#00CCFF><b>Tribal Favor</b></color>, which I will accept as a substitute for your payment.","_meta":"Rissa_NE_Call_To_A_21"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6945.0,"y":3585.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"201"},{"statement":{"_text":"Once you''re ready to leave Cierzo, speak with Burac, the Gate Warden. He will judge whether or not you are prepared to face the outside world.","_meta":"Rissa_NE_Call_To_A_18"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6990.0,"y":3705.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"203"},{"statement":{"_text":"Remember, [PlayerName], you only have <color=#00CCFF><b>5 days</b></color> to pay me the 150 silver, or show me proof you have earned a <color=#00CCFF><b>Tribal Favor</b></color> . May your ancestors watch over you.","_meta":"Rissa_NE_Call_To_A_19"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":7095.0,"y":3840.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"205"},{"_action":{"QuestEventRef":{"m_eventUID":"n_3w-BcFfEW52Ht4Q3ZCjw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":7215.0,"y":3975.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"207"},{"_position":{"x":7335.0,"y":4110.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"210"},{"_action":{"actions":[{"quest":{},"questRef":{"_value":{"m_itemID":7011001}},"$type":"NodeCanvas.Tasks.Actions.GiveQuest"},{"QuestEventRef":{"m_eventUID":"zoKR1_bqiUiAetJ3uxw-Ug"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":5775.0,"y":1770.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"211"},{"$ref":"138"},{"availableChoices":[{"statement":{"_text":"What are your duties as Chamberlady? I never have paid much mind to politics.","_meta":"Player_Rissa_NE_Call_To_A_a_27"}},{"statement":{"_text":"You\u2019re always trying to look after me. Why?","_meta":"Player_Rissa_NE_Call_To_A_b_27"}},{"statement":{"_text":"Enough talk about the past","_meta":"Player_Rissa_NE_Call_To_A_c_27"}},{"statement":{"_text":"This Blood Price is unfair! You can''t hold me responsible for things that are out of my control!","_meta":"Player_Rissa_NE_Call_To_A_c_05"},"condition":{"QuestEventRef":{"m_eventUID":"YQD53MKgwke6juWiSWI7jQ"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":7920.0,"y":2490.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"220"},{"statement":{"_text":"I represent us within the Blue Chamber Collective, whenever the tribes need to make a decision that affects us all. Each of us bears one vote on the matters at hand.","_meta":"Rissa_NE_Call_To_A_28"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":7305.0,"y":2730.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"232"},{"statement":{"_text":"Should a Chamberlord call a meeting, we gather in the city of Berg, the birthplace of our Collective, and hold a discussion, then a vote.","_meta":"Rissa_NE_Call_To_A_29"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":7260.0,"y":2835.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"234"},{"statement":{"_text":"As such, I wield much power within the town of Cierzo, but only so long as my decisions are accepted. After all, your grandmother was Chamberlady once, before she was exiled for her actions. No one in the Collective is above the law.","_meta":"Rissa_NE_Call_To_A_30"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":7245.0,"y":2940.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"236"},{"_targetNode":{"$ref":"220"},"_position":{"x":7380.0,"y":3105.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"238"},{"statement":{"_text":"Your Father and I were close, as children. I saw a future for us together. But he could not stand by as the woman who would be your mother bore the weight of your grandmother''s sins alone. There\u2019s only so much a single huntress can manage.","_meta":"Rissa_NE_Call_To_A_31"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":7890.0,"y":2745.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"239"},{"statement":{"_text":"They turned out to be a natural team as they worked to redeem your bloodline. They were better suited for each other than I ever would have been for him. Despite it all, we remained close friends.","_meta":"Rissa_NE_Call_To_A_32"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":7890.0,"y":2850.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"241"},{"statement":{"_text":"You may not be connected to me by blood or tradition, but I still consider you family, [PlayerName]. I don\u2019t want to see you suffer.","_meta":"Rissa_NE_Call_To_A_33"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":7935.0,"y":2970.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"243"},{"availableChoices":[{"statement":{"_text":"Enough talk about the past","_meta":"Player_Rissa_NE_Call_To_A_c_27"}},{"statement":{"_text":"Enough talk about the past","_meta":"Player_Rissa_NE_Call_To_A_c_27"}}],"_position":{"x":8010.0,"y":3165.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"245"},{"_targetNode":{"$ref":"56"},"_position":{"x":8055.0,"y":3315.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"251"},{"statement":{"_text":"Ancestors guide you, [PlayerName]. Be safe. Be smart.","_meta":"Rissa_NE_Call_To_A_38"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":8325.0,"y":2805.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"252"},{"_position":{"x":8460.0,"y":2940.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"254"},{"statement":{"_text":"Have you forgotten why your family bears this Blood Price? When your grandmother was Chamberlady, she turned away the followers of Elatt in our time of greatest need, forcing the tribe to pray only to the Five Winds for protection.","_meta":"Rissa_NE_Call_To_A_11"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":8685.0,"y":2730.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"255"},{"statement":{"_text":"Because of her pride, dozens were slain by the beasts of the Scourge. They paid for her foolishness with their lives. You owe it to them to pay back that debt to all those whose blood was lost in that tragedy.","_meta":"Rissa_NE_Call_To_A_12"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":8685.0,"y":2850.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"257"},{"availableChoices":[{"statement":{"_text":"I can''t possibly accomplish that! I need more time!","_meta":"Player_Rissa_NE_Call_To_A_b_05"}},{"statement":{"_text":"This Blood Price is unfair! You can''t hold me responsible for things that are out of my control!","_meta":"Player_Rissa_NE_Call_To_A_c_05"},"condition":{"QuestEventRef":{"m_eventUID":"YQD53MKgwke6juWiSWI7jQ"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}},{"statement":{"_text":"Then I will have to head outside the walls.","_meta":"Player_Rissa_NE_Call_To_A_d_05"}}],"_position":{"x":8970.0,"y":2955.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"259"},{"statement":{"_text":"If you understand, then get to work making that payment, [PlayerName].","_meta":"Rissa_NE_Call_To_A_13"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":8565.0,"y":3150.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"269"},{"_action":{"QuestEventRef":{"m_eventUID":"YQD53MKgwke6juWiSWI7jQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":8895.0,"y":3285.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"271"},{"_targetNode":{"$ref":"220"},"_position":{"x":8955.0,"y":3390.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"274"},{"statement":{"_text":"You live and breathe thanks to the deeds of your ancestors. You cannot embrace such a gift if you cannot also bear the costs of their actions. Without them, you would be nothing.","_meta":"Rissa_NE_Call_To_A_14"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":9000.0,"y":3150.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"275"},{"statement":{"_text":"If you think you can survive alone, out there in the wild, you have that right. But remember, your tribe is your family. Casting yourself out is a fool''s choice. We can achieve more together than we can on our own.","_meta":"Rissa_NE_Call_To_A_15"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":9435.0,"y":3150.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"277"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"211"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"12"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"143"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sour
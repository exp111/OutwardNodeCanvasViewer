  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-3604.0,"y":-4399.0},"nodes":[{"statement":{"_text":"Elatt''s breath, this is horrendous news. I will see to it that those responsible are brought before the Chamberlords and judged to the full extent of the law.","_meta":"Rissa_Purifier_01"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3765.0,"y":4050.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"2"},{"statement":{"_text":"Give me the proof, and the Blue Chamber will owe you a favor on behalf of all the tribes for this, as well as a 25 Silver bounty for your hard work.","_meta":"Rissa_Purifier_02"},"_actorName":"name_unpc_rissaaberdeen_01","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3720.0,"y":4185.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"4"},{"availableChoices":[{"statement":{"_text":"Let me think more about this.","_meta":"Player_Cyrill_Purifier_a_04"}},{"statement":{"_text":"Deal. Take the evidence.","_meta":"Player_Jager_Purifier_b_04"}}],"_position":{"x":3810.0,"y":4335.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"6"},{"statement":{"_text":"I see. If you think someone else is better suited to handle this investigation, then I have no objection. As long as justice is meted out, that is all that matters to me.","_meta":"Rissa_Purifier_03"},"_actorName":"Rissa Aberdeen","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3600.0,"y":4485.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"12"},{"_position":{"x":3645.0,"y":4650.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"14"},{"statement":{"_text":"Thank you, [PlayerName]. We will see to it that this never happens again. Everyone in the Collective owes you a debt of gratitude, [PlayerName]. We will not forget this.","_meta":"Rissa_Purifier_04"},"_actorNam
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-3265.0,"y":-3568.0},"zoomFactor":0.9999868,"nodes":[{"statement":{"_text":"Oh. [PlayerName]. What brings you here? How''s my brother doing?","_meta":"Roland_NE_Call_To_Advent_01"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3645.0,"y":3585.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"2"},{"availableChoices":[{"statement":{"_text":"Well, Yzan''s alive. That''s more than most of the other people on our expedition can say.","_meta":"Player_Roland_NE_Call_To_Advent_a_01"}}],"_position":{"x":3735.0,"y":3705.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"4"},{"statement":{"_text":"Y-yeah, shame about the the shipwreck. Accidents like this happen all the time.","_meta":"Roland_NE_Call_To_Advent_02"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3690.0,"y":3825.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"8"},{"availableChoices":[{"statement":{"_text":"Not if the Lighthouse is lit. Who was on duty that night?","_meta":"Player_Roland_NE_Call_To_Advent_a_02"}},{"statement":{"_text":"Well, the bay is pretty dangerous. If it keeps the pirates away, then I guess it''s dangerous to our ships too.","_meta":"Player_Roland_NE_Call_To_Advent_b_02"}}],"_position":{"x":3750.0,"y":3990.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"10"},{"statement":{"_text":"Not a clue. I\u2019m not the one organizing things around here. Wasn\u2019t me though, that\u2019s for sure.","_meta":"Roland_NE_Call_To_Advent_03"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3375.0,"y":4140.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"16"},{"availableChoices":[{"statement":{"_text":"Shouldn''t you be helping your brother look for salvage from the wreck?","_meta":"Player_Roland_NE_Call_To_Advent_a_04"}}],"_position":{"x":3765.0,"y":4275.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"18"},{"statement":{"_text":"I don''t think he wants my help. He''s always said I''m useless, stupid and clumsy. I''d probably just get in the way if I try to help. Ugh, this is the worst day I''ve had in forever.","_meta":"Roland_NE_Call_To_Advent_05"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3705.0,"y":4410.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"22"},{"_position":{"x":3870.0,"y":4530.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"24"},{"statement":{"_text":"Exactly! If my brother had a smaller ship, or you guys hadn''t picked up so many Kazite rugs or whatever else in Orochi, I guarantee that you wouldn''t have had any trouble coasting in!","_meta":"Roland_NE_Call_To_Advent_04"},"_actorName":"name_unpc_rolandargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4050.0,"y":4140.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"25"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"4"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"4"},"_targetNode":{"$ref":"8"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"8"},"_targetNode":{"$ref":"10"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"10"},"_targetNode":{"$ref":"16"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"10"},"_targetNode":{"$ref":"25"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"16"},"_targetNode":{"$ref":"18"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"18"},"_targetNode":{"$ref":"22"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"22"},"_targetNode":{"$ref":"24"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"25"},"_targetNode":{"$ref":"18"},"$type":"NodeCanvas.DialogueTrees.DTConnection"}],"localBlackboard":{"_name":"Local Blackboard","_variables":{"gInstigator":{"_name":"gInstigator","_id":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b","$type":"NodeCanvas.Framework.Variable`1[[Character, Assembly-CSharp, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null]]"}}},"derivedData":{"actorParameters":[{"_keyName":"name_unpc_rolandargenson_01 ","_id":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_actorObject":1}],"$type":"NodeCanvas.DialogueTrees.DialogueTree+DerivedSerializationData"}}'

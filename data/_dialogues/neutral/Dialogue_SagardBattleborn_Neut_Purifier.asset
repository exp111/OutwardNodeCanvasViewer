  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-4976.0,"y":-6206.0},"zoomFactor":0.8258255,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"q--_LPc6JEeCyRUjVQSgAA"},"MinStack":3,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":6090.0,"y":6330.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"statement":{"_text":"Well, well, well. Look at that. I''ll handle this investigation personally and see that justice comes down hard on everyone involved. They need to be made an example of!","_meta":"Sagard_Purifier_02"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":5865.0,"y":6480.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"5"},{"statement":{"_text":"Hand me the evidence and I\u2019ll give you 50 Silver and my sincere assurance that the perpetrator will pay dearly for this. This could be a great political gift. I\u2019d seriously owe you for this.","_meta":"Sagard_Purifier_03"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":5835.0,"y":6615.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"7"},{"availableChoices":[{"statement":{"_text":"Let me think more about this.","_meta":"Player_Cyrill_Purifier_a_04"}},{"statement":{"_text":"Deal. Take the evidence.","_meta":"Player_Jager_Purifier_b_04"}},{"statement":{"_text":"I want to train with you.","_meta":"Merchant_Generic_Train_01"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"67059ca8-8b6e-4cb0-a0fb-bd765e41e725"},"skill":{"_value":1},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_KnowSkill"}}],"_position":{"x":5940.0,"y":6720.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"9"},{"statement":{"_text":"I don\u2019t know who you think will get the job done better than me, but alright. As long as justice is done here, I\u2019m satisfied.","_meta":"Sagard_Purifier_04"},"_actorName":"SagardBattleborn","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":5625.0,"y":6915.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"20"},{"_position":{"x":5685.0,"y":7080.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"22"},{"_condition":{"QuestEventRef":{"m_eventUID":"1tkTEWMPbEGKmWItwxy4kQ"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":6015.0,"y":7005.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"23"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"LtINGWIMYkupqbFRXkW-DQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"jm1rWjLBL0WcOYhEgGpyug"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"n_qdf8jRWkKYY83X4u3ltQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":6105.0,"y":7200.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"26"},{"_action":{"SilverAmount":{"_value":50},"XpAmount":{},"ItemReward":[],"$type":"NodeCanvas.Tasks.Actions.GiveReward"},"_position":{"x":6135.0,"y":7350.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"35"},{"statement":{"_text":"Perfect. I\u2019m going to enjoy bringing down the hammer on Sorobor for this. I owe you one, [PlayerName], and Sagard Battleborn always honors his debts","_meta":"Sagard_Purifier_05"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":5985.0,"y":7455.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"40"},{"_position":{"x":6105.0,"y":7605.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"42"},{"_action":{"QuestEventRef":{"m_eventUID":"qRludSuL80abynZ8MjOVkg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":6270.0,"y":7095.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"43"},{"_targetNode":{"statement":{"_text":"If you pay me, sure, I\u2019ll give you a private lesson. You wanna know the most effective way to finish off a opponent with a 2-handed axe once you\u2019ve got them on the floor? Pay my tab for tonight, about 50 silver, and I\u2019ll teach you.","_meta":"Trainer_Micro_Finisher_01"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6780.0,"y":7155.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"47"},"_position":{"x":6450.0,"y":6930.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"46"},{"statement":{"_text":"Unless you have big news or we\u2019re under attack or you\u2019re here to drink with me, I don\u2019t think I have much to say to you. Free time is hard to come by for a Chamberlord of such a big city and intend to enjoy every moment I can snatch.","_meta":"Sagard_BC_Prequest_07"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"1b375fde-5a79-4f43-b8bb-a7a640f84a86","_position":{"x":6405.0,"y":6495.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"49"},{"_position":{"x":6465.0,"y":6660.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"51"},{"$ref":"47"},{"availableChoices":[{"statement":{"_text":"Deal, here\u2019s your money.","_meta":"Merchant_Generic_Pay_01"},"condition":{"character":{"_name":"gInstigator","_targetVariableID":"67059ca8-8b6e-4cb0-a0fb-bd765e41e725"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":50},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"}},{"statement":{"_text":"No, thanks.","_meta":"Merchant_Generic_No_02"}}],"_position":{"x":6810.0,"y":7290.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"52"},{"statement":{"_text":"Alright, come with me. We\u2019re going to go practice this on a couple of old logs. Here, I\u2019ll even let you use my-","_meta":"Trainer_Micro_Finisher_02"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6555.0,"y":7470.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"64"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":6675.0,"y":7590.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"66"},{"_action":{"actions":[{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"67059ca8-8b6e-4cb0-a0fb-bd765e41e725"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":50}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"},{"RewardReceiver":"Instigator","SilverAmount":{},"XpAmount":{},"ItemReward":[{"Item":{"_value":{"m_itemID":8100300}},"Quantity":{"_value":1},"TryToEquip":{}}],"$type":"NodeCanvas.Tasks.Actions.GiveReward"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":6675.0,"y":7695.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"68"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":6705.0,"y":7815.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"87"},{"statement":{"_text":"Hah, there you go, you\u2019ve gotten the hang of it! Remember, while you turn for extra momentum, you should step forward and put your full weight into it!","_meta":"Trainer_Micro_Finisher_03"},"_actorName":"name_unpc_sagardbattleborn_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":6630.0,"y":7905.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"89"},{"_position":{"x":6795.0,"y":8040.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"91"},{"_position":{"x":7110.0,"y":7485.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"92"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"49"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"7"},"$type":"NodeCanvas.D
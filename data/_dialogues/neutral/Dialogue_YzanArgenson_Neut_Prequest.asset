  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-1099.0,"y":-3714.0},"zoomFactor":0.8896035,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"seEIFfM9SkeZxc4CpR40oQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3795.0,"y":3150.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_condition":{"QuestEventRef":{"m_eventUID":"BgpOoGQF10O7IQyiB9HODw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3315.0,"y":3435.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"5"},{"_condition":{"QuestEventRef":{"m_eventUID":"-ybLyBSN4ESwCwU9IgzdUA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":1530.0,"y":3690.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"8"},{"_condition":{"QuestEventRef":{"m_eventUID":"kS9py1Sy4Eegcdl4_kOKBg"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":615.0,"y":3795.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"11"},{"statement":{"_text":"You\u2019ve gotten approval from someone? Perfect. You should be clear to <color=#00CCFF><b>go to the palace and request citizenship from Queen Calixa</b></color>. She typically never refuses, as long as you\u2019ve been vouched for.","_meta":"Yzan_KL_prequest_10"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":315.0,"y":3975.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"14"},{"_condition":{"conditions":[{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":50},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"},{"QuestEventRef":{"m_eventUID":"t7BcyPBUFEmdcEfe5UZhzA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"},"_position":{"x":585.0,"y":4200.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"16"},{"availableChoices":[{"statement":{"_text":"(Give 50 Silver) I\u2019m sorry about earlier. Here, I\u2019ll pay you back, plus interest.","_meta":"Player_Yzan_KL_prequest_b_02"}},{"statement":{"_text":"(Say nothing)","_meta":"Player_Yzan_KL_prequest_d_02"}}],"_position":{"x":465.0,"y":4365.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"27"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"t7BcyPBUFEmdcEfe5UZhzA"},"$type":"NodeCanvas.Tasks.Actions.RemoveQuestEvent"},{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":50}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":300.0,"y":4515.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"33"},{"statement":{"_text":"Maybe there\u2019s some hope for you after all. Fine. It\u2019s in the past now.","_meta":"Yzan_KL_prequest_05"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":255.0,"y":4665.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"47"},{"_position":{"x":795.0,"y":4860.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"49"},{"statement":{"_text":"You will <color=#00CCFF><b>need someone who has been here a while to vouch for you</b></color> if you want to stay here. We''re currently facing an overpopulation crisis and we need to be sure our new people can actually be of use.","_meta":"Yzan_KL_prequest_08"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":855.0,"y":3945.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"50"},{"statement":{"_text":"You should try <color=#00CCFF><b>Cyrene, by the entrance to the Palace</b></color>, she''ll approve you if she thinks you might be helpful to our army. Or you can always make a deal with Sir Montgomery, the <color=#00CCFF><b>Baron</b></color> of the slums. He''s usually <color=#00CCFF><b>outside his manor in the West District</b></color>.","_meta":"Yzan_KL_prequest_09"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":825.0,"y":4050.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"52"},{"_condition":{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":1695.0,"y":3825.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"54"},{"statement":{"_text":"Ah! [PlayerName]! You made it! Welcome to the Heroic Kingdom of Levant, my friend!","_meta":"Yzan_KL_prequest_01"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1410.0,"y":4095.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"57"},{"availableChoices":[{"statement":{"_text":"I''m just passing through.","_meta":"Player_Yzan_KL_prequest_a_02"}},{"statement":{"_text":"I want to join Levant.","_meta":"Player_Yzan_KL_prequest_c_02"}}],"_position":{"x":1485.0,"y":4305.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"59"},{"statement":{"_text":"Enjoy your stay, then, my friend.","_meta":"Yzan_KL_prequest_03"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1230.0,"y":4470.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"65"},{"_position":{"x":1230.0,"y":4590.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"67"},{"statement":{"_text":"Good to hear! We could always use someone who knows what they''re doing.","_meta":"Yzan_KL_prequest_07"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1530.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"68"},{"_condition":{"QuestEventRef":{"m_eventUID":"QMe_j2TIWEKpssXkLHMMZA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":1560.0,"y":4545.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"70"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"-ybLyBSN4ESwCwU9IgzdUA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":1395.0,"y":4635.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"73"},{"statement":{"_text":"You will <color=#00CCFF><b>need someone who has been here a while to vouch for you</b></color> if you want to stay here. We''re currently facing an overpopulation crisis and we need to be sure our new people can actually be of use.","_meta":"Yzan_KL_prequest_08"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1305.0,"y":4725.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"82"},{"statement":{"_text":"You should try <color=#00CCFF><b>Cyrene, by the entrance to the Palace</b></color>, she''ll approve you if she thinks you might be helpful to our army. Or you can always make a deal with Sir Montgomery, the <color=#00CCFF><b>Baron</b></color> of the slums. He''s usually <color=#00CCFF><b>outside his manor in the West District</b></color>.","_meta":"Yzan_KL_prequest_09"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1275.0,"y":4875.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"84"},{"_position":{"x":1500.0,"y":4995.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"86"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"-ybLyBSN4ESwCwU9IgzdUA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":1665.0,"y":4650.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"87"},{"statement":{"_text":"Well, look what the sand swept in. My favourite bandit. What brings you to my city, [PlayerName]?","_meta":"Yzan_KL_prequest_02"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2145.0,"y":4005.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"94"},{"_condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":50},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"},"_position":{"x":2280.0,"y":4140.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"96"},{"availableChoices":[{"statement":{"_text":"(Give 50 Silver) I\u2019m sorry about earlier. Here, I\u2019ll pay you back, plus interest.","_meta":"Player_Yzan_KL_prequest_b_02"}},{"statement":{"_text":"(Say nothing)","_meta":"Player_Yzan_KL_prequest_d_02"}}],"_position":{"x":2130.0,"y":4290.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"103"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"t7BcyPBUFEmdcEfe5UZhzA"},"$type":"NodeCanvas.Tasks.Actions.RemoveQuestEvent"},{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":50}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":1995.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"109"},{"statement":{"_text":"Maybe there\u2019s some hope for you after all. Fine. It\u2019s in the past now.","_meta":"Yzan_KL_prequest_05"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1950.0,"y":4590.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"123"},{"_targetNode":{"$ref":"59"},"_position":{"x":2085.0,"y":4725.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"125"},{"availableChoices":[{"statement":{"_text":"I''m just passing through.","_meta":"Player_Yzan_KL_prequest_a_02"}},{"statement":{"_text":"I want to join Levant.","_meta":"Player_Yzan_KL_prequest_c_02"}},{"statement":{"_text":"What can you tell me about The Heroic Kingdom?","_meta":"Yzan_BC_Prequest_19"}}],"_position":{"x":2415.0,"y":4620.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"126"},{"statement":{"_text":"Sure, fine. Just be aware that in this city, thieves get locked up outdoors in the sun. Unless you\u2019re looking for a nasty tan, behave yourself.","_meta":"Yzan_KL_prequest_04"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":1860.0,"y":5085.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"134"},{"_position":{"x":1875.0,"y":5220.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"136"},{"statement":{"_text":"I suppose even I can''t turn my nose up at an extra pair of hands, even when they''re known to steal from me.","_meta":"Yzan_KL_prequest_06"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2250.0,"y":5100.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"137"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"-ybLyBSN4ESwCwU9IgzdUA"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":2355.0,"y":5220.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"139"},{"statement":{"_text":"You will <color=#00CCFF><b>need someone who has been here a while to vouch for you</b></color> if you want to stay here. We''re currently facing an overpopulation crisis and we need to be sure our new people can actually be of use.","_meta":"Yzan_KL_prequest_08"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2280.0,"y":5325.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"146"},{"statement":{"_text":"You should try <color=#00CCFF><b>Cyrene, by the entrance to the Palace</b></color>, she''ll approve you if she thinks you might be helpful to our army. Or you can always make a deal with Sir Montgomery, the <color=#00CCFF><b>Baron</b></color> of the slums. He''s usually <color=#00CCFF><b>outside his manor in the West District</b></color>.","_meta":"Yzan_KL_prequest_09"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2250.0,"y":5445.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"148"},{"_position":{"x":2430.0,"y":5580.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"150"},{"statement":{"_text":"The Heroic Kingdom doesn\u2019t buy into this idea that we can only ever focus on survival. They believe in pushing the boundaries and chasing progress to create a more enriched society.","_meta":"Yzan_BC_Prequest_17"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2610.0,"y":4860.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"151"},{"statement":{"_text":"It\u2019s a place where you can do what you know you were meant to do, without having to answer to oppressive tribal laws and the misdeeds of your ancestors. You succeed or fail on your own merits, no one else\u2019s.","_meta":"Yzan_BC_Prequest_18"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2745.0,"y":4965.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"153"},{"_targetNode":{"$ref":"126"},"_position":{"x":2880.0,"y":5070.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"155"},{"_condition":{"QuestEventRef":{"m_eventUID":"t7BcyPBUFEmdcEfe5UZhzA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3480.0,"y":3525.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"156"},{"statement":{"_text":"Oh, great. It''s you. Come to make my day even more awful? Well, too bad. I\u2019m leaving Cierzo to go join the <color=#00CCFF><b>Heroic Kingdom of Levant</b></color>. Maybe there people will treat me with some damn respect!","_meta":"Yzan_BC_Prequest_01"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3090.0,"y":4170.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"159"},{"statement":{"_text":"The Heroic Kingdom doesn\u2019t buy into this idea that we can only ever focus on survival. They believe in pushing the boundaries and chasing progress to create a more enriched society.","_meta":"Yzan_BC_Prequest_17"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3105.0,"y":4305.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"161"},{"statement":{"_text":"It\u2019s a place where you can do what you know you were meant to do, without having to answer to oppressive tribal laws and the misdeeds of your ancestors. You succeed or fail on your own merits, no one else\u2019s.","_meta":"Yzan_BC_Prequest_18"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3105.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"163"},{"_condition":{"character":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"item":{"_value":{"m_itemID":9000010}},"minAmount":{"_value":50},"itemMustBeEquiped":{},"$type":"NodeCanvas.Tasks.Conditions.Condition_OwnsItem"},"_position":{"x":3405.0,"y":4710.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"165"},{"availableChoices":[{"statement":{"_text":"(Give 50 Silver) I\u2019m sorry about earlier. Here, I\u2019ll pay you back, plus interest.","_meta":"Player_Yzan_KL_prequest_b_02"}},{"statement":{"_text":"(Say nothing)","_meta":"Player_Yzan_KL_prequest_d_02"}}],"_position":{"x":3240.0,"y":4860.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"172"},{"statement":{"_text":"Maybe there\u2019s some hope for you after all. Fine. It\u2019s in the past now.","_meta":"Yzan_KL_prequest_05"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3030.0,"y":5055.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"178"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"t7BcyPBUFEmdcEfe5UZhzA"},"$type":"NodeCanvas.Tasks.Actions.RemoveQuestEvent"},{"fromCharacter":{"_name":"gInstigator","_targetVariableID":"0483cf6b-da62-4a57-aaed-40f25b4bdf9b"},"Items":[{"_value":{"m_itemID":9000010}}],"Amount":[{"_value":50}],"$type":"NodeCanvas.Tasks.Actions.RemoveItem"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3135.0,"y":5190.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"180"},{"_targetNode":{"availableChoices":[{"statement":{"_text":"Why so far away?","_meta":"Player_Yzan_BC_Prequest_a_03"}},{"statement":{"_text":"You have a duty to your tribe! You can''t abandon it like this!","_meta":"Player_Yzan_BC_Prequest_b_03"}},{"statement":{"_text":"Good choice. Go chase your own dreams!","_meta":"Player_Yzan_BC_Prequest_c_03"}}],"_position":{"x":3825.0,"y":4050.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"193"},"_position":{"x":3210.0,"y":5295.0},"$type":"NodeCanvas.DialogueTrees.GoToNode","$id":"192"},{"statement":{"_text":"I''m not going to forgive you, [PlayerName]. But <color=#00CCFF><b>if we meet again in Levant</b></color>... maybe we can start over.","_meta":"Yzan_BC_Prequest_15"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3495.0,"y":5415.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"201"},{"statement":{"_text":"If you plan to make the journey to <color=#00CCFF><b>Levant</b></color> completely on foot, be prepared for a <color=#00CCFF><b>long, long journey</b></color>. You\u2019ll have to <color=#00CCFF><b>leave the Chersonese by going south east</b></color>, then travel <color=#00CCFF><b>east through the entire Enmerkar Forest</b></color>, and then finally <color=#00CCFF><b>south through the Abrassar Desert</b></color> itself.","_meta":"Yzan_BC_Prequest_12"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3555.0,"y":5580.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"203"},{"statement":{"_text":"If you have the courage to face the Abrassar desert and come to the Heroic Kingdom of Levant... <color=#00CCFF><b>I''ll be waiting for you.</b></color>","_meta":"Yzan_BC_Prequest_14"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4020.0,"y":5760.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"205"},{"_action":{"QuestEventRef":{"m_eventUID":"JlFMC_51RUalSa8yLkhTmg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4065.0,"y":5880.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"207"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeOut"},"_position":{"x":4125.0,"y":5970.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"210"},{"_action":{"actions":[{"QuestEventRef":{"m_eventUID":"BgpOoGQF10O7IQyiB9HODw"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},{"QuestEventRef":{"m_eventUID":"gAJAjuzl7ESFpMooq1oOCg"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"}],"$type":"NodeCanvas.Framework.ActionList"},"_position":{"x":3990.0,"y":6210.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"212"},{"_condition":{"QuestEventRef":{"m_eventUID":"t7BcyPBUFEmdcEfe5UZhzA"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3960.0,"y":6330.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"219"},{"_action":{"$type":"NodeCanvas.Tasks.Actions.FadeIn"},"_position":{"x":4080.0,"y":6585.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"222"},{"_position":{"x":4080.0,"y":6690.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"224"},{"_action":{"QuestEventRef":{"m_eventUID":"nr9KDCbQzUae1Gwf-6yOIQ"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":4170.0,"y":6480.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"225"},{"statement":{"_text":"I take it you''ve heard the news, [PlayerName]? Come to see your old pal off?","_meta":"Yzan_BC_Prequest_02"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3735.0,"y":3645.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"228"},{"statement":{"_text":"I''ve got one little fishing barge to my name, and by Elatt I''m going to use it. I''m leaving for the <color=#00CCFF><b>Heroic Kingdom of Levant</b></color>, never to darken this stain of a city ever again.","_meta":"Yzan_BC_Prequest_03"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3735.0,"y":3750.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"230"},{"statement":{"_text":"The Heroic Kingdom doesn\u2019t buy into this idea that we can only ever focus on survival. They believe in pushing the boundaries and chasing progress to create a more enriched society.","_meta":"Yzan_BC_Prequest_17"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3735.0,"y":3855.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"232"},{"statement":{"_text":"It\u2019s a place where you can do what you know you were meant to do, without having to answer to oppressive tribal laws and the misdeeds of your ancestors. You succeed or fail on your own merits, no one else\u2019s.","_meta":"Yzan_BC_Prequest_18"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3735.0,"y":3945.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"234"},{"$ref":"193"},{"statement":{"_text":"Levant believes that everyone deserves a shot at what they''re best at, and doesn''t have these backwards Tribal Laws to deal with. It''s the rational place for someone like me to start over fresh. ","_meta":"Yzan_BC_Prequest_04"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3555.0,"y":4200.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"236"},{"statement":{"_text":"And honestly, the farther away I am from my dolt of a brother and that tyrant Rissa, the better I''ll sleep at night!","_meta":"Yzan_BC_Prequest_05"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3570.0,"y":4335.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"238"},{"availableChoices":[{"statement":{"_text":"I take it you''re not going to forgive your brother, are you?","_meta":"Player_Yzan_BC_Prequest_a_07"}},{"statement":{"_text":"How do you plan on making it to Levant? The sea can only take you so close. I mean, it''s in the middle of a desert!","_meta":"Player_Yzan_BC_Prequest_b_07"}},{"statement":{"_text":"..."}}],"_position":{"x":3900.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"240"},{"statement":{"_text":"Not in a thousand years. Roland had one job, keep the lighthouse lit. And he couldn\u2019t even manage that! This was preventable, dammit!","_meta":"Yzan_BC_Prequest_08"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3630.0,"y":4575.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"248"},{"statement":{"_text":"This is the last time I let his foolishness and clumsiness hurt me. He can go feed a hyena with his own hide for all I care. I\u2019m done with him. Now and forever.","_meta":"Yzan_BC_Prequest_09"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3735.0,"y":4680.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"250"},{"availableChoices":[{"statement":{"_text":"Can I come along? I want out of this place too.","_meta":"Player_Yzan_BC_Prequest_a_10"}},{"statement":{"_text":"I''m going to miss you, Yzan. Wind be at your back.","_meta":"Player_Yzan_BC_Prequest_b_10"}},{"statement":{"_text":"..."}}],"_position":{"x":3960.0,"y":4815.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"252"},{"statement":{"_text":"I''d take you if I could, but you can''t swim, and on a barge that small, you''d be nothing but a liability. I''m sorry, [PlayerName], but that''s just not going to happen. If you manage to get to the Kingdom of Levant on your own somehow, though, I''ll do my best to look out for you.","_meta":"Yzan_BC_Prequest_11"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3765.0,"y":4935.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"260"},{"statement":{"_text":"If you plan to make the journey to <color=#00CCFF><b>Levant</b></color> completely on foot, be prepared for a <color=#00CCFF><b>long, long journey</b></color>. You\u2019ll have to <color=#00CCFF><b>leave the Chersonese by going south east</b></color>, then travel <color=#00CCFF><b>east through the entire Enmerkar Forest</b></color>, and then finally <color=#00CCFF><b>south through the Abrassar Desert</b></color> itself.","_meta":"Yzan_BC_Prequest_12"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4035.0,"y":5115.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"262"},{"statement":{"_text":"Likewise, my friend. I hope we meet again under better circumstances. Wind be at your back.","_meta":"Yzan_BC_Prequest_13"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4275.0,"y":4980.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"264"},{"statement":{"_text":"We''ll go by ocean as far as we can, then leg it across the <color=#00CCFF><b>Abrassar desert</b></color> on foot. We''ll be rationing water the whole way. If I could afford it, I''d have bought potions to help with the heat... but we''ll have to try our luck without them.","_meta":"Yzan_BC_Prequest_10"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4170.0,"y":4590.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"266"},{"statement":{"_text":"Make me. This entire notion of Tribal Duty to one another is oppressive at its core. My individual life is important, asking me to willingly sacrifice my wants and needs for the \"collective good\" is impossible. I refuse to be chained by things outside of my control!","_meta":"Yzan_BC_Prequest_06"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3990.0,"y":4200.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"268"},{"statement":{"_text":"That I will. I will chase the Levantine dream and make it my own. I''ll take any risk over being trapped here by my brother''s idiocy.","_meta":"Yzan_BC_Prequest_07"},"_actorName":"Yzan Argenson","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4410.0,"y":4215.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"270"},{"statement":{"_text":"Greetings. I hope you\u2019re well!","_meta":"Salutation_Generic_02"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3975.0,"y":3330.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"272"},{"_position":{"x":3990.0,"y":3450.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"274"},{"statement":{"_text":"(Yzan shoves off with two men on his little fishing barge. He doesn''t give Cierzo a backwards glance)","_meta":"Yzan_BC_Prequest_16"},"_actorName":"name_unpc_yzanargenson_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3660.0,"y":6045.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"275"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"272"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"8"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"156"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"8"},"_targetNode":{"$ref":"11"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"8"},"_targetNode":{"$ref":"54"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"11"},"_targetNode":{"$ref":"14"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"11"},"_targetNode":{"$ref":"50"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"14"},"_targetNode":{"$ref":"16"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"16"},"_targetNode":{"$ref":"27"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sou
  m_Name: Dialogue_Trainers_CierzoHermit
  m_EditorClassIdentifier:
  _serializedGraph: '{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-5340.0,"y":-5894.0},"nodes":[{"statement":{"_text":"Visitors are rare up here. The Cabal of Winds only shares its heritage with those who respect the Five Winds. Reason, Passion, Fury, Kindness and the Inside Wind.","_meta":"Trainer_Cierzo_Hermit_01"},"_actorName":"name_trainer_hermittrainer_01 ","_actorParameterID":"8b9c0625-6c3d-4588-ae4e-76f04f07aeb3","_position":{"x":5775.0,"y":5265.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"2"},{"statement":{"_text":"If you wish to use the spells I teach you, seek out the totems of the Winds in each region you travel to. Only then will the Winds recognize you and lend you their power. They look like a smaller kind of tower to my own.","_meta":"Trainer_Cierzo_Hermit_02"},"_actorName":"name_trainer_hermittrainer_01 ","_actorParameterID":"8b9c0625-6c3d-4588-ae4e-76f04f07aeb3","_position":{"x":5760.0,"y":5385.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"4"},{"availableChoices":[{"statement":{"_text":"I want to train with you.","_meta":"Merchant_Generic_Train_01"}},{"statement":{"_text":"Do you know a Mage named Balira? I''m looking for her husband.","_meta":"Player_Hermit_Vendavel_a_00"},"condition":{"conditions":[{"QuestEventRef":{"m_eventUID":"iBXvhnEkykqE6iiex4MOvw"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},{"questRef":{"_value":{"m_itemID":7011004}},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},{"QuestEventRef":{"m_eventUID":"fy76RwKIxEChxODBcR50_w"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}],"$type":"NodeCanvas.Framework.ConditionList"}},{"statement":{"_text":"I\u2019m leaving.","_meta":"Merchant_Generic_Leave_01"}}],"_position":{"x":5835.0,"y":5520.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"6"},{"_action":{"Trainer":{"_name":"myTrainer","_targetVariableID":"763c1c0e-163c-4e01-9626-9cd0a32f0fa6"},"PlayerCharacter":{"_name":"gInstigator","_targetVariableID":"78d35a0e-9529-4014-860f-d63311e1cef1"},"$type":"NodeCanvas.Tasks.Actions.TrainDialogueAction"},"_position":{"x":5595.0,"y":5700.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"23"},{"statement":{"_text":"I know her, she was an apprentice of mine. Then, she twisted the gifts of the Five Winds to perform forbidden and horrible magic. Manipulating a person''s Inside Wind, no matter the reason, is a violation of a person''s most sacred boundary.","_meta":"Hermit_Vendavel_00"},"_actorName":"name_trainer_hermittrainer_01 ","_actorParameterID":"8b9c0625-6c3d-4588-ae4e-76f04f07aeb3","_position":{"x":5730.0,"y":5790.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"27"},{"statement":{"_text":"I told her she was never to come here again after what she did. And yet...Although I cannot forgive her sacrilege...It is long past time to lay this tragedy to bed for good.","_meta":"Hermit_Vendavel_01"},"_actorName":"name_trainer_hermittrainer_01 ","_actorParameterID":"8b9c0625-6c3d-4588-ae4e-76f04f07aeb3","_position":{"x":5730.0,"y":5925.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"29"},{"statement":{"_text":"The man...Or rather, what''s left of the man you''re looking for is hiding in the old broken mines within this valley. He, or rather, \"it\" lives in the frozen cavern at the mouth of it. Put an end to it''s misery, and break their connection.","_meta":"Hermit_Vendavel_02"},"_actorName":"name_trainer_hermittrainer_01 ","_actorParameterID":"8b9c0625-6c3d-4588-ae4e-76f04f07aeb3","_position":{"x":5745.0,"y":6075.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"31"},{"_action":{"QuestEventRef":{"m_eventUID":"Sjees2STH0m8V2C8nAuMog"},"$type":"NodeCanvas.Tasks.Actions.SendQuestEvent"},"_position":{"x":5790.0,"y":6225.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"33"},{"_position":{"x":5880.0,"y":6330.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"36"},{"_position":{"x":6360.0,"y":5670.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"37"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"4"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"4"},"_targetNode":{"$ref":"6"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"6"},"_targetNode":{"$ref":"23"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"6"},"_targetNode":{"$ref":"27"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"6"},"_targetNode":{"$ref":"37"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"27"},"_targetNode":{"$ref":"29"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"29"},"_targetNode":{"$ref":"31"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"31"},"_targetNode":{"$ref":"33"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"33"},"_targetNode":{"$ref":"36"},"$type":"NodeCanvas.DialogueTrees.DTConnection"}],"localBlackboard":{"_name":"Local Blackboard","_variables":{"gInstigator":{"_name":"gInstigator","_id":"78d35a0e-9529-4014-860f-d63311e1cef1","$type":"NodeCanvas.Framework.Variable`1[[Character, Assembly-CSharp, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null]]"},"myTrainer":{"_name":"myTrainer","_id":"763c1c0e-163c-4e01-9626-9cd0a32f0fa6","$type":"NodeCanvas.Framework.Variable`1[[Trainer, Assembly-CSharp, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null]]"}}},"derivedData":{"actorParameters":[{"_keyName":"name_trainer_hermittrainer_01 ","_id":"8b9c0625-6c3d-4588-ae4e-76f04f07aeb3","_actorObject":1}],"$type":"NodeCanvas.DialogueTrees.DialogueTree+DerivedSerializationData"}}'

{"version":2.92,"type":"NodeCanvas.DialogueTrees.DialogueTreeExt","translation":{"x":-1655.0,"y":-4258.0},"zoomFactor":0.9999778,"nodes":[{"_condition":{"QuestEventRef":{"m_eventUID":"nDy01eTHlUa_BPDlbIhZPQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3585.0,"y":2835.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"2"},{"_condition":{"QuestEventRef":{"m_eventUID":"f1JVZyhg2UiBA8xmC-w6Hw"},"_invert":true,"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3255.0,"y":3000.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"5"},{"statement":{"_text":"You still allowed to visit, but you only deal with Gold-Belly. You leave rest of people alone!","_meta":"Cyr_BC_Giants_53"},"_actorName":"name_unpc_highlordcyr_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2970.0,"y":3195.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"8"},{"_position":{"x":3315.0,"y":3390.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"10"},{"statement":{"_text":"If stay any longer, you really will feel sorry. Get out now, or I throw you out myself!","_meta":"Cyr_BC_Giants_49"},"_actorName":"name_unpc_highlordcyr_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3405.0,"y":3195.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"11"},{"_condition":{"questRef":{"_value":{"m_itemID":7011002}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":4140.0,"y":3525.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"13"},{"_condition":{"QuestEventRef":{"m_eventUID":"nllwi4FnR0qN7968EsJ07A"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3855.0,"y":3765.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"17"},{"_condition":{"quest":{"_value":1},"$type":"NodeCanvas.Tasks.Conditions.Condition_KnowQuest"},"_position":{"x":3645.0,"y":3885.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"20"},{"_condition":{"questRef":{"_value":{"m_itemID":7011202}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":3180.0,"y":3975.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"23"},{"_condition":{"questRef":{"_value":{"m_itemID":7011204}},"$type":"NodeCanvas.Tasks.Conditions.Condition_IsQuestCompleted"},"_position":{"x":2490.0,"y":4290.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"27"},{"statement":{"_text":"I\u2019m afraid I\u2019m quite busy, perhaps some other time.","_meta":"Salutation_Generic_03"},"_actorName":"name_unpc_highlordcyr_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":2205.0,"y":4440.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"31"},{"_position":{"x":2295.0,"y":4575.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"33"},{"_action":{"dialogueStarter":{"_name":"Dialogue_HighlordCyr_BB/AncPeacemaker","_targetVariableID":"2ce80f12-5b68-406c-8e6e-d4004bd47c95"},"$type":"NodeCanvas.Tasks.Actions.BranchDialogue"},"_position":{"x":2625.0,"y":4455.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"34"},{"_condition":{"QuestEventRef":{"m_eventUID":"QrDHfm8z_EWjFfh1gFKrtw"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"},"_position":{"x":3345.0,"y":4155.0},"$type":"NodeCanvas.DialogueTrees.ConditionNode","$id":"37"},{"statement":{"_text":"I\u2019m afraid I\u2019m quite busy, perhaps some other time.","_meta":"Salutation_Generic_03"},"_actorName":"name_unpc_highlordcyr_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3090.0,"y":4260.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"40"},{"_position":{"x":3120.0,"y":4380.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"42"},{"_action":{"dialogueStarter":{"_name":"Dialogue_HighlordCyr_BB/AshGiant","_targetVariableID":"ded8b144-63c0-4e15-ac75-ed3f29b831ec"},"$type":"NodeCanvas.Tasks.Actions.BranchDialogue"},"_position":{"x":3420.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.ActionNode","$id":"43"},{"statement":{"_text":"A human, in my village? What brings you before the Ashborn of the Marsh, little one?","_meta":"Cyr_BC_Giants_01"},"_actorName":"name_unpc_highlordcyr_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4035.0,"y":4005.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"46"},{"availableChoices":[{"statement":{"_text":"I''m just passing through.","_meta":"Player_Cyr_BC_Giants_a_01"}},{"statement":{"_text":"I''m here to learn about your people.","_meta":"Player_Cyr_BC_Giants_b_01"}},{"statement":{"_text":"I''m here on behalf of the Blue Chamber Collective. We would like to form an alliance with you.","_meta":"Player_Cyr_BC_Giants_c_01"},"condition":{"QuestEventRef":{"m_eventUID":"pOHuHF0q9Uu5W3ccIwIZBQ"},"$type":"NodeCanvas.Tasks.Conditions.Condition_QuestEventOccured"}}],"_position":{"x":4065.0,"y":4170.0},"$type":"NodeCanvas.DialogueTrees.MultipleChoiceNodeExt","$id":"48"},{"statement":{"_text":"Then pass through faster. You bring worry and change. If you visit, speak only to Gold-Belly. Leave the rest of my people alone.","_meta":"Cyr_BC_Giants_02"},"_actorName":"Highlord Cyr","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":3795.0,"y":4395.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"58"},{"_position":{"x":3930.0,"y":4530.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"60"},{"statement":{"_text":"Oh, another curious one. I am Highlord Cyr. I am lead this village. I am master of the sacred ash-silk weaving, I weave cloth as a prayer in the name of the Dawn Weaver and I teach this art to the other highborns.","_meta":"Cyr_BC_Giants_03"},"_actorName":"name_unpc_highlordcyr_01 ","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4215.0,"y":4410.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"61"},{"statement":{"_text":"Our monks make the porcelain in the name of the Dusk Father, and fill it with his magic. Your people very much like the porcelain and silks and many come here offering much silver and gold for some of it.","_meta":"Cyr_BC_Giants_04"},"_actorName":"Highlord Cyr","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4290.0,"y":4530.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"63"},{"statement":{"_text":"That is all there is to tell. Your curiosity satisfied. Now, go, little human.","_meta":"Cyr_BC_Giants_05"},"_actorName":"Highlord Cyr","_actorParameterID":"40fbaef3-4bf8-4bc9-bf2f-ae0d25ded760","_position":{"x":4305.0,"y":4635.0},"$type":"NodeCanvas.DialogueTrees.StatementNodeExt","$id":"65"},{"_position":{"x":4395.0,"y":4770.0},"$type":"NodeCanvas.DialogueTrees.FinishNode","$id":"67"}],"connections":[{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"5"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"2"},"_targetNode":{"$ref":"13"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"8"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"5"},"_targetNode":{"$ref":"11"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"8"},"_targetNode":{"$ref":"10"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"11"},"_targetNode":{"$ref":"10"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"13"},"_targetNode":{"$ref":"17"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"13"},"_targetNode":{"$ref":"46"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"17"},"_targetNode":{"$ref":"20"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"17"},"_targetNode":{"$ref":"46"},"$type":"NodeCanvas.DialogueTrees.DTConnection"},{"_sourceNode":{"$ref":"20"},"_targetNode":{"$ref":"23"},"$type":"NodeCanvas.Dia